\n  >\r\n    <div class=\"card\">\r\n      <DataTable\r\n        :value=\"prevPrescriptionList\"\r\n        class=\"p-datatable-sm\"\r\n        responsiveLayout=\"scroll\"\r\n      >\r\n        <Column field=\"appoinment_no\" header=\"No\"></Column>\r\n        <Column field=\"appoint_date\" header=\"Date\"></Column>\r\n        <Column field=\"\" header=\"Download\">\r\n        <template #body=\"slotProps\">\r\n          <span class=\"p-column-title\">Download</span>\r\n          <Button\r\n            icon=\"pi pi-download\"\r\n            class=\"p-button-rounded p-button-success p-mr-2\"\r\n            @click=\"downloadPrescription(slotProps.data.appoinment_id)\"\r\n          />\r\n        </template>\r\n        </Column>\r\n      </DataTable>\r\n    </div>\r\n\r\n    <template #footer>\r\n      <Button\r\n        label=\"Close\"\r\n        icon=\"pi pi-times\"\r\n        class=\"p-button-text\"\r\n        @click=\"hideDialog\"\r\n      />\r\n    </template>\r\n  </Dialog>\r\n<Dialog\r\n    v-model:visible=\"treatmentDialog\"\r\n    :style=\"{ width: '450px' }\"\r\n    header=\"Treatment\"\r\n    :modal=\"true\"\r\n    class=\"p-fluid\"\r\n  >\r\n  \r\n    <div class=\"p-field\">\r\n      <label for=\"medicine_id\"\r\n            >Medicine<span style=\"color: red\">*</span></label\r\n          >\r\n          <InputText\r\n            id=\"medicine_id\"\r\n            v-model.trim=\"treatmentSetup.medicineName\"\r\n            required=\"true\"\r\n            autofocus\r\n            disabled\r\n          />\r\n    </div>\r\n    <div class=\"p-field\">\r\n      <label for=\"treatment_rules_id\"\r\n            >Rules<span style=\"color: red\">*</span></label\r\n          >\r\n          <Dropdown\r\n            id=\"treatment_rules_id\"\r\n            v-model=\"treatmentSetup.ddltreatment_rules\"\r\n            :options=\"treatmentRulesList\"\r\n            optionLabel=\"text\"\r\n            \r\n            :filter=\"true\"\r\n            :showClear=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !treatmentSetup.treatment_rules_id,\r\n            }\"\r\n          />\r\n          <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !treatmentSetup.treatment_rules_id\"\r\n          >Rules is required.</small\r\n        >\r\n    </div>\r\n    <div class=\"p-field\">\r\n      <label for=\"treatment_duration_id\"\r\n            >Duration<span style=\"color: red\">*</span></label\r\n          >\r\n      <Dropdown\r\n            id=\"treatment_duration_id\"\r\n            v-model=\"treatmentSetup.ddltreatment_duration\"\r\n            :options=\"treatmentDurationList\"\r\n            optionLabel=\"text\"\r\n            \r\n            :filter=\"true\"\r\n            :showClear=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !treatmentSetup.treatment_duration_id,\r\n            }\"\r\n          />\r\n          <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !treatmentSetup.treatment_duration_id\"\r\n          >Duration is required.</small\r\n        >\r\n    </div>\r\n    <div class=\"p-field\">\r\n      <label for=\"treatment_dose_id\"\r\n            >Dose<span style=\"color: red\">*</span></label\r\n          >\r\n          <Dropdown\r\n            id=\"treatment_dose_id\"\r\n            v-model=\"treatmentSetup.ddltreatment_dose\"\r\n            :options=\"treatmentDoseList\"\r\n            optionLabel=\"text\"\r\n            \r\n            :filter=\"true\"\r\n            :showClear=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !treatmentSetup.treatment_dose_id,\r\n            }\"\r\n          />\r\n          <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !treatmentSetup.treatment_dose_id\"\r\n          >Dose is required.</small\r\n        >\r\n    </div>\r\n    \r\n    <template #footer>\r\n     