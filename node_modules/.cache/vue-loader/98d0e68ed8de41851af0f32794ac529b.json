\">\r\n      <div class=\"p-field p-col-12 p-md-\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"service_head_name\"\r\n            v-model.trim=\"serviceHeadSetup.service_head_name\"\r\n            :disabled=\"serviceHeadSetup.service_head_id\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !serviceHeadSetup.service_head_name,\r\n            }\"\r\n            \r\n          />\r\n          <label for=\"service_head_name\"\r\n            >Name<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !serviceHeadSetup.service_head_name\"\r\n          >Name is required.</small\r\n        >\r\n      </div>\r\n      \r\n      <div class=\"p-field p-col-12 p-md-10\">\r\n        <div class=\"p-field-checkbox\">\r\n          <Checkbox id=\"active\" v-model=\"serviceHeadSetup.active\" :binary=\"true\" />\r\n          <label for=\"active\">Active</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Save\"\r\n          \r\n          @click=\"add\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5;border: none;\"\r\n        />\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Cancel\"\r\n          \r\n          @click=\"hideNew\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #FB465E;border: none;\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"p-grid crud-demo\" v-show=\"dataList\">\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <Toolbar class=\"p-mb-4\">\r\n          <template v-slot:left>\r\n            <Button\r\n              label=\"New\"\r\n              icon=\"pi pi-plus\"\r\n              class=\"p-button-suc