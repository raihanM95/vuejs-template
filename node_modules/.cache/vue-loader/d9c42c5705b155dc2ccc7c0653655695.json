 <h5 class=\"p-m-0\">Manage Treatments</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"_department_name\" header=\"Department\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Department</span>\r\n              {{ slotProps.data._department_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_doctor_name\" header=\"Doctor\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Doctor</span>\r\n              {{ slotProps.data._doctor_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_medicine_name\" header=\"Medicine\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Medicine</span>\r\n              {{ slotProps.data._medicine_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_treatment_rule_name\" header=\"Rule\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Rule</span>\r\n              {{ slotProps.data._treatment_rule_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_treatment_dose_name\" header=\"Dose\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Dose</span>\r\n              {{ slotProps.data._treatment_dose_name }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"_treatment_duration_name\"\r\n            header=\"Duration\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Duration</span>\r\n              {{ slotProps.data._treatment_duration_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"description\" header=\"Description\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Description</span>\r\n              {{ slotProps.data.description }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"is_favourite\" header=\"Favourite\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Status</span>\r\n              <span\r\n                v-show=\"slotProps.data.is_favourite\"\r\n                class=\"product-badge status-active\"\r\n                >YES</span\r\n              >\r\n              <span\r\n                v-show=\"!slotProps.data.is_favourite\"\r\n                class=\"product-badge status-inactive\"\r\n                >NO</span\r\n              >\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from  \"../../scripts/setup/TreatmentSetup\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5r