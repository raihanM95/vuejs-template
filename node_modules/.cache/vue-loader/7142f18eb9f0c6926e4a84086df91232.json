checkbox\"\r\n                id=\"show_in_prescription\"\r\n                name=\"show_in_prescription\"\r\n                v-on:click=\"showInPrescription(admissionPrescriptionInfo.lstAppoinmentPrescriptionFigure, admissionPrescriptionInfo.pastHistory_show_in_prescription)\"\r\n                v-model=\"admissionPrescriptionInfo.pastHistory_show_in_prescription\"\r\n              />\r\n              <label for=\"show_in_prescription\">Print</label>\r\n        </Panel>\r\n      </div>\r\n      <div class=\"p-col-12 p-md-6\" style=\"height: 100%;\">\r\n        <Panel header=\"Treatment (Ctrl+Alt+T)\" @click=\"getTreatment\">\r\n            <p style=\"height: 260px;\">\r\n              <DataTable :value=\"admissionPrescriptionInfo.lstAppoinmentPrescriptionTreatment\" dataKey=\"medicine_id\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" :scrollable=\"true\" scrollHeight=\"260px\" v-show=\"admissionPrescriptionInfo.lstAppoinmentPrescriptionTreatment.length > 0\">\r\n                <Column field=\"sl\">\r\n                <template #body=\"slotProps\">\r\n                  <b> {{slotProps.index +1}}</b>\r\n                </template>\r\n                </Column>\r\n                <Column field=\"_medicine_type_name\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data._medicine_type_name}}\r\n                </template>\r\n                </Column>\r\n                <Column field=\"name\">\r\n                <template #body=\"slotProps\">\r\n                  <b> {{slotProps.data.name}}</b>\r\n                </template>\r\n                </Column>\r\n                <Column field=\"_treatment_dose_name\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data._treatment_dose_name}}\r\n                </template>\r\n                </Column>\r\n                <Column field=\"_treatment_rule_name\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data._treatment_rule_name}}\r\n                </template>\r\n                </Column>\r\n                <Column field=\"_treatment_duration_name\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data._treatment_duration_name}}\r\n                </template>\r\n                </Column>\r\n                \r\n                <Column>\r\n                  <template #body=\"slotProps\">\r\n                    <label class=\"pi pi-pencil pencil-style\" @click=\"editTreatment(slotProps.data, slotProps.index)\"></label>\r\n                    <label class=\"pi pi-trash trash-style\" @click=\"deleteItem(admissionPrescriptionInfo.lstAppoinmentPrescriptionTreatment, slotProps.data)\"></label>\r\n                  </template>\r\n                </Column>\r\n            </DataTable>\r\n            </p>\r\n        </Panel>\r\n        <Panel header=\"Advice (Ctrl+Alt+A)\" class=\"margin-top\" @click=\"getAdvice\">\r\n          <p style=\"height: 100px;\">\r\n            <DataTable :value=\"admissionPrescriptionInfo.lstAppoinmentPrescriptionAdvice\" dataKey=\"advice_id\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" :scrollable=\"true\" scrollHeight=\"100px\" v-show=\"admissionPrescriptionInfo.lstAppoinmentPrescriptionAdvice.length > 0\">\r\n                <Column field=\"name\" style=\"margin: 0;padding: 1px\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data.name}}\r\n                </template>\r\n                </Column>\r\n                \r\n                <Column>\r\n                  <template #body=\"slotProps\">\r\n                    <label class=\"pi pi-trash trash-style\" @click=\"deleteItem(admissionPrescriptionInfo.lstAppoinmentPrescriptionAdvice, slotProps.data)\"></label>\r\n                  </template>\r\n                </Column>\r\n            </DataTable>\r\n          </p>\r\n        </Panel>\r\n        <div class=\"p-fluid p-grid margin-top\">\r\n        <div class=\"p-field p-col-12 p-md-8\" style=\"text-align: right;\">\r\n          <label for=\"folowup_date\"\r\n            ><b><i>Next Visit Date </i></b></label\r\n          >\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-4\">\r\n          <Calendar\r\n            id=\"folowup_date\"\r\n            v-model=\"admissionPrescriptionInfo.cdfolowup_date\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"2022:2040\"\r\n            :minDate=\"minDate\"\r\n            :manualInput=\"false\"\r\n          />\r\n        </div>\r\n      </div>\r\n      </div>\r\n      <div class=\"p-col-12 p-md-3\" style=\"height: 100%;\">\r\n        <Panel :header=headerName>\r\n            <p style=\"height: 385px;\">\r\n              \r\n              <DataTable :value=\"tempData\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" v-model:filters=\"filters\" :scrollable=\"true\" scrollHeight=\"300px\" v-show=\"tempData.length > 0\">\r\n                <div class=\"p-fluid p-grid\">\r\n                  <div class=\"p-field p-col-12 p-md-12\">\r\n                    <span class=\"p-input-icon-left\">\r\n                      <i class=\"pi pi-search\" />\r\n                      <InputText\r\n                        v-model=\"filters['global'].value\"\r\n                        placeholder=\"Search...\"\r\n                      />\r\n                    </span>\r\n                    <div v-show=\"isAdvice\" style=\"right: 0;position: absolute;padding-top: 5px;\">\r\n                    <input\r\n                      type=\"checkbox\"\r\n                      id=\"addAllAdvice\"\r\n                      name=\"addAllAdvice\"\r\n                      v-on:click=\"addAllAdvice()\"\r\n                      v-model=\"admissionPrescriptionInfo.allAdvice\"\r\n                    />\r\n                    <label for=\"addAllAdvice\">Select All</label>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <Column field=\"name\" style=\"margin: 0;padding: 1px\">\r\n                <template #body=\"slotProps\">\r\n                  <label style=\"cursor: cell;margin: 0;padding: 1px\" @click=\"getValue(slotProps.data)\">{{slotProps.data.name}}</label>\r\n                </template>\r\n              </Column>\r\n            </DataTable>\r\n            </p>\r\n        </Panel>\r\n        <div class=\"p-fluid p-grid margin-top\">\r\n          <div class=\"p-field p-col-12 p-md-6\">\r\n            <Button\r\n              label=\"Print\"\r\n              @click=\"add\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #F3969A; border: none\"\r\n            />\r\n          </div>\r\n          <div class=\"p-field p-col-12 p-md-6\">\r\n            <Button\r\n              label=\"Share\"\r\n              @click=\"share()\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #56CC9D; border: none\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"p-fluid p-grid\">\r\n          <div class=\"p-field p-col-12 p-md-6\">\r\n            <Button\r\n              label=\"New\"\r\n              @click=\"newPrescription()\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #6CC3D5; border: none\"\r\n            />\r\n          </div>\r\n          <div class=\"p-field p-col-12 p-md-6\">\r\n            <Button\r\n              label=\"Dashboard\"\r\n              @click=\"reset()\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #FF7851; border: none\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  </div>\r\n  <div v-show=\"dataList\">\r\n    <div div class=\"card\">\r\n      <h5 class=\"p-text-center\" style=\"margin-bottom: 28px\">Patient List</h5>\r\n      <div class=\"p-fluid p-grid\">\r\n        <div class=\"p-field p-col-12 p-md-5\">\r\n          <div