  <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"admittedPatientList\"\r\n          dataKey=\"nurse_duty_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Admitted Patient List</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"registration_no\" header=\"Patient Reg No\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Reg No</span>\r\n              {{ slotProps.data.registration_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"admission_no\" header=\"ADM No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">ADM No</span>\r\n              {{ slotProps.data.admission_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"patient_name\" header=\"Patient Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.patient_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"word_cabin_name\" header=\"Word/Cabin\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Word/Cabin</span>\r\n              {{ slotProps.data.word_cabin_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"bed_name\" header=\"Bed\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Bed</span>\r\n              {{ slotProps.data.bed_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"doctor_name\" header=\"Doctor\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Doctor</span>\r\n              {{ slotProps.data.doctor_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"admission_date\" header=\"ADM Date\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">ADM Date</span>\r\n              {{ formatDate(slotProps.data.admission_date) }}\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Rx\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Rx</span>\r\n              <Button\r\n                v-show=\"isDoctor\"\r\n                icon=\"pi pi-file-excel\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"prescription(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/ipd/IPDPatientList\";\r\nexport default app;\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.drill-down {\r\n  float: right;\r\n  curs