d=\"serviceSetup.service_id\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !serviceSetup.service_head_id,\r\n            }\"\r\n            ref=\"service_head_id\"\r\n            v-on:keyup.enter=\"this.$refs.service_type_id.$el.focus()\"\r\n          />\r\n          <label for=\"service_head_id\"\r\n            >Head<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !serviceSetup.service_head_id\"\r\n          >Head is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <Dropdown\r\n            id=\"service_type_id\"\r\n            v-model=\"serviceSetup.service_type_id\"\r\n            :options=\"serviceTypeList\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !serviceSetup.service_type_id,\r\n            }\"\r\n            ref=\"service_type_id\"\r\n            v-on:keyup.enter=\"this.$refs.service_charge.$el.focus()\"\r\n          />\r\n          <label for=\"service_type_id\"\r\n            >Type<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !serviceSetup.service_type_id\"\r\n          >Type is required.</small\r\n        >\r\n      </div>\r\n\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"service_charge\"\r\n            v-model.trim=\"serviceSetup.service_charge\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n            ref=\"service_charge\"\r\n            v-on:keyup.enter=\"this.$refs.er_service_charge.$el.focus()\"\r\n          />\r\n          <label for=\"service_charge\">Charge(tk)</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"er_service_charge\"\r\n            v-model.trim=\"serviceSetup.er_service_charge\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n            ref=\"er_service_charge\"\r\n            v-on:keyup.enter=\"this.$refs.discount.$el.focus()\"\r\n          />\r\n          <label for=\"er_service_charge\">ER Charge(tk)</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"discount\"\r\n            v-model.trim=\"serviceSetup.discount\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n            ref=\"discount\"\r\n            v-on:keyup.enter=\"this.$refs.effect_from.$el.focus()\"\r\n          />\r\n          <label for=\"discount\">Discount(%)</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <Calendar\r\n            id=\"effect_from\"\r\n            v-model=\"serviceSetup.cdeffect_from\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"2022:2030\"\r\n            ref=\"effect_from\"\r\n          />\r\n          <label for=\"effect_from\">Effect From</label>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"p-field p-col-12 p-md-5\">\r\n        <div class=\"p-field-checkbox\">\r\n          <Checkbox id=\"active\" v-model=\"serviceSetup.active\" :binary=\"true\" />\r\n          <label for=\"active\">Active</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-5\">\r\n        <div class=\"p-field-checkbox\">\r\n          <Checkbox\r\n            id=\"vatable\"\r\n            v-model=\"serviceSetup.vatable\"\r\n            :binary=\"true\"\r\n          />\r\n          <label for=\"vatable\">Vatable</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Save\"\r\n          @click=\"add\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5; border: none\"\r\n        />\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Cancel\"\r\n          @click=\"hideNew\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #fb465e; border: none\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"p-grid crud-demo\" v-show=\"dataList\">\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <Toolbar class=\"p-mb-4\">\r\n          <template v-slot:left>\r\n            <Button\r\n              label=\"New\"\r\n              icon=\"pi pi-plus\"\r\n              class=\"p-button-success p-mr-2\"\r\n              @click=\"openNew\"\r\n            />\r\n          </template>\r\n        </Toolbar>\r\n\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"serviceList\"\r\n          dataKey=\"service_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Manage Service</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"service_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data.service_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_service_head_name\" header=\"Head\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Head</span>\r\n              {{ slotProps.data._service_head_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_service_type_name\" header=\"Type\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Type</span>\r\n              {{ slotProps.data._service_type_name }}\r\n            </template>\r\n          </Column>\r\n\r\n          <Column field=\"service_charge\" header=\"Charge\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Charge</span>\r\n              {{ slotProps.data.service_charge }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"er_service_charge\" header=\"ER Charge\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">ER Charge</span>\r\n              {{ slotProps.data.er_service_charge }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"discount\" header=\"Disc(%)\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Disc(%)</span>\r\n              {{ slotProps.data.discount }}\r\n            </template>\r\n          </Column>\r\n\r\n          <Column field=\"active\" header=\"Status\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Status</span>\r\n              <span\r\n                v-show=\"slotProps.data.active\"\r\n                class=\"product-badge status-active\"\r\n                >Active</span\r\n              >\r\n              <span\r\n                v-show=\"!slotProps.data.active\"\r\n                class=\"product-badge status-inactive\"\r\n                >Inactive\r\n              </span>\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/setup/ServiceSetup\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-active {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-inactive {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n}\r\n</style>"]}]}