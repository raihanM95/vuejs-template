div>\r\n\r\n  <div class=\"card\" v-show=\"invList\">\r\n    <h5 class=\"p-text-center\" style=\"margin-bottom: 28px\">\r\n      Patient Investigation\r\n    </h5>\r\n    <div class=\"p-fluid p-grid\">\r\n      <div class=\"p-field p-col-12 p-md-3\">\r\n        <div class=\"p-float-label\">\r\n          <Calendar\r\n            id=\"fromdate\"\r\n            v-model=\"patientInvestigation.cdfromdate\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"1800:2030\"\r\n            required=\"true\"\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientInvestigation.cdfromdate,\r\n            }\"\r\n            v-on:keyup.enter=\"this.$refs.todate.$el.focus()\"\r\n            @blur=\"show()\"\r\n          />\r\n          <label for=\"fromdate\"\r\n            >From Date<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientInvestigation.cdfromdate\"\r\n          >From Date is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-3\">\r\n        <div class=\"p-float-label\">\r\n          <Calendar\r\n            id=\"todate\"\r\n            v-model=\"patientInvestigation.cdtodate\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"1800:2030\"\r\n            required=\"true\"\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientInvestigation.cdtodate,\r\n            }\"\r\n            ref=\"todate\"\r\n            v-on:keyup.enter=\"this.$refs.delivery_status.$el.focus()\"\r\n            @blur=\"show()\"\r\n          />\r\n          <label for=\"todate\">To Date<span style=\"color: red\">*</span></label>\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientInvestigation.cdtodate\"\r\n          >To Date is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-3\">\r\n        <div class=\"p-float-label\">\r\n          <Dropdown\r\n            id=\"delivery_status\"\r\n            v-model=\"patientInvestigation.delivery_status\"\r\n            :options=\"reportDeliveryStatusList\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientInvestigation.delivery_status,\r\n            }\"\r\n            ref=\"delivery_status\"\r\n            v-on:keyup.enter=\"this.$refs.payment_status.$el.focus()\"\r\n            @change=\"show()\"\r\n          />\r\n          <label for=\"delivery_status\"\r\n            >Delivery Status<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientInvestigation.delivery_status\"\r\n          >Delivery Status is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-2\">\r\n        <div class=\"p-float-label\">\r\n          <Dropdown\r\n            id=\"payment_status\"\r\n            v-model=\"patientInvestigation.payment_status\"\r\n            :options=\"paymentStatusList\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientInvestigation.payment_status,\r\n            }\"\r\n            ref=\"payment_status\"\r\n            @change=\"show()\"\r\n          />\r\n          <label for=\"payment_status\"\r\n            >Payment Status<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientInvestigation.payment_status\"\r\n          >Payment Status is required.</small\r\n        >\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-2\"></div> -->\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Show\"\r\n          @click=\"show()\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5; border: none\"\r\n        />\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-2\">\r\n        <Button\r\n          label=\"Reset\"\r\n          @click=\"reset\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #fb465e; border: none\"\r\n        />\r\n      </div> -->\r\n    </div>\r\n  </div>\r\n  <div class=\"p-grid crud-demo\" v-if=\"dataList && invList\">\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"patientInvestigationList\"\r\n          dataKey=\"investigation_request_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Manage Investigation Request List</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"investigation_request_no\" header=\"Inv No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Inv No</span>\r\n              {{ slotProps.data.investigation_request_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"invoice_no\" header=\"Invoice No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Invoice No</span>\r\n              {{ slotProps.data.invoice_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.registration_no\"\r\n            header=\"Patient Reg No\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Reg No</span>\r\n              {{ slotProps.data.aPatient.registration_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.first_name\"\r\n            header=\"Patient Name\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.aPatient.first_name }}\r\n              {{ slotProps.data.aPatient.last_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"aPatient.contact_no\" header=\"Contact\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Contact</span>\r\n              {{ slotProps.data.aPatient.contact_no }}\r\n            </template>\r\n          </Column>\r\n          <!-- <Column\r\n            field=\"aPatient.emmergency_contact_no\"\r\n            header=\"Emergency Contact\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Emergency Contact</span>\r\n              {{ slotProps.data.aPatient.emmergency_contact_no }}\r\n            </template>\r\n          </Column> -->\r\n          <Column\r\n            field=\"investigation_request_date\"\r\n            header=\"Inv Req Date\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Inv Req Date</span>\r\n              {{ formatDate(slotProps.data.investigation_request_date) }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"cost_price\" header=\"Cost Price\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Cost Price</span>\r\n              {{ slotProps.data.cost_price }} tk\r\n            </template>\r\n          </Column>\r\n          <!-- <Column\r\n            field=\"discount_percent\"\r\n            header=\"Disc(%)\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Disc(%)</span>\r\n              {{ slotProps.data.discount_percent }} %\r\n            </template>\r\n          </Column> -->\r\n          <!-- <Column\r\n            field=\"discount_amount\"\r\n            header=\"Disc Amt\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Disc Amt</span>\r\n              {{ slotProps.data.discount_amount }} tk\r\n            </template>\r\n          </Column> -->\r\n          <Column field=\"total_amount\" header=\"Total Amt\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Total Amt</span>\r\n              {{ slotProps.data.total_amount }} tk\r\n            </template>\r\n          </Column>\r\n          <Column field=\"inv_due_amount\" header=\"Due Amt\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Due Amt</span>\r\n              {{ slotProps.data.inv_due_amount }} tk\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"investigationDeliveryStatus\"\r\n            header=\"Delivery Status\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Delivery Status</span>\r\n              <span\r\n                v-if=\"slotProps.data.investigationDeliveryStatus == 'Completed'\"\r\n                class=\"product-badge status-paid\"\r\n                >Delivered</span\r\n              >\r\n              <span\r\n                v-if=\"\r\n                  slotProps.data.investigationDeliveryStatus ==\r\n                  'Delivery Pending'\r\n                \"\r\n                class=\"product-badge status-partial-paid\"\r\n                >Pending</span\r\n              >\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"payment_status\"\r\n            header=\"Pmt Status\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Pmt Status</span>\r\n              <span\r\n                v-if=\"slotProps.data.payment_status == 'Paid'\"\r\n                class=\"product-badge status-paid\"\r\n                >Paid</span\r\n              >\r\n              <span\r\n                v-if=\"slotProps.data.payment_status == 'Partial Paid'\"\r\n                class=\"product-badge status-partial-paid\"\r\n                >Partial Paid</span\r\n              >\r\n              <span\r\n                v-if=\"slotProps.data.payment_status == 'Unpaid'\"\r\n                class=\"product-badge status-unpaid\"\r\n                >Unpaid</span\r\n              >\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Inv Token\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Inv Token</span>\r\n              <Button\r\n                icon=\"pi pi-print\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"printInvestigationToken(slotProps.data.investigation_request_id)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Money Recept\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Money Recept</span>\r\n              <Button\r\n                icon=\"pi pi-print\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"moneyReceiptPrint(slotProps.data.invoice_id)\"\r\n                v-if=\"slotProps.data.payment_status == 'Paid' || slotProps.data.payment_status == 'Partial Paid'\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from  \"../../scripts/front-desk/PatientInvestigationList\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-paid {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-unpaid {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n\r\n  &.status-partial-paid {\r\n    background: #feedaf;\r\n    color: #8a5340;\r\n  }\r\n}\r\n</style>"]}]}