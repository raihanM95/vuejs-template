\n      <div class=\"p-col-12 p-md-12\">\r\n        <div class=\"p-grid\" style=\"margin-left: 3px\">\r\n          <div class=\"p-col-12 p-md-12 card\">\r\n            <TabView ref=\"tabview2\" v-model:activeIndex=\"active\">\r\n              <TabPanel header=\"Investigation Request\">\r\n                <div class=\"p-fluid p-grid\" style=\"margin-top: 10px\">\r\n                  <div class=\"p-field p-col-12 p-md-12\">\r\n                    <div style=\"width: 72px\">\r\n                      <Button\r\n                        label=\"New\"\r\n                        icon=\"pi pi-plus\"\r\n                        class=\"p-button-success p-mr-2 p-button-sm\"\r\n                        @click=\"newInvestigationRequest\"\r\n                      />\r\n                    </div>\r\n\r\n                    <DataTable\r\n                      ref=\"dt\"\r\n                      :value=\"iPDInvestigationRequestList\"\r\n                      dataKey=\"investigation_request_id\"\r\n                      :paginator=\"true\"\r\n                      :rows=\"10\"\r\n                      v-model:filters=\"filters\"\r\n                      paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n                      :rowsPerPageOptions=\"[10, 15, 20]\"\r\n                      currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n                      class=\"p-datatable-sm\"\r\n                      responsiveLayout=\"scroll\"\r\n                    >\r\n                      <Column\r\n                        field=\"investigation_request_no\"\r\n                        header=\"INV No\"\r\n                        :sortable=\"true\"\r\n                      >\r\n                        <template #body=\"slotProps\">\r\n                          <span class=\"p-column-title\">INV No</span>\r\n                          {{ slotProps.data.investigation_request_no }}\r\n                        </temp