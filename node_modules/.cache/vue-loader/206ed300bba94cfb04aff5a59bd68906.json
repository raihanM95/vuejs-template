  <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Total</span>\r\n              {{ slotProps.data.roundqty }}\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n    <template #footer> </template>\r\n  </Dialog>\r\n  <!-- Round Doctor -->\r\n  <Dialog\r\n    v-model:visible=\"roundDoctorDialog\"\r\n    :style=\"{ width: '450px' }\"\r\n    header=\"Doctor Assign\"\r\n    :modal=\"true\"\r\n    class=\"p-fluid\"\r\n  >\r\n    <div class=\"p-field\"></div>\r\n    <div class=\"p-field\">\r\n      <label for=\"round_doctor_id\" class=\"p-mb-3\">Department</label\r\n      ><label style=\"color: red\">*</label>\r\n      <div style=\"float: right\">\r\n        <Checkbox\r\n          id=\"isMainConsultant\"\r\n          v-model=\"assignRoundDoctor.isMainConsultant\"\r\n          :binary=\"true\"\r\n          v-on:click=\"setMainConsultant()\"\r\n        />\r\n        <label for=\"isMainConsultant\"> Is Main Dr?</label>\r\n      </div>\r\n      <Dropdown\r\n        id=\"department_id\"\r\n        v-model=\"assignRoundDoctor.department_id\"\r\n        :options=\"departmentList\"\r\n        optionLabel=\"text\"\r\n        optionValue=\"value\"\r\n        placeholder=\"Select a Department\"\r\n        :filter=\"true\"\r\n        :showClear=\"true\"\r\n        :class=\"{\r\n          'p-invalid': submitted && !assignRoundDoctor.department_id,\r\n        }\"\r\n        :disabled=\"assignRoundDoctor.isMainConsultant\"\r\n      />\r\n      <small\r\n        class=\"p-invalid\"\r\n        v-if=\"submitted && !assignRoundDoctor.department_id\"\r\n        >Department is required.</small\r\n      >\r\n    </div>\r\n    <div class=\"p-field\">\r\n      <label for=\"round_doctor_id\" class=\"p-mb-3\">Doctor</label\r\n      ><label style=\"color: red\">*</label>\r\n      <Dropdown\r\n        id=\"round_doctor_id\"\r\n        v-model=\"assignRoundDoctor.round_doctor_id\"\r\n        :options=\"filterDoctors