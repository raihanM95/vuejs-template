olumn>\r\n          <Column field=\"patient_name\" header=\"Patient Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.patient_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"word_cabin_name\" header=\"Word/Cabin\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Word/Cabin</span>\r\n              {{ slotProps.data.word_cabin_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"bed_name\" header=\"Bed\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Bed</span>\r\n              {{ slotProps.data.bed_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"doctor_name\" header=\"Doctor\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Doctor</span>\r\n              {{ slotProps.data.doctor_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"admission_date\" header=\"ADM Date\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">ADM Date</span>\r\n              {{ formatDate(slotProps.data.admission_date) }}\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Package\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Package</span>\r\n              <Button\r\n                icon=\"pi pi-plus-circle\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"takePackage(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Service\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Service</span>\r\n              <Button\r\n                icon=\"pi pi-plus-circle\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"takeService(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Taken List\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Taken List</span>\r\n              <Button\r\n                icon=\"pi pi-eye\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"takenList(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Request\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Request</span>\r\n              <Button\r\n                v-show=\"isNurse\"\r\n                icon=\"pi fa fa-info-circle\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"collectionRequest(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Doctor\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Doctor</span>\r\n              <Button\r\n                icon=\"fa fa-user-md\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"viewRoundDoctor(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <!-- Prescribed Investigation -->\r\n  <Dialog\r\n    v-model:visible=\"investigationRequestDialog\"\r\n    :style=\"{ width: '600px' }\"\r\n    header=\"Prescribed Investigation\"\r\n    :modal=\"true\"\r\n    class=\"p-fluid\"\r\n  >\r\n    <div class=\"card\">\r\n      <DataTable\r\n        :value