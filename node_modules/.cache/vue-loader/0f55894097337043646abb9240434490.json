>\r\n          <template v-slot:right>\r\n            <Button\r\n              label=\"Print All\"\r\n              icon=\"pi pi-print\"\r\n              class=\"p-button-help\"\r\n              @click=\"printAllPatient\"\r\n            />\r\n          </template>\r\n        </Toolbar>\r\n\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"patientRegistrationList\"\r\n          dataKey=\"patient_id\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Manage Patient Reg</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                  v-on:keyup=\"onPage(0)\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n          <Column field=\"registration_no\" header=\"Reg No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Reg No</span>\r\n              {{ slotProps.data.registration_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"first_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data.first_name }}\r\n              {{ slotProps.data.last_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"gender\" header=\"Gender\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Gender</span>\r\n              {{ slotProps.data.gender }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"blood_group\" header=\"Blood\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Blood</span>\r\n              {{ slotProps.data.blood_group }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"contact_no\" header=\"Contact No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Contact No</span>\r\n              {{ slotProps.data.contact_no }}\r\n            </template>\r\n          </Column>\r\n          <Column header=\"ID Card\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">ID Card</span>\r\n              <Button\r\n                icon=\"pi pi-print\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"printID(slotProps.data.registration_no)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Service\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Service</span>\r\n              <Button\r\n                icon=\"pi pi-plus-circle\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"takenList(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Transfer IPD\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Transfer IPD</span>\r\n              <Button\r\n                icon=\"pi pi-directions\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"transferIPD(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            header=\"Action\"\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                v-show=\"!isDoctor\"\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n              <Button\r\n                v-show=\"isDoctor\"\r\n                icon=\"pi pi-file-excel\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"prescription(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          \r\n        </DataTable>\r\n        <!-- server side pagination -->\r\n        <Paginator\r\n          :rows=\"10\"\r\n          :totalRecords=\"page\"\r\n          :rowsPerPageOptions=\"[10, 20, 30]\"\r\n          @page=\"onPage($event)\"\r\n        ></Paginator>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/emergency/PatientRegTreatment\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.drill-down {\r\n  float: right;\r\n  cursor: pointer;\r\n  font-weight: 900;\r\n}\r\n.rx-new {\r\n  cursor: pointer;\r\n  font-weight: 900;\r\n}\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-success {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-unsuccessful {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n\r\n  &.status-warning {\r\n    background: #feedaf;\r\n    color: #8a5340;\r\n  }\r\n}\r\n\r\n.margin-top {\r\n  margin-top: 10px;\r\n}\r\n\r\n.p-border {\r\n  border: 1px solid #6cc3d5 !important;\r\n  border-radius: 5px;\r\n  padding: 5px;\r\n  padding-