n  <div class=\"card\" v-show=\"!isHistory && !dataList && !newInfo\">\r\n    <!-- <h3 class=\"p-text-center\" style=\"margin-bottom: 28px\">Prescription</h3> -->\r\n    <p class=\"p-border\">\r\n        <table style=\"width: 100%;\">\r\n          <tr>\r\n            <th style=\"text-align: left\">Reg No: {{ admittedPatientInfo.registration_no }}</th>\r\n            <th style=\"text-align: right\">Age: {{ admittedPatientInfo.age_year }} Y {{ admittedPatientInfo.age_month }} M {{ admittedPatientInfo.age_day }} D | Gender: {{ admittedPatientInfo.gender }}</th>\r\n          </tr>\r\n          <tr>\r\n            <th style=\"text-align: left\">Name: {{ admittedPatientInfo.patient_name }}</th>\r\n            <th style=\"text-align: right\">Date: {{ admissionPrescriptionInfo.cdprescription_date }}</th>\r\n          </tr>\r\n        </table>\r\n      </p>\r\n    <div class=\"p-grid\">\r\n      <Prescription ref=\"prescriptionComponent\"/>\r\n    </div>\r\n    <!-- <Panel :header=headerName>\r\n            <p style=\"height: 425px;\">\r\n              \r\n              <DataTable :value=\"tempData\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" v-model:filters=\"filters\" :scrollable=\"true\" scrollHeight=\"315px\" v-show=\"tempData.length > 0\">\r\n                <div class=\"p-fluid p-grid\">\r\n                  <div class=\"p-field p-col-12 p-md-12\">\r\n                    <span class=\"p-input-icon-left\">\r\n                      <i class=\"pi pi-search\" />\r\n                      <InputText\r\n                        v-model=\"filters['global'].value\"\r\n                        placeholder=\"Search...\"\r\n                      />\r\n                    </span>\r\n                    <div v-show=\"isAdvice\" style=\"right: 0;position: absolute;padding-top: 5px;\">\r\n                    <input\r\n                      type=\"checkbox\"\r\n                      id=\"addAllAdvice\"\r\n                      name=\"addAllAdvice\"\r\n                      v-on:click=\"addAllAdvice()\"\r\n                      v-model=\"admissionPrescriptionInfo.allAdvice\"\r\n                    />\r\n                    <label for=\"addAllAdvice\">Select All</label>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              \r\n              <Column field=\"name\" style=\"margin: 0;padding: 1px\">\r\n                <template #body=\"slotProps\">\r\n                  <label style=\"cursor: cell;margin: 0;padding: 1px\" @click=\"getValue(slotProps.data)\">{{slotProps.data.name}}</label>\r\n                </template>\r\n              </Column>\r\n            </DataTable>\r\n            </p>\r\n        </Panel> -->\r\n        <div class=\"p-fluid p-grid\">\r\n          <div class=\"p-field p-col-12 p-md-4\">\r\n            <div class=\"p-float-label\">\r\n              <Calendar\r\n            id=\"folowup_date\"\r\n            v-model=\"admittedPatientInfo.cdfolowup_date\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"2022:2040\"\r\n            :minDate=\"minDate\"\r\n            :manualInput=\"false\"\r\n          />\r\n          <label for=\"folowup_date\"><b><i>Next Visit Date </i></b></label\r\n          >\r\n        </div>\r\n          </div>\r\n          <div class=\"p-field p-col-12 p-md-2\">\r\n            <Button\r\n              label=\"History\"\r\n              @click=\"historyCollection()\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #56CC9D; border: none\"\r\n            />\r\n          </div>\r\n        \r\n          <div class=\"p-field p-col-12 p-md-2\">\r\n            <Button\r\n              label=\"New\"\r\n              @click=\"newPrescription()\"\r\n              class=\"p-button-lg\"\r\n              style=\"background-color: #6CC3D5; border: none\"\r\n            />\r\n          </div>\r\n          <div class=\"p-field p-col-12 p-md-2\">\r\n            <Button\r\n              labe