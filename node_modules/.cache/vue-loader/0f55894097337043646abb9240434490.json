    <div class=\"p-field p-col-12 p-md-2\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"age_year\"\r\n            v-model.trim=\"patientRegistration.age_year\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientRegistration.age_year,\r\n            }\"\r\n            ref=\"age_year\"\r\n            v-on:keyup.enter=\"this.$refs.age_month.$el.focus()\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n          />\r\n          <label for=\"age_year\"\r\n            >Age (Y)<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientRegistration.age_year\"\r\n          >Age (Y) is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-2\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"age_month\"\r\n            v-model.trim=\"patientRegistration.age_month\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientRegistration.age_month,\r\n            }\"\r\n            ref=\"age_month\"\r\n            v-on:keyup.enter=\"this.$refs.age_day.$el.focus()\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n          />\r\n          <label for=\"age_month\"\r\n            >Age (M)<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientRegistration.age_month\"\r\n          >Age (M) is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-2\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"age_day\"\r\n            v-model.trim=\"patientRegistration.age_day\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientRegistration.age_day,\r\n            }\"\r\n            ref=\"age_day\"\r\n            v-on:keyup.enter=\"this.$refs.contact_no.$el.focus()\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n          />\r\n          <label for=\"age_day\"\r\n            >Age (D)<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientRegistration.age_day\"\r\n          >Age (D) is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"contact_no\"\r\n            v-model.trim=\"patientRegistration.contact_no\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientRegistration.contact_no,\r\n            }\"\r\n            ref=\"contact_no\"\r\n            v-on:keyup.enter=\"this.$refs.email.$el.focus()\"\r\n            v-on:keyup=\"isExistContact()\"\r\n          />\r\n          <label for=\"contact_no\"\r\n            >Contact No<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientRegistration.contact_no\"\r\n          >Contact No is required.</small\r\n        >\r\n        <div v-if=\"isContactExist\" style=\"color: red\">\r\n          {{ isContactExist }}\r\n        </div>\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"email\"\r\n            v-model.trim=\"patientRegistration.email\"\r\n            autofocus\r\n            ref=\"email\"\r\n            v-on:keyup.enter=\"this.$refs.nid.$el.focus()\"\r\n            v-on:keyup=\"this.validation.is