text-align: right\"\r\n          footerStyle=\"width: 123px; text-align: right\"\r\n        >\r\n          <template #body=\"slotProps\">\r\n            <span class=\"p-column-title\">Total</span>\r\n            {{ slotProps.data.taken_service_no }}\r\n          </template>\r\n        </Column>\r\n      </DataTable>\r\n    </div>\r\n  </div>\r\n    <template #footer>\r\n      <Button\r\n        label=\"Close\"\r\n        icon=\"pi pi-times\"\r\n        class=\"p-button-text\"\r\n        @click=\"hideDialog\"\r\n      />\r\n    </template>\r\n  </Dialog>\r\n  <div class=\"card\" v-show=\"newInfo\">\r\n    <h5 class=\"p-text-center\" style=\"margin-bottom: 28px\">Patient Reg Info</h5>\r\n    <div class=\"p-fluid p-grid\">\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"first_name\"\r\n            v-model.trim=\"patientRegistration.first_name\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !patientRegistration.first_name,\r\n            }\"\r\n            v-on:keyup.enter=\"this.$refs.last_name.$el.focus()\"\r\n          />\r\n          <label for=\"first_name\"\r\n            >First Name<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !patientRegistration.first_name\"\r\n          >First Name is required.</small\r\n        >\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"last_name\"\r\n            v-model.trim=\"patientRegistration.last_name\"\r\n            autofocus\r\n            ref=\"last_name\"\r\n            v-on:keyup.enter=\"this.$refs.gender.$el.focus()\"\r\n          />\r\n          <label for=\"last_name\">Last Name</label>\r\n        </div>\r\n      </div> -->\r\n      <div cl