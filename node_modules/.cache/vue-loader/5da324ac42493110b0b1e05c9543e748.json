-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"system_and_substance_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data.system_and_substance_name }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"_system_and_substance_category_name\"\r\n            header=\"Category\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Category</span>\r\n              {{ slotProps.data._system_and_substance_category_name }}\r\n            </template>\r\n          </Column>\r\n          \r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                v-if=\"slotProps.data.system_and_substance_id != 120 && slotProps.data.system_and_substance_id != 121\"\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/setup/SystemAndSubstanceSetup\";\r\nexport default app;\r\n</script>"]}]}