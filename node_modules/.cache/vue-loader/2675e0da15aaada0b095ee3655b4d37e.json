\r\n        </div>\r\n        <div class=\"p-field\">\r\n          <label for=\"nurse_id\" class=\"p-mb-3\"\r\n            >Qty ({{ usingProduct._uom_name }})</label\r\n          ><label style=\"color: red\">*</label>\r\n          <InputText\r\n            id=\"qty\"\r\n            v-model.trim=\"usingProduct.qty\"\r\n            :class=\"{ 'p-invalid': submitted && !usingProduct.qty }\"\r\n            type=\"number\"\r\n            :min=\"0\"\r\n          />\r\n          <small class=\"p-invalid\" v-if=\"submitted && !usingProduct.qty\"\r\n            >Qty is required.</small\r\n          >\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <template #footer>\r\n      <Button\r\n        label=\"Close\"\r\n        icon=\"pi pi-times\"\r\n        class=\"p-button-text\"\r\n        @click=\"hideDialog\"\r\n      />\r\n      <Button\r\n        label=\"Submit\"\r\n        icon=\"pi pi-check\"\r\n        class=\"p-button-text\"\r\n        @click=\"addUsing\"\r\n      />\r\n    </template>\r\n  </Dialog>\r\n  <!-- using product end -->\r\n  <!-- used product start -->\r\n  <Dialog\r\n    v-model:visible=\"usedProductInfoDialog\"\r\n    :style=\"{ width: '900px' }\"\r\n    header=\"Used Products Info\"\r\n    :modal=\"true\"\r\n    class=\"p-fluid\"\r\n  >\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <!-- using new -->\r\n        <Toolbar class=\"p-mb-4\">\r\n          <template v-slot:left>\r\n            <Button\r\n              label=\"New\"\r\n              icon=\"pi pi-plus\"\r\n              class=\"p-button-success p-mr-2 p-button-sm\"\r\n              @click=\"usingProductInfo\"\r\n            />\r\n          </template>\r\n        </Toolbar>\r\n        <!-- used list -->\r\n        <DataTable\r\n          :value=\"usedProductList\"\r\n          class=\"p-datatable-sm\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <Column field=\"product_code\" header=\"Code\"></Column>\r\n          <Column field=\"_product_name\" header=\"Product\"></Column>\r\n          <Column field=\"_uom_name\" header=\"Used UOM\"></Column>\r\n          <Column field=\"use_qty\" header=\"Used Qty\"></Column>\r\n          <Column field=\"create_date\" header=\"Used Date\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Used Date</span>\r\n              {{ formatDate(slotProps.data.create_date) }}\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n    <!-- <template #footer>\r\n        <Button\r\n          label=\"Close\"\r\n          icon=\"pi pi-times\"\r\n          class=\"p-button-text\"\r\n          @click=\"hideDialog\"\r\n        />\r\n      </template> -->\r\n  </Dialog>\r\n  <!-- used product end -->\r\n  <div class=\"card\">\r\n    <h5 class=\"p-text-center\" style=\"margin-bottom: 28px\">Using Product</h5>\r\n    <div class=\"p-fluid p-grid\">\r\n      <div class=\"p-field p-col-12 p-md-5\">\r\n        <div class=\"p-float-label\">\r\n          <Calendar\r\n            id=\"fromdate\"\r\n            v-model=\"pendingSampleCollection.cdfromdate\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"1800:2030\"\r\n            :class=\"{\r\n              'p-invalid': submitted && !pendingSampleCollection.cdfromdate,\r\n            }\"\r\n            @blur=\"show()\"\r\n          />\r\n          <label for=\"fromdate\"\r\n            >From Date<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !pendingSampleCollection.cdfromdate\"\r\n          >From Date is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-5\">\r\n        <div class=\"p-float-label\">\r\n          <Calendar\r\n            id=\"todate\"\r\n            v-model=\"pendingSampleCollection.cdtodate\"\r\n            :monthNavigator=\"true\"\r\n            :yearNavigator=\"true\"\r\n            yearRange=\"1800:2030\"\r\n            :class=\"{\r\n              'p-invalid': submitted && !pendingSampleCollection.cdtodate,\r\n            }\"\r\n            @blur=\"show()\"\r\n          />\r\n          <label for=\"todate\">To Date<span style=\"color: red\">*</span></label>\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !pendingSampleCollection.cdtodate\"\r\n          >To Date is required.</small\r\n        >\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-2\"></div> -->\r\n      <div class=\"p-field p-col-12 p-md-2\">\r\n        <Button\r\n          label=\"Show\"\r\n          @click=\"show()\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5; border: none\"\r\n        />\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-2\">\r\n          <Button\r\n            label=\"Reset\"\r\n            @click=\"reset\"\r\n            class=\"p-button-lg\"\r\n            style=\"background-color: #fb465e; border: none\"\r\n          />\r\n        </div> -->\r\n    </div>\r\n  </div>\r\n  <div class=\"p-grid crud-demo\" v-show=\"dataList\">\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"pendingSampleCollectionList\"\r\n          dataKey=\"appoinment_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Sample Collected List</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column\r\n            field=\"_investigation_request_no\"\r\n            header=\"No\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">No</span>\r\n              {{ slotProps.data._investigation_request_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"_investingation_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data._investingation_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"barcode_or_level\" header=\"Barcode\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Barcode</span>\r\n              {{ slotProps.data.barcode_or_level }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.registration_no\"\r\n            header=\"Patient Reg No\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Reg No</span>\r\n              {{ slotProps.data.aPatient.registration_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.first_name\"\r\n            header=\"Patient Name\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.aPatient.first_name }}\r\n              {{ slotProps.data.aPatient.last_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"aPatient.contact_no\" header=\"Contact\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Contact</span>\r\n              {{ slotProps.data.aPatient.contact_no }}\r\n            </template>\r\n          </Column>\r\n\r\n          <Column\r\n            field=\"_investigation_status\"\r\n            header=\"Status\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Status</span>\r\n              <span\r\n                v-if=\"\r\n                  slotProps.data._investigation_status == 'Sent to Diagnosis'\r\n                \"\r\n                class=\"product-badge status-paid\"\r\n                >Send</span\r\n              >\r\n              <span\r\n                v-if=\"\r\n                  slotProps.data._investigation_status == 'Sample Collected'\r\n                \"\r\n                class=\"product-badge status-partial-paid\"\r\n                >Collected</span\r\n              >\r\n              <span\r\n                v-if=\"\r\n                  slotProps.data._investigation_status == 'Waiting For Sample'\r\n                \"\r\n                class=\"product-badge status-unpaid\"\r\n                >Waiting</span\r\n              >\r\n            </template>\r\n          </Column>\r\n          <Column header=\"Print\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Print</span>\r\n              <Button\r\n                icon=\"pi pi-print\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"print(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n            header=\"Action\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Action</span>\r\n              <Button\r\n                icon=\"pi pi-inbox\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"useEntry(slotProps.data.investigation_request_id)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nimport app from \"../../scripts/inventory/InventoryInvestigationUse\";\r\nexport default app;\r\n</script>\r\n  \r\n  <style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-paid {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-unpaid {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n\r\n  &.status-partial-paid {\r\n    background: #feedaf;\r\n    color: #8a5340;\r\n  }\r\n}\r\n</style>"]}]}