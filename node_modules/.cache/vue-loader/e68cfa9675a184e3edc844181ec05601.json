ba(0,0,0,.2), 0 1px 1px 0 rgba(0,0,0,.14), 0 1px 3px 0 rgba(0,0,0,.12);\r\n\t}\r\n\r\n\t.p-d-flex > div {\r\n\t\twidth: 8rem;\r\n\t}\r\n\r\n\t::v-deep(pre[class*=\"language-\"]) {\r\n\t\t&:after, &:before {\r\n\t\t\tbox-shadow: none;\r\n\t\t}\r\n\r\n\t\tpadding: 0 !important;\r\n\t\tbackground: transparent;\r\n\t\toverflow: visible;\r\n\r\n\t\t> code {\r\n\t\t\tbox-shadow: none !important;\r\n\t\t}\r\n\t}\r\n\r\n\ti:not([class~=\"pi\"]) {\r\n\t\tbackground-color: #ffffff;\r\n\t\tcolor: #2196f3;\r\n\t\tfont-family: Monaco, courier, monospace;\r\n\t\tfont-style: normal;\r\n\t\tfont-size: 12px;\r\n\t\tpadding: 2px 4px;\r\n\t\tletter-spacing: .5px;\r\n\t\tborder-radius: 3px;\r\n\t\tfont-weight: 600;\r\n\t\tmargin: 0 2px;\r\n\t}\r\n</style>\r\n"]}]}