"pi pi-check\"\r\n          class=\"p-button-text\"\r\n          @click=\"addDiabetesChart\"\r\n        />\r\n      </template>\r\n    </Dialog>\r\n    <!-- Diet Chart -->\r\n    <Dialog\r\n      v-model:visible=\"dietChartDialog\"\r\n      :style=\"{ width: '850px' }\"\r\n      header=\"Diet Chart\"\r\n      :modal=\"true\"\r\n      class=\"p-fluid\"\r\n    >\r\n      <div class=\"p-field\">\r\n        <label for=\"diet_chart_template_id\"\r\n          >Template<span style=\"color: red\">*</span></label\r\n        >\r\n        <Dropdown\r\n          id=\"diet_chart_template_id\"\r\n          v-model=\"dietChart.diet_chart_template_id\"\r\n          :options=\"dietChartTemplateList\"\r\n          optionLabel=\"text\"\r\n          optionValue=\"value\"\r\n          required=\"true\"\r\n          autofocus\r\n          :class=\"{\r\n            'p-invalid': submitted && !dietChart.diet_chart_template_id,\r\n          }\"\r\n          @change=\"getDietChartByTemplateId()\"\r\n        />\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !dietChart.diet_chart_template_id\"\r\n          >Template is required.</small\r\n        >\r\n      </div>\r\n  \r\n      <div class=\"card\">\r\n        <DataTable\r\n          :value=\"dietChartTempList\"\r\n          class=\"p-datatable-sm\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <Column field=\"meal\" header=\"Meal\"></Column>\r\n          <Column field=\"item\" header=\"Item\"></Column>\r\n          <Column field=\"fat_calories\" header=\"Fat (gm) Calories\"></Column>\r\n          <Column field=\"fibre\" header=\"Fibre\"></Column>\r\n          <Column field=\"gm\" header=\"(gm)\"></Column>\r\n        </DataTable>\r\n      </div>\r\n      <template #footer>\r\n        <Button\r\n          label=\"Close\"\r\n          icon=\"pi pi-times\"\r\n          class=\"p-button-text\"\r\n          @click=\"hideDialog\"\r\n        />\r\n        <Button\r\n          label=\"Submit\"\r