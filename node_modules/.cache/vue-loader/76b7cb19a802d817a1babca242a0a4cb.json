\"iOChart.intake_iv1\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"intake_iv2\">IV (2)</label>\r\n          <InputNumber\r\n            id=\"intake_iv2\"\r\n            v-model=\"iOChart.intake_iv2\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"intake_other_name\">Other Name</label>\r\n          <InputText\r\n            id=\"intake_other_name\"\r\n            v-model.trim=\"iOChart.intake_other_name\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"intake_other_value\">Other Value</label>\r\n          <InputNumber\r\n            id=\"intake_other_value\"\r\n            v-model=\"iOChart.intake_other_value\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"intake_note\">Note</label>\r\n          <InputText id=\"intake_note\" v-model.trim=\"iOChart.intake_note\" />\r\n        </div>\r\n      </div>\r\n      <Divider align=\"center\">\r\n        <div class=\"inline-flex align-items-center\">\r\n          <b>Output</b>\r\n        </div>\r\n      </Divider>\r\n      <div class=\"p-fluid p-grid\">\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_urine\">Urine</label>\r\n          <InputNumber\r\n            id=\"output_urine\"\r\n            v-model=\"iOChart.output_urine\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_ng\">NG</label>\r\n          <InputNumber\r\n            id=\"output_ng\"\r\n            v-model=\"iOChart.output_ng\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_other_vomit\">Vomit</label>\r\n          <InputNumber\r\n            id=\"output_other_vomit\"\r\n            v-model=\"iOChart.output_other_vomit\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_other_bowels\">Bowels</label>\r\n          <InputNumber\r\n            id=\"output_other_bowels\"\r\n            v-model=\"iOChart.output_other_bowels\"\r\n            mode=\"decimal\"\r\n            :minFractionDigits=\"1\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_initials\">Initial</label>\r\n          <InputText\r\n            id=\"output_initials\"\r\n            v-model.trim=\"iOChart.output_initials\"\r\n          />\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <label for=\"output_note\">Note</label>\r\n          <InputText id=\"output_note\" v-model.trim=\"iOChart.output_note\" />\r\n        </div>\r\n      </div>\r\n  \r\n      <template #footer>\r\n        <Button\r\n          label=\"Close\"\r\n          icon=\"pi pi-times\"\r\n          class=\"p-button-text\"\r\n          @click=\"hideDialog\"\r\n        />\r\n        <Button\r\n          label=\"Submit\"\r\n          icon=\"pi pi-check\"\r\n          class=\"p-button-text\"\r\n          @click=\"addIOChart\"\r\n        />\r\n      </template>\r\n    </Dialog>\r\n    <!-- Diabetes Chart -->\r\n    <Dialog\r\n      v-model:visible=\"diabetesChartDialog\"\r\n      :style=\"{ width: '450px' }\"\r\n      header=\"Diabetes Chart\"\r\n      :modal=\"true\"\r\n      class=\"p-fluid\"\r\n    >\r\n      <div class=\"p-field\">\r\n        <label for=\"meal\">Meal<span style=\"c