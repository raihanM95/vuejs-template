} of {totalRecords}\"\r\n            responsiveLayout=\"scroll\"\r\n          >\r\n            <template #header>\r\n              <div\r\n                class=\"\r\n                  table-header\r\n                  p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n                \"\r\n              >\r\n                <h5 class=\"p-m-0\">Emergency Patient List</h5>\r\n                <span class=\"p-input-icon-left\">\r\n                  <i class=\"pi pi-search\" />\r\n                  <InputText\r\n                    v-model=\"filters['global'].value\"\r\n                    placeholder=\"Search...\"\r\n                  />\r\n                </span>\r\n              </div>\r\n            </template>\r\n  \r\n            <Column field=\"emergency_no\" header=\"No\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">No</span>\r\n                {{ slotProps.data.emergency_no }}\r\n              </template>\r\n            </Column>\r\n            <Column\r\n              field=\"registration_no\"\r\n              header=\"Patient Reg No\"\r\n              :sortable=\"true\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Reg No</span>\r\n                {{ slotProps.data.registration_no }}\r\n              </template>\r\n            </Column>\r\n  \r\n            <Column field=\"first_name\" header=\"Patient Name\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Name</span>\r\n                {{ slotProps.data.first_name }}\r\n                {{ slotProps.data.last_name }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"contact_no\" header=\"Contact\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Contact</span>\r\n               