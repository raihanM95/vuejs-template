BuC,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,GAAlB,CAAtB;AACA,WAAKzC,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD;AAjGM;AAxCI,CAAf","sourcesContent":["import NutritionSetupService from \"../../service/setup/NutritionSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      departmentList: [],\r\n      nutritionList: [],\r\n      nutritionSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.nutritionSetupService = new NutritionSetupService();\r\n    this.validation = new Validation();\r\n    this.consultancyDepartmentSetupService =\r\n      new ConsultancyDepartmentSetupService();\r\n\r\n    this.nutritionSetupService\r\n      .getAll()\r\n      .then((data) => (this.nutritionList = data.data));\r\n\r\n    this.consultancyDepartmentSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.departmentList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.nutritionSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.nutritionSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.nutritionSetup.nutrition_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.nutritionList, 'nutrition_name', this.nutritionSetup.nutrition_name, 'nutrition_id', this.nutritionSetup.nutrition_id, this)) {\r\n          return;\r\n        } else if (!this.nutritionSetup.consultancy_department_id) {\r\n          return;\r\n        } else {\r\n          if (!this.nutritionSetup.nutrition_id) {\r\n            let result = await this.nutritionSetupService.add(this.nutritionSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.nutritionSetupService\r\n                .getAll()\r\n                .then((data) => (this.nutritionList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.nutritionSetupService.put(\r\n              this.nutritionSetup,\r\n              this.nutritionSetup.nutrition_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.nutritionSetupService\r\n                .getAll()\r\n                .then((data) => (this.nutritionList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.nutritionSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}