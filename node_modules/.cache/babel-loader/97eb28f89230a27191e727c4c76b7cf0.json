,\r\n                        constraints: [\r\n                            { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n                        ],\r\n                    },\r\n                };\r\n            },\r\n            submitted: false,\r\n            submitted2: false,\r\n            submitted3: false,\r\n            packageDialog: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.servicePackageDetailsSetupService = new ServicePackageDetailsSetupService();\r\n        this.serviceSetupService = new ServiceSetupService();\r\n        this.servicePackageSetupService = new ServicePackageSetupService();\r\n        this.validation = new Validation();\r\n\r\n        this.servicePackageSetupService\r\n                .getActiveCombo()\r\n                .then((data) => (this.packageList = data.data));\r\n\r\n        this.serviceSetupService\r\n            .getActiveCombo()\r\n            .then((data) => (this.serviceList = data.data));\r\n    },\r\n    methods: {\r\n        getServicePackage() {\r\n            this.servicePackageSetupService\r\n                .getActiveCombo()\r\n                .then((data) => this.getServicePackageFormatted(data.data));\r\n        },\r\n        getServicePackageFormatted(data){\r\n            this.packageList = data;\r\n            this.packageDialog = false;\r\n            this.submitted2 = false;\r\n        },\r\n        hideNew() {\r\n            //this.servicePackageDetailsList = [];\r\n            this.servicePackageDetailsSetup = {\r\n                lstServicePackageDetails: []\r\n            };\r\n            this.servicePackageSetup = {};\r\n\r\n            this.submitted = false;\r\n            this.submitted2 = false;\r\n            this.submitted3 = false;\r\n            this.isExist = \"\";\r\n            this.addList = false;\r\n            this.button = false;\r\n        },\r\n\r\n        packageWiseServiceList() {\r\n            this.servicePackageDetailsSetupService\r\n                .getServiceByPackageId(this.servicePackageDetailsSetup.service_package_id)\r\n                .then((data) => this.formatPackageDetailsList(data.data));\r\n        },\r\n        formatPackageDetailsList(data) {\r\n            this.servicePackageDetailsSetup = data;\r\n            // this.testParameterList = data.filter(function (el) {\r\n            //     return el.machine_id == this.testParameterSetup.machine_id;\r\n            // }, this);\r\n            this.addList = true;\r\n            this.button = true;\r\n        },\r\n        addService() {\r\n            this.submitted = true;\r\n            var aService = this.servicePackageDetailsSetup.lstServicePackageDetails.filter(function (el) {\r\n                return (\r\n                    el.service_id ===\r\n                    this.servicePackageDetailsSetup.ddlservice.value\r\n                );\r\n            }, this);\r\n\r\n            if (aService.length) {\r\n                this.isExist = \"This Service already added!\";\r\n            } else {\r\n                if (!this.servicePackageDetailsSetup.service_package_id) {\r\n                    return;\r\n                } else if (!this.servicePackageDetailsSetup.ddlservice) {\r\n                    return;\r\n                } else {\r\n                    this.isExist = \"\";\r\n                    this.servicePackageDetailsSetup.lstServicePackageDetails.push({\r\n                        service_package_id: this.servicePackageDetailsSetup.service_package_id,\r\n                        service_id: this.servicePackageDetailsSetup.ddlservice.value,\r\n                        _service_name: this.servicePackageDetailsSetup.ddlservice.text,\r\n\r\n                    });\r\n                }\r\n            }\r\n\r\n            this.addList = true;\r\n            this.button = true;\r\n        },\r\n        deleteFromList(data) {\r\n            this.servicePackageDetailsSetup.lstServicePackageDetails = this.servicePackageDetailsSetup.lstServicePackageDetails.filter(function (el) {\r\n                return el.service_id != data.service_id;\r\n            }, this);\r\n\r\n            if (!this.servicePackageDetailsSetup.lstServicePackageDetails.length) {\r\n                this.addList = false;\r\n                this.button = false;\r\n            } else {\r\n                this.button = true;\r\n            }\r\n        },\r\n        newPackage() {\r\n            this.packageDialog = true;\r\n        },\r\n        hideDialog() {\r\n            this.servicePackageDetailsSetup = {\r\n                lstServicePackageDetails: []\r\n            };\r\n            this.servicePackageSetup = {};\r\n            \r\n            this.packageDialog = false;\r\n            this.submitted2 = false;\r\n        },\r\n        async addPackage() {\r\n            try {\r\n                this.submitted2 = true;\r\n                if (!this.servicePackageSetup.service_package_name) {\r\n                    return;\r\n                } else if (this.validation.isNameExists(this.packageList, 'text', this.servicePackageSetup.service_package_name, 'value', this.servicePackageSetup.service_package_id, this)) {\r\n                    return;\r\n                } else {\r\n                    let result = await this.servicePackageSetupService.add(\r\n                        this.servicePackageSetup\r\n                    );\r\n                    if (result.status === 201) {\r\n                        this.$toast.add({\r\n                            severity: \"success\",\r\n                            summary: \"Success\",\r\n                            detail: \"Saved Successfully\",\r\n                            life: 10000,\r\n                        });\r\n                        this.getServicePackage();\r\n                        this.hideNew();\r\n                        //this.hideDialog();\r\n                    } else {\r\n                        this.$toast.add({\r\n                            severity: \"error\",\r\n                            summary: \"Failed\",\r\n                            detail: result.res,\r\n                            life: 10000,\r\n                        });\r\n                    }\r\n                }\r\n            } catch (error) {\r\n                this.$toast.add({\r\n                    severity: \"error\",\r\n                    summary: \"Failed\",\r\n                    detail: error,\r\n                    life: 10000,\r\n                });\r\n            }\r\n        },\r\n        async add() {\r\n            try {\r\n                this.submitted3 = true;\r\n                if (!this.servicePackageDetailsSetup.service_package_id) {\r\n                    return;\r\n                } else if (!this.servicePackageDetailsSetup.charge) {\r\n                    return;\r\n                } else if (!this.servicePackageDetailsSetup.lstServicePackageDetails.length) {\r\n                    this.isExist = \"Please add a Service\";\r\n                    return;\r\n                } else {\r\n                    let result = await this.servicePackageDetailsSetupService.put(\r\n                        this.servicePackageDetailsSetup\r\n                    );\r\n                    if (result.status === 202) {\r\n                        this.$toast.add({\r\n                            severity: \"success\",\r\n                            summary: \"Success\",\r\n                            detail: \"Saved Successfully\",\r\n                            life: 10000,\r\n                        });\r\n                        \r\n                        this.hideNew();\r\n                    } else if (result.status === 401) {\r\n                        this.$toast.add({\r\n                            severity: \"error\",\r\n                            summary: \"Error\",\r\n                            detail: result.data,\r\n                            life: 10000,\r\n                        });\r\n                    } else {\r\n                        this.$toast.add({\r\n                            severity: \"error\",\r\n                            summary: \"Failed\",\r\n                            detail: result.res,\r\n                            life: 10000,\r\n                        });\r\n                    }\r\n                }\r\n            } catch (error) {\r\n                this.$toast.add({\r\n                    severity: \"error\",\r\n                    summary: \"Failed\",\r\n                    detail: error,\r\n                    life: 10000,\r\n                });\r\n            }\r\n        },\r\n    },\r\n};"]}]}