     constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.adviceSetupService = new AdviceSetupService();\r\n    this.adviceGroupSetupService = new AdviceGroupSetupService();\r\n    this.validation = new Validation();\r\n    this.consultancyDepartmentSetupService =\r\n      new ConsultancyDepartmentSetupService();\r\n\r\n    this.adviceGroupSetupService\r\n      .getAll()\r\n      .then((data) => (this.adviceGroupList = data.data));\r\n\r\n    this.adviceSetupService\r\n      .getAll()\r\n      .then((data) => (this.adviceList = data.data));\r\n\r\n    this.consultancyDepartmentSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.departmentList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.adviceSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.adviceSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.adviceSetup.advice_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.adviceList, 'advice_name', this.adviceSetup.advice_name, 'advice_id', this.adviceSetup.advice_id, this)) {\r\n          return;\r\n        } else if (!this.adviceSetup.advice_group_id) {\r\n          return;\r\n        } else if (!this.adviceSetup.consultancy_department_id) {\r\n          return;\r\n        } else {\r\n          if (!this.adviceSetup.advice_id) {\r\n            let result = await this.adviceSetupService.add(this.adviceSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Saved Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.adviceSetupService\r\n                .getAll()\r\n                .then((data) => (this.adviceList = data.data));\r\n            } else if (result.status === 400) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: \"Duplicate name\",\r\n                life: 10000,\r\n              });\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.adviceSetupService.put(\r\n              this.adviceSetup,\r\n              this.adviceSetup.advice_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Updated Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.adviceSetupService\r\n                .getAll()\r\n                .then((data) => (this.adviceList = data.data));\r\n            } else if (result.status === 400) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: \"Duplicate name\",\r\n                life: 10000,\r\n              });\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Faild\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.adviceSetup = Object.assign({}, obj);\r\n      this.adviceSetup.advice_group_id = obj.advice_group_id;\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}