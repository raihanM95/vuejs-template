er: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().post('/api/Prescription/Save', appointmentPrescription).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    getById(id) {\r\n        return new AxiosHelper().getById('/api/Prescription/GetById?id=', id).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getPrescriptionDataByDoctorId(doctorId) {\r\n        return new AxiosHelper().getById('/api/Prescription/GetPrescriptionDataByDoctorId?doctorId=', doctorId).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getPatientWisePrescriptionList(patientId) {\r\n        return new AxiosHelper().getById('/api/Prescription/GetPatientWisePrescriptionList?id=', patientId).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    prescriptionWiseInvestigations(appointmentNoOrAdmissionNo, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getById('/api/Prescription/PrescriptionWiseInvestigations?appointmentNoOrAdmissionNo=', appointmentNoOrAdmissionNo).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    prescriptionDoctorAndPatientWise(doctor_id, patient_id, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getAll('/api/Prescription/PrescriptionDoctorAndPatientWise?doctor_id=' + doctor_id + '&patient_id=' + patient_id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}