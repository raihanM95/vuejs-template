n    }\r\n    getOTDoctorBookedList(doctor_id, ot_start_time) {\r\n        return new AxiosHelper().getAllHOS('/api/OTSchedule/GetOTDoctorBookedList?doctor_id=' + doctor_id + '&ot_start_time=' + ot_start_time).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getOTTableList(doctor_id, ot_start_time, ot_end_time) {\r\n        return new AxiosHelper().getAllHOS('/api/OTSchedule/GetOTTableList?doctor_id=' + doctor_id + '&ot_start_time=' + ot_start_time + '&ot_end_time=' + ot_end_time).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    otAssignSave(obj, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().postHOS('/api/OTSchedule/OTAssignSave', obj).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    otStatusUpdate(obj, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().postHOS('/api/OTSchedule/OTStatusUpdate', obj).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}