OADI;AAEdC,kBAAAA,OAAO,EAAE,OAFK;AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AA1FQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiGX,KAjHM;AAkHPG,IAAAA,IAlHO,gBAkHFC,GAlHE,EAkHG;AACR,WAAKnD,kBAAL,GAA0BoD,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,GAAlB,CAA1B;AAEA,UAAIG,cAAc,GAChB,KAAKzD,yBAAL,CAA+B0D,MAA/B,CACE,UAAUC,EAAV,EAAc;AACZ,eACEA,EAAE,CAAChB,yBAAH,KACAW,GAAG,CAACX,yBAFN;AAID,OANH,EAOE,IAPF,CADF;AAUA,WAAKxC,kBAAL,CAAwB6B,yBAAxB,GAAoDyB,cAAc,CAAC,CAAD,CAAlE;AACA,WAAK3D,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD,KAlIM;AAmIP+D,IAAAA,SAnIO,qBAmIGN,GAnIH,EAmIQ;AACbO,MAAAA,YAAY,CAACC,OAAb,CAAqB,kBAArB,EAAyCR,GAAG,CAAClB,gBAA7C;AACA,WAAK2B,OAAL,CAAaC,IAAb,CAAkB,uBAAlB;AACD;AAtIM;AA5DI,CAAf","sourcesContent":["import InvestigationSetupService from \"../../service/setup/InvestigationSetupService\";\r\nimport InvestigationPreinformationSetupService from \"../../service/setup/InvestigationPreinformationSetupService\";\r\nimport InvestigationCategorySetupService from \"../../service/setup/InvestigationCategorySetupService\";\r\nimport InvestigationSampleTypeSetupService from \"../../service/setup/InvestigationSampleTypeSetupService\";\r\nimport CollectionCenterSetupService from \"../../service/setup/CollectionCenterSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      investigationPreinformationList: [],\r\n      investigationCategoryList: [],\r\n      sampleTypesList: [],\r\n      collectionCenterList: [],\r\n      investigationSetup: {},\r\n      investingationList: [],\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.investigationSetupService = new InvestigationSetupService();\r\n    this.investigationPreinformationSetupService =\r\n      new InvestigationPreinformationSetupService();\r\n    this.investigationCategorySetupService =\r\n      new InvestigationCategorySetupService();\r\n    this.investigationSampleTypeSetupService =\r\n      new InvestigationSampleTypeSetupService();\r\n    this.collectionCenterSetupService = new CollectionCenterSetupService();\r\n    this.validation = new Validation();\r\n\r\n    this.investigationPreinformationSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.investigationPreinformationList = data.data));\r\n\r\n    this.investigationCategorySetupService\r\n      .getAll()\r\n      .then((data) => (this.investigationCategoryList = data.data));\r\n\r\n    this.investigationSampleTypeSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.sampleTypesList = data.data));\r\n\r\n    this.collectionCenterSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.collectionCenterList = data.data));\r\n\r\n    this.investigationSetupService\r\n      .getAll()\r\n      .then((data) => (this.investingationList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.investigationSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.investigationSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    getDiscountPercent() {\r\n      this.investigationSetup.discount_percent = this.investigationSetup.ddlinvestigation_category.discount_percent;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.investigationSetup.investingation_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.investingationList, 'investingation_name', this.investigationSetup.investingation_name, 'investigation_id', this.investigationSetup.investigation_id, this)) {\r\n          return;\r\n        } else if (!this.investigationSetup.manual_code) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.investingationList, 'manual_code', this.investigationSetup.manual_code, 'investigation_id', this.investigationSetup.investigation_id, this)) {\r\n          return;\r\n        } else if (!this.investigationSetup.charge) {\r\n          return;\r\n        } else if (!this.investigationSetup.report_delivery_duration) {\r\n          return;\r\n        } else if (!this.investigationSetup.investigation_preinformation_id) {\r\n          return;\r\n        } else if (!this.investigationSetup.ddlinvestigation_category) {\r\n          return;\r\n        } else if (!this.investigationSetup.i_sample_types_id) {\r\n          return;\r\n        } else if (!this.investigationSetup.collection_center_id) {\r\n          return;\r\n        } else {\r\n          this.investigationSetup.investigation_category_id = this.investigationSetup.ddlinvestigation_category.investigation_category_id;\r\n\r\n          if (!this.investigationSetup.investigation_id) {\r\n            let result = await this.investigationSetupService.add(\r\n              this.investigationSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Saved Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationSetupService\r\n                .getAll()\r\n                .then((data) => (this.investingationList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.investigationSetupService.put(\r\n              this.investigationSetup,\r\n              this.investigationSetup.investigation_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Updated Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationSetupService\r\n                .getAll()\r\n                .then((data) => (this.investingationList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Faild\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.investigationSetup = Object.assign({}, obj);\r\n      \r\n      var aInvestigation =\r\n        this.investigationCategoryList.filter(\r\n          function (el) {\r\n            return (\r\n              el.investigation_category_id ===\r\n              obj.investigation_category_id\r\n            );\r\n          },\r\n          this\r\n        );\r\n      this.investigationSetup.ddlinvestigation_category = aInvestigation[0];\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n    paramEdit(obj) {\r\n      localStorage.setItem('investigation_id', obj.investigation_id);\r\n      this.$router.push(\"/test-parameter-setup\");\r\n    },\r\n  },\r\n};"]}]}