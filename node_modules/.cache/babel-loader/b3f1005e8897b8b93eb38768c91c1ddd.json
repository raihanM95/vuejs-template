   investigationCategoryList: [], ///new\r\n      investigationList: [],\r\n      investigationRequest: {\r\n        cdfromdate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() - 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        reportType: \"User wise Investigation Request Summary\",\r\n        type: \"All_details\",\r\n        user_name:\"All\",\r\n        doctor_id:\"All\",\r\n        investigation_id:\"All\",\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.diagnosisReportService = new DiagnosisReportService();\r\n    this.userSetupService = new UserSetupService();\r\n    this.doctorSetupService =\r\n      new DoctorSetupService();\r\n    this.investigationCategorySetupService =\r\n      new InvestigationCategorySetupService();\r\n    this.investigationSetupService = new InvestigationSetupService();\r\n    this.commonService = new CommonService();\r\n\r\n    this.userSetupService.getAll().then((data) => (this.userFormatted(data.data)));\r\n\r\n    this.doctorSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.doctorFormatted(data.data)));\r\n\r\n    this.investigationSetupService\r\n      .getAll()\r\n      .then((data) => this.formatInvestigationList(data.data));\r\n\r\n    this.investigationCategorySetupService\r\n      .getAll()\r\n      .then((data) => (this.investigationCategoryList = data.data));\r\n  },\r\n  methods: {\r\n    userFormatted(data) {\r\n      var selectAll = {\r\n        user_name: \"All\",\r\n        full_name: \"All\",\r\n      }\r\n\r\n      data.splice(0, 0, selectAll)\r\n      this.usersList = data;\r\n      \r\n    },\r\n\r\n    doctorFormatted(data) {\r\n      var selectAll = {\r\n        text: \"All\",\r\n        value: \"All\",\r\n      }\r\n\r\n      data.splice(0, 0, selectAll)\r\n      this.doctorList = data;\r\n    },\r\n\r\n    // investigationFormatted(data) {\r\n    //   var selectAll = {\r\n    //     investingation_name: \"All\",\r\n    //     investigation_id: \"All\",\r\n    //   }\r\n\r\n    //   data.splice(0, 0, selectAll)\r\n    //   this.doctorList = data;\r\n    // },\r\n\r\n\r\n    formatInvestigationList(data) {\r\n      let filterData = data.filter(function (el) {\r\n        return el.active == true;\r\n      }, this);\r\n      filterData.forEach((element) => {\r\n        var obj = element;\r\n        obj.investingation_name = element.manual_code + \"-\" + element.investingation_name;\r\n        this.investigationList.push(obj);\r\n      });\r\n    },\r\n    filterInvestigationCategoryWise2() {\r\n      return this.investigationList;\r\n    },\r\n\r\n    filterInvestigationCategoryWise() {\r\n      if(this.investigationRequest.investigation_category_id==null || this.investigationRequest.investigation_category_id==\"undefine\"||this.investigationRequest.investigation_category_id==\"null\"){\r\n       {\r\n        \r\n        return this.investigationList;\r\n       }\r\n\r\n      }\r\n      else{\r\n        return this.investigationList.filter(function (el) {\r\n          return el.investigation_category_id === this.investigationRequest.investigation_category_id;\r\n        }, this);\r\n      }\r\n      \r\n      // var selectAll = {\r\n      //     investigation_id: \"All\",\r\n      //     investingation_name: \"All\",\r\n      //   }\r\n      //   this.investigationList.push(this.investigationList.splice(0, 0, selectAll));\r\n    },\r\n\r\n    show() {\r\n      this.submitted = true;\r\n      if (!this.investigationRequest.cdfromdate) {\r\n        return;\r\n      } else if (!this.investigationRequest.cdtodate) {\r\n        return;\r\n      } else {\r\n        this.investigationRequest.fromdate = moment(\r\n          String(this.investigationRequest.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.investigationRequest.todate = moment(\r\n          String(this.investigationRequest.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n\r\n        if (this.investigationRequest.reportType == \"User wise Investigation Request Summary\") {\r\n          if (!this.investigationRequest.user_name) {\r\n            this.allUser = true;\r\n          } else {\r\n            this.allUser = false;\r\n          }\r\n          this.diagnosisReportService.GetUserWiseInvestigationRequestSummary(this.investigationRequest.fromdate, this.investigationRequest.todate, this.allUser, this.investigationRequest.user_name, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        } else if (this.investigationRequest.reportType == \"Doctor wise Investigation Request\") {\r\n          if (!this.investigationRequest.doctor_id) {\r\n            this.investigationRequest.doctor_id = \"All\";\r\n          }\r\n          this.diagnosisReportService.GetDoctorWiseInvestigationRequestSummary(this.investigationRequest.fromdate, this.investigationRequest.todate, this.investigationRequest.doctor_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        } else if (this.investigationRequest.reportType == \"Invoice wise Investigation Request\") {\r\n          if (!this.investigationRequest.invoice_no) {\r\n            this.investigationRequest.invoice_no = \"All\";\r\n          }\r\n          this.diagnosisReportService.GetUserWiseInvestigationRequestSummary(this.investigationRequest.fromdate, this.investigationRequest.todate, this.investigationRequest.invoice_no, \"\", this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n          this.investigationRequest.invoice_no = \"\";\r\n        } else if (this.investigationRequest.reportType == \"Investigation wise Investigation Request\") {\r\n          if (!this.investigationRequest.investigation_id) {\r\n            this.investigationRequest.investigation_id = \"All\";\r\n          }\r\n          if (this.investigationRequest.type == \"Details\") {\r\n            this.diagnosisReportService.GetInvest