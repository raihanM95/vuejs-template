tmentList = data.data));\r\n\r\n    this.figureSetupService\r\n      .getAll()\r\n      .then((data) => (this.figureList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.figureSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n      this.$refs.inputImage.value = \"\";\r\n      this.imageError = \"\";\r\n    },\r\n    hideNew() {\r\n      this.figureSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n      this.$refs.inputImage.value = \"\";\r\n      this.imageError = \"\";\r\n    },\r\n    onFileChange: function (event) {\r\n      const file = event.target.files[0];\r\n      if (!file) {\r\n        return false;\r\n      }\r\n      if (!file.type.match(\"image.*\")) {\r\n        return false;\r\n      }\r\n      const reader = new FileReader();\r\n      const that = this;\r\n\r\n      // validate picture size\r\n      if (file.size/1024 > 250) {\r\n        //event.preventDefault();\r\n        //alert(\"File too big (> 250KB)\");\r\n        this.imageError = `The image size (${parseInt(file.size/1024)} KB) is too much! (max is 250KB).`;\r\n        that.figureSetup.figure_image = \"\";\r\n        return;\r\n      } else {\r\n        this.imageError = \"\";\r\n        reader.onload = function (e) {\r\n          that.figureSetup.cffigure_image = e.target.result;\r\n        };\r\n        reader.readAsDataURL(file);\r\n      }\r\n    },\r\n    // getBase64(file) {\r\n    //   return new Promise(function (resolve, reject) {\r\n    //     let reader = new FileReader();\r\n    //     let imgResult = \"\";\r\n    //     reader.readAsDataURL(file);\r\n    //     reader.onload = function () {\r\n    //       imgResult = reader.result;\r\n    //     };\r\n    //     reader.onerror = function (error) {\r\n    //       reject(error);\r\n    //     };\r\n    //     reader.onloadend = function () {\r\n    //       resolve(imgResult);\r\n    //     };\r\n    //   });\r\n    // },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.figureSetup.figure_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.figureList, 'figure_name', this.figureSetup.figure_name, 'figure_id', this.figureSetup.figure_id, this)) {\r\n          return;\r\n        } else if (!this.figureSetup.figure_group_id) {\r\n          return;\r\n        } else if (!this.figureSetup.department_id) {\r\n          return;\r\n        } else if (this.imageError) {\r\n          return;\r\n        } else {\r\n          this.figureSetup.figure_image =\r\n            this.figureSetup.cffigure_image.split(\",\")[1];\r\n          if (!this.figureSetup.figure_id) {\r\n            let result = await this.figureSetupService.add(this.figureSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.figureSetupService\r\n                .getAll()\r\n                .then((data) => (this.figureList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.figureSetupService.put(\r\n              this.figureSetup,\r\n              this.figureSetup.figure_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.figureSetupService\r\n                .getAll()\r\n                .then((data) => (this.figureList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.figureSetupService\r\n        .getById(obj.figure_id)\r\n        .then((data) => this.getFormatted(data.data));\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n    getFormatted(data) {\r\n      this.figureSetup = data;\r\n      this.figureSetup.cffigure_image =\r\n        \"data:image/jpg;base64,\" + this.figureSetup.figure_image;\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}