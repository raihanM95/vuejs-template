etupService\";//added \r\nimport DoctorSetupService from \"../../service/setup/DoctorSetupService\";\r\nimport EmployeeSetupService from \"../../service/setup/EmployeeSetupService\";\r\nimport UserSetupService from \"../../service/security/UserSetupService\";///add new\r\nimport ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport DiagnosisReportService from \"../../service/report/DiagnosisReportService\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      usersList: [],\r\n      referredByList: [],\r\n      employeeList:[],\r\n      doctorList:[],\r\n        ipdBedReport: {\r\n        cdfromdate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() - 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        reportType: \"percentageWiseBedOccupation\",\r\n        user_no:\"All\",\r\n        type:\"IPD\"\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.mISReportService = new MISReportService();\r\n    this.commonService = new CommonService();\r\n    this.userSetupService = new UserSetupService(); //add new\r\n    this.referredBySetupService = new ReferredBySetupService();//add new\r\n    this.employeeSetupService = new EmployeeSetupService();//add new\r\n    this.doctorSetupService = new DoctorSetupService(); //add new \r\n    this.consultancyDepartmentSetupService = new ConsultancyDepartmentSetupService();\r\n    this.diagnosisReportService = new DiagnosisReportService();\r\n\r\n    this.consultancyDepartmentSetupService\r\n            .getActiveCombo()\r\n            .then((data) => (this.departmentList = data.data));\r\n            \r\n        this.doctorSetupService\r\n            .getAll()\r\n            .then((data) => this.mergeDoctorInfo(data.data));\r\n  },\r\n  methods: {\r\n\r\n    filterDoctorsDepartmentWise() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.refid === this.ipdBedReport.department_id;\r\n      }, this);\r\n    },\r\n\r\n    mergeDoctorInfo(data) {\r\n      data.forEach((element) => {\r\n        var obj = element;\r\n        obj.doctor_degree = \"\";\r\n        obj.lstConsultantDegree.forEach((el) => {\r\n          obj.doctor_degree += el.consultant_degree_name + \". \";\r\n        });\r\n\r\n        obj.doctor_name = element.doc_no + \" - (\" + obj.doctor_degree + \") - \" + element.doctor_name;\r\n        this.doctorList.push(obj);\r\n      });\r\n    },\r\n  filterDoctorsDepartmentWise2() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.department_id === this.ipdBedReport.department_id;\r\n      }, this);\r\n    },\r\n\r\n\r\n\r\n    show() {\r\n      this.submitted = true;\r\n      if (!this.ipdBedReport.cdfromdate) {\r\n        return;\r\n      } else if (!this.ipdBedReport.cdtodate) {\r\n        return;\r\n      } else {\r\n        this.ipdBedReport.fromdate = moment(\r\n          String(this.ipdBedReport.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.ipdBedReport.todate = moment(\r\n          String(this.ipdBedReport.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n\r\n      if (!this.ipdBedReport.department_id) {\r\n        this.ipdBedReport.department_id = \"All\"\r\n        }\r\n        if (!this.ipdBedReport.doctor_id) {\r\n            this.ipdBedReport.doctor_id = \"All\"\r\n        }\r\n\r\n\r\n     \r\n\r\n\r\n            if(this.ipdBedReport.reportType==='percentageWiseBedOccupation')\r\n            {\r\n              //\r\n              this.mISReportService.GetPercentageWiseBedOccupation(this.ipdBedReport.fromdate, this.ipdBedReport.todate, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n            if(this.ipdBedReport.reportType==='bedTransferList')\r\n            {\r\n              this.mISReportService.GetBedTransferListData(this.ipdBedReport.fromdate, this.ipdBedReport.todate, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n\r\n\r\n           \r\n        \r\n        \r\n        this.submitted = false;\r\n      }\r\n    },\r\n  \r\n\r\n\r\n\r\n  },\r\n};"]}]}