tus from \"../../components/DashboardInvestigationStatus\";\r\nimport DashboardInvestigationResult from \"../../components/DashboardInvestigationResult\";\r\n\r\nexport default {\r\n    components: {\r\n        DashboardInvestigationStatus,\r\n        DashboardInvestigationResult\r\n    },\r\n    data() {\r\n        return {\r\n            isDoctor: false,\r\n            isAdmin: false,\r\n            todaysAppointmentCount: 0,\r\n            todaysAppointmentList: [],\r\n            visitedAppointmentCount: 0,\r\n            visitedAppointmentList: [],\r\n            upcomingAppointmentCount: 0,\r\n            upcomingAppointmentList: [],\r\n            scheduleList: [],\r\n            totalDoctor: 0,\r\n            totalPatient: 0,\r\n            totalAssistant: 0,\r\n            totalEmployee: 0,\r\n            topInvestigationList: [],\r\n            topDrConsultancyList: [],\r\n            marketingTeamReferredByList: [],\r\n            activeAppointmentCount: 0,\r\n            expiredAppointmentCount: 0,\r\n\r\n            // chart\r\n            chartData: {},\r\n            lightOptions: {},\r\n            genderData: {},\r\n            genderOptions: {},\r\n            submitted: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.appointmentPrescriptionService = new AppointmentPrescriptionService();\r\n        this.doctorAppointmentService = new DoctorAppointmentService();\r\n        this.doctorSetupService = new DoctorSetupService();\r\n        this.patientRegistrationService = new PatientRegistrationService();\r\n        this.dashboardService = new DashboardService();\r\n\r\n        var fromDate = \"2021-01-01\";\r\n        var toDate = moment(String(new Date(new Date().setDate(new Date().getDate() + 7)))).format(\"YYYY-MM-DD\");\r\n        var status = \"All\";\r\n        this.userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.doctorAppointmentService\r\n                .getDynamicDoctorWise(\r\n                    fromDate,\r\n                    toDate,\r\n                    status,\r\n                    this.userInfo.doctor_id\r\n                )\r\n                .then((data) => this.formatted(data.data));\r\n\r\n            this.getDrSchedule(this.userInfo.doctor_id);\r\n\r\n            this.isDoctor = true;\r\n        } else {\r\n            this.dashboardService.opdGetAllAdmin()\r\n                .then((data) => this.getFormattedRegistered(data.data));\r\n\r\n            this.doctorAppointmentService\r\n                .getDynamic(\r\n                    fromDate,\r\n                    toDate,\r\n                    status,\r\n                    status\r\n                )\r\n                .then((data) => this.formattedAppointment(data.data));\r\n\r\n            this.patientRegistrationService.getAll()\r\n                .then((data) => this.getFormattedRegisteredPatient(data.data));\r\n            this.isAdmin = true;\r\n        }\r\n    },\r\n    mounted() {\r\n        // pass data into component \r\n        this.$refs.dashboardInvestigationStatusComponent.getInvestigationStatus('OPD');\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.$refs.dashboardInvestigationResultComponent.getInvestigationResult(this.userInfo.doctor_id);\r\n        }\r\n    },\r\n    methods: {\r\n        prescription(data) {\r\n            localStorage.setItem(\"prescriptionData\", JSON.stringify(data));\r\n            this.$router.push(\"/appointment-prescription\");\r\n        },\r\n        formatted(data) {\r\n            this.todaysAppointment(data);\r\n            this.visitedAppointment(data);\r\n            this.upcomingAppointment(data);\r\n        },\r\n        todaysAppointment(data) {\r\n            var todaysData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Active\" && moment(String(el.appoint_date)).format(\"YYYY-MM-DD\") == moment(String(new Date())).format(\"YYYY-MM-DD\"));\r\n            }, this);\r\n            this.todaysAppointmentList = todaysData;\r\n            this.todaysAppointmentCount = todaysData.length;\r\n        },\r\n        visitedAppointment(data) {\r\n            var visitedData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Visited\");\r\n            }, this);\r\n            this.visitedAppointmentList = visitedData;\r\n            this.visitedAppointmentCount = visitedData.length;\r\n        },\r\n        upcomingAppointment(data) {\r\n            var upcomingData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Active\" && moment(String(el.appoint_date)).format(\"YYYY-MM-DD\") <= moment(String(new Date(new Date().setDate(new Date().getDate() + 7)))).format(\"YYYY-MM-DD\"));\r\n            }, this);\r\n            this.upcomingAppointmentList = upcomingData;\r\n            this.upcomingAppointmentCount = upcomingData.length;\r\n        },\r\n        getFormattedSchedule(data) {\r\n            data.forEach((element) => {\r\n                element.start_time1 = moment(\r\n                    String(new Date(\"2000-01-01 \" + element.start_time1))\r\n                ).format(\"h:mm a\");\r\n                element.end_time1 = moment(\r\n                    String(new Date(\"2000-01-01 \" + element.end_time1))\r\n                ).format(\"h:mm a\");\r\n                element.start_time2 = moment(\r\n                    String(new Date(\"2000-01-01 \" + element.start_time2))\r\n                ).format(\"h:mm a\");\r\n                element.end_time2 = moment(\r\n                    String(new Date(\"2000-01-01 \" + element.end_time2))\r\n                ).format(\"h:mm a\");\r\n            });\r\n\r\n            this.scheduleList = data;\r\n        },\r\n        getDrSchedule(doctorId) {\r\n            this.doctorSetupService\r\n                .getDoctorScheduleById(doctorId)\r\n                .then((data) => this.getFormattedSchedule(data.data));\r\n        },\r\n        \r\n        //\r\n        formatDate(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY h:mm:ss a\");\r\n        },\r\n        formatDate2(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY\");\r\n        },\r\n        getFormattedRegistered(data) {\r\n            this.totalDoctor = data.total_doctor;\r\n            this.totalPatient = data.total_patient;\r\n            this.totalAssistant = data.total_doctor_assistant;\r\n            this.totalEmployee = data.total_employee;\r\n            this.topInvestigationList = JSON.parse(data.investigation_request_count);\r\n            this.topDrConsultancyList = JSON.parse(data.doctor_appointment_request_count);\r\n            this.marketingTeamReferredByList = JSON.parse(data.marketing_team_reffered_by_count);\r\n        },\r\n        formattedAppointment(data) {\r\n            var activeData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Active\");\r\n            }, this);\r\n            this.activeAppointmentCount = activeData.length;\r\n\r\n            var visitedData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Visited\");\r\n            }, this);\r\n            this.visitedAppointmentCount = visitedData.length;\r\n\r\n            var expiredData = data.filter(function (el) {\r\n                return (\r\n                    el.appoinment_status == \"Expired\");\r\n            }, this);\r\n            this.expiredAppointmentCount = expiredData.length;\r\n\r\n            // chart\r\n            this.chartData = ref({\r\n                labels: ['Active', 'Visited', 'Expired'],\r\n                datasets: [\r\n                    {\r\n                        data: [this.activeAppointmentCount, this.visitedAppointmentCount, this.expiredAppointmentCount],\r\n                        backgroundColor: [\"#42A5F5\", \"#66BB6A\", \"#FFA726\"],\r\n                        hoverBackgroundColor: [\"#64B5F6\", \"#81C784\", \"#FFB74D\"]\r\n                    }\r\n                ]\r\n            });\r\n            this.lightOptions = ref({\r\n                plugins: {\r\n                    legend: {\r\n                        labels: {\r\n                            color: '#495057'\r\n                        }\r\n                    }\r\n                }\r\n            });\r\n        },\r\n        getFormattedRegisteredPatient(data) {\r\n            var maleData = data.filter(function (el) {\r\n                return (\r\n                    el.gender == \"Male\");\r\n            }, this);\r\n            this.maleCount = maleData.length;\r\n\r\n            var femaleData = data.filter(function (el) {\r\n                return (\r\n                    el.gender == \"Female\");\r\n            }, this);\r\n            this.femaleCount = femaleData.length;\r\n\r\n            var thirdData = data.filter(function (el) {\r\n                return (\r\n                    el.gender == \"Third\");\r\n            }, this);\r\n            this.thirdCount = thirdData.length;\r\n\r\n            // chart\r\n            this.genderData = ref({\r\n                labels: ['Male', 'Female', 'Third'],\r\n                datasets: [\r\n                    {\r\n                        data: [this.maleCount, this.femaleCount, this.thirdCount],\r\n                        backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"],\r\n                        hoverBackgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"]\r\n                    }\r\n                ]\r\n            });\r\n\r\n            this.genderOptions = ref({\r\n                plugins: {\r\n                    legend: {\r\n                        labels: {\r\n                            color: '#495057'\r\n                        }\r\n                    }\r\n                }\r\n            });\r\n        }\r\n    },\r\n};"]}]}