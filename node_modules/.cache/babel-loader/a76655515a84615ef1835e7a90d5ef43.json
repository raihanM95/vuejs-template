tchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      investigationCategoryHeadList: [],\r\n      investigationCategoryHeadSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.investigationCategoryHeadSetupService = new InvestigationCategoryHeadSetupService();\r\n    this.validation = new Validation();\r\n    this.investigationCategoryHeadSetupService.getAll().then((data) => (this.investigationCategoryHeadList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.investigationCategoryHeadSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.investigationCategoryHeadSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.investigationCategoryHeadSetup.investigation_category_head_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.investigationCategoryHeadList, 'investigation_category_head_name', this.investigationCategoryHeadSetup.investigation_category_head_name, 'investigation_category_head_id', this.investigationCategoryHeadSetup.investigation_category_head_id, this)) {\r\n          return;\r\n        } else {\r\n          if (!this.investigationCategoryHeadSetup.investigation_category_head_id) {\r\n            let result = await this.investigationCategoryHeadSetupService.add(this.investigationCategoryHeadSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationCategoryHeadSetupService\r\n                .getAll()\r\n                .then((data) => (this.investigationCategoryHeadList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.investigationCategoryHeadSetupService.put(\r\n              this.investigationCategoryHeadSetup,\r\n              this.investigationCategoryHeadSetup.investigation_category_head_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationCategoryHeadSetupService\r\n                .getAll()\r\n                .then((data) => (this.investigationCategoryHeadList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.investigationCategoryHeadSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}