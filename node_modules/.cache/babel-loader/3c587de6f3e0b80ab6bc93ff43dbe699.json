,MAAI,CAAC0B,YAAL,CAAkBuB,cAHX;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA,oBAKA,MAAI,CAACvB,YAAL,CAAkB3B,SALlB;AAAA;AAAA;AAAA;;AAAA;;AAAA;AAAA;AAAA,uBAQW,MAAI,CAAC6B,iBAAL,CAAuByE,cAAvB,CACf,MAAI,CAAC3E,YADU,EACI,MADJ,CARX;;AAAA;AAQJ+D,gBAAAA,MARI;;AAYN,oBAAIA,MAAM,CAACC,MAAP,KAAkB,GAAtB,EAA2B;AACzB,kBAAA,MAAI,CAACC,MAAL,CAAYC,GAAZ,CAAgB;AACdC,oBAAAA,QAAQ,EAAE,SADI;AAEdC,oBAAAA,OAAO,EAAE,SAFK;AAGdC,oBAAAA,MAAM,EAAE,sBAHM;AAIdC,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;;AAMA,kBAAA,MAAI,CAACxD,IAAL;;AACA,kBAAA,MAAI,CAACyD,UAAL;AACD,iBATD,MASO;AACL,kBAAA,MAAI,CAACN,MAAL,CAAYC,GAAZ,CAAgB;AACdC,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,QAFK;AAGdC,oBAAAA,MAAM,EAAEN,MAAM,CAACS,GAHD;AAIdF,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD;;AA5BK;AAAA;AAAA;;AAAA;AAAA;AAAA;;AA+BZ,gBAAA,MAAI,CAACL,MAAL,CAAYC,GAAZ,CAAgB;AACZC,kBAAAA,QAAQ,EAAE,OADE;AAEZC,kBAAAA,OAAO,EAAE,QAFG;AAGZC,kBAAAA,MAAM,cAHM;AAIZC,kBAAAA,IAAI,EAAE;AAJM,iBAAhB;;AA/BY;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsCnB,KA3MI;AA4MLC,IAAAA,UA5MK,wBA4MQ;AACT,WAAKpF,cAAL,GAAsB,KAAtB;AACA,WAAKM,MAAL,GAAc;AACVC,QAAAA,cAAc,EAAE,EADN;AAEVC,QAAAA,uBAAuB,EAAE,EAFf;AAGVC,QAAAA,aAAa,EAAE;AAHL,OAAd;AAMA,WAAKC,kBAAL,GAA0B,KAA1B;AACA,WAAKG,YAAL,GAAoB,EAApB;AAGA,WAAK1B,SAAL,GAAiB,KAAjB;AACH,KAzNI;AA0NLsG,IAAAA,YA1NK,wBA0NQrD,cA1NR,EA0NuB;AAAA;;AACxBsD,MAAAA,OAAO,CAACC,GAAR,CAAYvD,cAAZ;AACA,WAAKpB,eAAL,CAAqB4E,oBAArB,CAA0CxD,cAA1C,EAA0D,IAA1D,EAAgEZ,IAAhE,CAAqE,UAAClD,IAAD;AAAA,eAAW,MAAI,CAAC2C,aAAL,CAAmB4E,YAAnB,CAAgCvH,IAAI,CAACA,IAArC,CAAX;AAAA,OAArE;AACH;AA7NI;AArFE,CAAf","sourcesContent":["import CommonService from \"../../service/CommonService\";\r\nimport OTScheduleService from \"../../service/ot/OTScheduleService\";\r\nimport OTReportService from \"../../service/report/OTReportService\";\r\n// Assign\r\nimport ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport DoctorSetupService from \"../../service/setup/DoctorSetupService\";\r\nimport DoctorAssistantSetupService from \"../../service/setup/DoctorAssistantSetupService\";\r\nimport DepartmentSetupService from \"../../service/setup/DepartmentSetupService\";\r\nimport NurseSetupService from \"../../service/setup/NurseSetupService\";\r\n// Service & Package\r\n// import ServiceSetupService from \"../../service/setup/ServiceSetupService\";\r\n// import ServicePackageSetupService from \"../../service/setup/ServicePackageSetupService\";\r\n\r\nimport moment from \"moment\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            dataList: false,\r\n            otStatusList: [],\r\n            otScheduleList: [],\r\n            assignedList: [],\r\n            isAssigned: true,\r\n            otSchedule: {\r\n                cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n                cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n                // cdtodate: moment(\r\n                //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n                // ).format(\"MM/DD/YYYY\"),\r\n                ot_status: \"All\",\r\n            },\r\n            submitted: false,\r\n            filters: {\r\n                global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n                name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            },\r\n            initFilters() {\r\n                this.filters = {\r\n                    global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n                    name: {\r\n                        operator: FilterOperator.AND,\r\n                        constraints: [\r\n                            { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n                        ],\r\n                    },\r\n                };\r\n            },\r\n            // Assign\r\n            otAssignedInfoDialog: false,\r\n            otAssignDialog: false,\r\n            consultancyDepartmentList: [],\r\n            departmentList: [],\r\n            doctorList: [],\r\n            assistantList: [],\r\n            nurseList: [],\r\n            assign: {\r\n                otAssignDoctor: {},\r\n                otAssignDoctorAssistant: {},\r\n                otAssignNurse: {},\r\n            },\r\n            // Status update\r\n            updateStatusDialog: false,\r\n            updateStatusList: [\r\n                { text: \"Start\", value: \"Start\" },\r\n                { text: \"End\", value: \"End\" },\r\n                { text: \"Canceled\", value: \"Canceled\" },\r\n            ],\r\n            statusUpdate: {\r\n\r\n            }\r\n        };\r\n    },\r\n    created() {\r\n        this.otScheduleService = new OTScheduleService();\r\n        this.otReportService = new OTReportService();\r\n        this.commonService = new CommonService();\r\n        // Assign\r\n        this.doctorSetupService = new DoctorSetupService();\r\n        this.consultancyDepartmentSetupService = new ConsultancyDepartmentSetupService();\r\n        this.doctorAssistantSetupService = new DoctorAssistantSetupService();\r\n        this.departmentSetupService = new DepartmentSetupService();\r\n        this.nurseSetupService = new NurseSetupService();\r\n        // Service & Package\r\n        // this.serviceSetupService = new ServiceSetupService();\r\n        // this.servicePackageSetupService = new ServicePackageSetupService();\r\n\r\n        this.consultancyDepartmentSetupService\r\n            .getActiveCombo()\r\n            .then((data) => (this.consultancyDepartmentList = data.data));\r\n\r\n        this.departmentSetupService\r\n            .getAll()\r\n            .then((data) => (this.departmentList = data.data));\r\n\r\n        this.commonService\r\n            .GetAllOTStatus()\r\n            .then((data) => (this.otStatusList = data.data));\r\n\r\n        this.show();\r\n    },\r\n    methods: {\r\n        reset() {\r\n            this.otSchedule = {\r\n                cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n                cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n                // cdtodate: moment(\r\n                //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n                // ).format(\"MM/DD/YYYY\"),\r\n                ot_status: \"All\",\r\n            };\r\n            this.dataList = false;\r\n            this.submitted = false;\r\n            this.show();\r\n        },\r\n        show() {\r\n            this.submitted = true;\r\n            if (!this.otSchedule.cdfromdate) {\r\n                return;\r\n            } else if (!this.otSchedule.cdtodate) {\r\n                return;\r\n            } else if (!this.otSchedule.ot_status) {\r\n                return;\r\n            } else {\r\n                this.otSchedule.fromdate = moment(\r\n                    String(this.otSchedule.cdfromdate)\r\n                ).format(\"YYYY-MM-DD\");\r\n                this.otSchedule.todate = moment(\r\n                    String(this.otSchedule.cdtodate)\r\n                ).format(\"YYYY-MM-DD\");\r\n                this.otScheduleService\r\n                    .getDynamic(\r\n                        this.otSchedule.fromdate,\r\n                        this.otSchedule.todate,\r\n                        this.otSchedule.ot_status\r\n                    )\r\n                    .then((data) => (this.otScheduleList = data.data));\r\n            }\r\n            this.dataList = true;\r\n            this.submitted = false;\r\n        },\r\n        formatDate(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY\");\r\n        },\r\n        formatDateTime(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY h:mm:ss a\");\r\n        },\r\n\r\n        // reSchedule(data) {\r\n        //   localStorage.setItem('ot_schedule_no_for_reschedule', data);\r\n        //   this.$router.push(\"/ot-form\");\r\n        // },\r\n\r\n        showOTAssignedInfo(data) {\r\n            this.assign.ot_schedule_id = data.ot_schedule_id;\r\n            var ot_start_time = moment(\r\n                String(data.ot_start_time)\r\n            ).format(\"YYYY-MM-DD HH:mm\");\r\n            var ot_end_time = moment(\r\n                String(data.ot_end_time)\r\n            ).format(\"YYYY-MM-DD HH:mm\");\r\n            this.doctorSetupService.getOTAssignEligibleDoctors(ot_start_time, ot_end_time).then((data) => this.mergeDoctorInfo(data.data));\r\n            this.nurseSetupService.getOTAssignEligibleNurses(ot_start_time, ot_end_time).then((data) => this.mergeNurseInfo(data.data));\r\n            this.doctorAssistantSetupService.getOTAssignEligibleDoctorAssistants(ot_start_time, ot_end_time).then((data) => this.mergeAssistantInfo(data.data));\r\n\r\n            if (data.ot_status == \"Created\" || data.ot_status == \"Start\") {\r\n                this.isAssigned = true;\r\n            } else {\r\n                this.isAssigned = false;\r\n            }\r\n\r\n            this.otScheduleService.getOTAssignedList(data.ot_schedule_id).then((data) => this.formattedAssignedList(data.data));\r\n        },\r\n        mergeDoctorInfo(data) {\r\n            this.doctorList = [];\r\n            data.forEach((element) => {\r\n                var obj = element;\r\n                obj.doctor_degree = \"\";\r\n                obj.lstConsultantDegree.forEach((el) => {\r\n                    obj.doctor_degree += el.consultant_degree_name + \". \";\r\n                });\r\n\r\n                obj.doctor_name = element.doc_no + \" - (\" + obj.doctor_degree + \") - \" + element.doctor_name;\r\n                this.doctorList.push(obj);\r\n            });\r\n        },\r\n        filterDoctorsDepartmentWise() {\r\n            return this.doctorList.filter(function (el) {\r\n                return el.department_id === this.assign.otAssignDoctor.department_id;\r\n            }, this);\r\n        },\r\n        mergeNurseInfo(data) {\r\n            this.nurseList = [];\r\n            data.forEach((element) => {\r\n                var obj = element;\r\n                obj.nurse_name = element.nurse_no + \" - (\" + obj._designation_name + \") - \" + element.first_name + \" \" + element.last_name;\r\n                this.nurseList.push(obj);\r\n            });\r\n        },\r\n        filterNursesDepartmentWise() {\r\n            return this.nurseList.filter(function (el) {\r\n                return el.department_id === this.assign.otAssignNurse.ndepartment_id;\r\n            }, this);\r\n        },\r\n        mergeAssistantInfo(data) {\r\n            this.assistantList = [];\r\n            data.forEach((element) => {\r\n                var obj = element;\r\n                obj.doctor_assistant_name = element.doctor_assistant_name + \" \" + element.contact_no;\r\n                this.assistantList.push(obj);\r\n            });\r\n        },\r\n        formattedAssignedList(data) {\r\n            this.assignedList = data;\r\n            this.otAssignedInfoDialog = true;\r\n        },\r\n        // Assign\r\n        otAssignInfo() {\r\n            this.otAssignedInfoDialog = false;\r\n            this.otAssignDialog = true;\r\n        },\r\n        async addAssign() {\r\n            this.submitted = true;\r\n            try {\r\n                if (this.assign.type == \"Doctor\" && !this.assign.otAssignDoctor.department_id && !this.assign.otAssignDoctor.doctor_id) {\r\n                    return;\r\n                } else if (this.assign.type == \"Nurse\" && !this.assign.otAssignNurse.ndepartment_id && !this.assign.otAssignNurse.nurse_id) {\r\n                    return;\r\n                } else if (this.assign.type == \"Assistant\" && !this.assign.otAssignDoctorAssistant.doctor_assistant_id) {\r\n                    return;\r\n                } else {\r\n                    let result = await this.otScheduleService.otAssignSave(\r\n                        this.assign, this\r\n                      );\r\n            \r\n                      if (result.status === 201) {\r\n                        this.$toast.add({\r\n                          severity: \"success\",\r\n                          summary: \"Success\",\r\n                          detail: \"Assigned Successfully\",\r\n                          life: 10000,\r\n                        });\r\n                        this.hideDialog();\r\n                      } else {\r\n                        this.$toast.add({\r\n                          severity: \"error\",\r\n                          summary: \"Failed\",\r\n                          detail: result.res,\r\n                          life: 10000,\r\n                        });\r\n                      }\r\n                }\r\n            } catch (error) {\r\n                this.$toast.add({\r\n                    severity: \"error\",\r\n                    summary: \"Failed\",\r\n                    detail: error,\r\n                    life: 10000,\r\n                });\r\n            }\r\n        },\r\n        updateStatusDialogShow(data){\r\n            this.statusUpdate = {};\r\n            this.statusUpdate.ot_schedule_id = data.ot_schedule_id;\r\n            this.updateStatusDialog = true;\r\n        },\r\n        async updateStatus(){\r\n            this.submitted = true;\r\n            try {\r\n                if (!this.statusUpdate.ot_schedule_id) {\r\n                    return;\r\n                } else if (!this.statusUpdate.ot_status) {\r\n                    return;\r\n                } else {\r\n                    let result = await this.otScheduleService.otStatusUpdate(\r\n                        this.statusUpdate, this\r\n                      );\r\n            \r\n                      if (result.status === 201) {\r\n                        this.$toast.add({\r\n                          severity: \"success\",\r\n                          summary: \"Success\",\r\n                          detail: \"Updated Successfully\",\r\n                          life: 10000,\r\n                        });\r\n                        this.show();\r\n                        this.hideDialog();\r\n                      } else {\r\n                        this.$toast.add({\r\n                          severity: \"error\",\r\n                          summary: \"Failed\",\r\n                          detail: result.res,\r\n                          life: 10000,\r\n                        });\r\n                      }\r\n                }\r\n            } catch (error) {\r\n                this.$toast.add({\r\n                    severity: \"error\",\r\n                    summary: \"Failed\",\r\n                    detail: error,\r\n                    life: 10000,\r\n                });\r\n            }\r\n        },\r\n        hideDialog() {\r\n            this.otAssignDialog = false;\r\n            this.assign = {\r\n                otAssignDoctor: {},\r\n                otAssignDoctorAssistant: {},\r\n                otAssignNurse: {},\r\n            };\r\n\r\n            this.updateStatusDialog = false;\r\n            this.statusUpdate = {\r\n\r\n            }\r\n            this.submitted = false;\r\n        },\r\n        detailsPrint(ot_schedule_id){\r\n            console.log(ot_schedule_id);\r\n            this.otReportService.GetOTScheduleDetails(ot_schedule_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        }\r\n    }\r\n};"]}]}