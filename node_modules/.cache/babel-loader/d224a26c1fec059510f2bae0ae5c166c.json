,CAAmBiC,YAAnB,CAAgCpD,IAAI,CAACA,IAArC,CAAX;AAAA,WAAlH;AACD;;AAED,YAAG,KAAKK,cAAL,CAAoBQ,UAApB,KAAiC,eAApC,EACA;AACE;AACA,eAAKK,gBAAL,CAAsBqC,mBAAtB,CAA0C,KAAKlD,cAAL,CAAoB2C,QAA9D,EAAwE,KAAK3C,cAAL,CAAoB4C,MAA5F,EAAoG,KAAK5C,cAAL,CAAoB6C,SAAxH,EAAmI,aAAnI,EAAkJ,KAAK7C,cAAL,CAAoB+B,aAAtK,EAAqL,IAArL,EAA2LT,IAA3L,CAAgM,UAAC3B,IAAD;AAAA,mBAAW,MAAI,CAACmB,aAAL,CAAmBiC,YAAnB,CAAgCpD,IAAI,CAACA,IAArC,CAAX;AAAA,WAAhM;AACD;;AAGD,YAAG,KAAKK,cAAL,CAAoBQ,UAApB,KAAiC,qBAApC,EACA;AACE;AACA,eAAKY,sBAAL,CAA4B+B,0BAA5B,CAAuD,KAAKnD,cAAL,CAAoB2C,QAA3E,EAAqF,KAAK3C,cAAL,CAAoB4C,MAAzG,EAAiH,WAAjH,EAA8H,WAA9H,EAA2I,KAA3I,EAAkJ,IAAlJ,EAAwJtB,IAAxJ,CAA6J,UAAC3B,IAAD;AAAA,mBAAW,MAAI,CAACmB,aAAL,CAAmBiC,YAAnB,CAAgCpD,IAAI,CAACA,IAArC,CAAX;AAAA,WAA7J;AACD,SA9CA,CA+CD;;;AAGJ,aAAKgB,SAAL,GAAiB,KAAjB;AACD;AACF;AAvFM;AAvCI,CAAf","sourcesContent":["import MISReportService from \"../../service/report/MISReportService\";\r\nimport CommonService from \"../../service/CommonService\";\r\nimport moment from \"moment\";\r\nimport ReferredBySetupService from \"../../service/setup/ReferredBySetupService\";\r\nimport DoctorSetupService from \"../../service/setup/DoctorSetupService\";\r\nimport EmployeeSetupService from \"../../service/setup/EmployeeSetupService\";\r\nimport UserSetupService from \"../../service/security/UserSetupService\";\r\nimport ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport DiagnosisReportService from \"../../service/report/DiagnosisReportService\";\r\n\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      usersList: [],\r\n      referredByList: [],\r\n      employeeList:[],\r\n      doctorList:[],\r\n        allIncomeField: {\r\n        cdfromdate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() - 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        reportType: \"DocWiseIncome\",\r\n        user_no:\"All\",\r\n        type:\"IPD\"\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.mISReportService = new MISReportService();\r\n    this.commonService = new CommonService();\r\n    this.userSetupService = new UserSetupService(); \r\n    this.referredBySetupService = new ReferredBySetupService();\r\n    this.employeeSetupService = new EmployeeSetupService();\r\n    this.doctorSetupService = new DoctorSetupService();  \r\n    this.consultancyDepartmentSetupService = new ConsultancyDepartmentSetupService();\r\n    this.diagnosisReportService = new DiagnosisReportService();\r\n\r\n    this.consultancyDepartmentSetupService\r\n            .getActiveCombo()\r\n            .then((data) => (this.departmentList = data.data));\r\n        // this.doctorSetupService\r\n        //     .getActiveCombo()\r\n        //     .then((data) => (this.doctorList = data.data));\r\n        this.doctorSetupService\r\n            .getAll()\r\n            .then((data) => this.mergeDoctorInfo(data.data));\r\n  },\r\n  methods: {\r\n\r\n    filterDoctorsDepartmentWise() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.refid === this.allIncomeField.department_id;\r\n      }, this);\r\n    },\r\n\r\n    mergeDoctorInfo(data) {\r\n      data.forEach((element) => {\r\n        var obj = element;\r\n        obj.doctor_degree = \"\";\r\n        obj.lstConsultantDegree.forEach((el) => {\r\n          obj.doctor_degree += el.consultant_degree_name + \". \";\r\n        });\r\n\r\n        obj.doctor_name = element.doc_no + \" - (\" + obj.doctor_degree + \") - \" + element.doctor_name;\r\n        this.doctorList.push(obj);\r\n      });\r\n    },\r\n  filterDoctorsDepartmentWise2() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.department_id === this.allIncomeField.department_id;\r\n      }, this);\r\n    },\r\n\r\n\r\n\r\n    show() {\r\n      debugger;\r\n      this.submitted = true;\r\n      if (!this.allIncomeField.cdfromdate) {\r\n        return;\r\n      } else if (!this.allIncomeField.cdtodate) {\r\n        return;\r\n      } else {\r\n        this.allIncomeField.fromdate = moment(\r\n          String(this.allIncomeField.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.allIncomeField.todate = moment(\r\n          String(this.allIncomeField.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n\r\n      if (!this.allIncomeField.department_id) {\r\n        this.allIncomeField.department_id = \"All\"\r\n        }\r\n        if (!this.allIncomeField.doctor_id) {\r\n            this.allIncomeField.doctor_id = \"All\"\r\n        }\r\n\r\n\r\n        // if(this.user_no===\"null\")\r\n        // {\r\n        //   this.user_no=\"All\"\r\n        // }\r\n            if(this.allIncomeField.reportType==='serWiseIncome')\r\n            {\r\n              this.mISReportService.GetServiceWiseIncome(this.allIncomeField.fromdate, this.allIncomeField.todate, this.allIncomeField.type,  this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n            if(this.allIncomeField.reportType==='medReqInc')\r\n            {\r\n              //\r\n              this.mISReportService.GetMedicineReqWiseIncome(this.allIncomeField.fromdate, this.allIncomeField.todate,  this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n            if(this.allIncomeField.reportType==='BedCabinIncome')\r\n            {\r\n              //\r\n              this.mISReportService.GetBedCabinWiseIncome(this.allIncomeField.fromdate, this.allIncomeField.todate,  this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n\r\n            if(this.allIncomeField.reportType==='DocWiseIncome')\r\n            {\r\n              //\r\n              this.mISReportService.GetDoctorWiseIncome(this.allIncomeField.fromdate, this.allIncomeField.todate, this.allIncomeField.doctor_id, \"Appointment\", this.allIncomeField.department_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n\r\n\r\n            if(this.allIncomeField.reportType==='investigationIncome')\r\n            {\r\n              //\r\n              this.diagnosisReportService.GetInvestigationPaidAmount(this.allIncomeField.fromdate, this.allIncomeField.todate, \"undefined\", \"undefined\", \"OPD\", this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n            }\r\n            // this.mISReportService.GetDoctorLedgerAllDetails(this.allIncomeField.fromdate, this.allIncomeField.todate, this.allIncomeField.user_no,  this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        \r\n        \r\n        this.submitted = false;\r\n      }\r\n    },\r\n  \r\n\r\n\r\n\r\n  },\r\n};"]}]}