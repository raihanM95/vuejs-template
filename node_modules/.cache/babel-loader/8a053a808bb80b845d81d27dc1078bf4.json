oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;;AAMA,kBAAA,MAAI,CAAC1B,OAAL;;AACA,kBAAA,MAAI,CAACd,qBAAL,CACGU,MADH,GAEGD,IAFH,CAEQ,UAAClC,IAAD;AAAA,2BAAW,MAAI,CAACU,aAAL,GAAqBV,IAAI,CAACA,IAArC;AAAA,mBAFR;AAGD,iBAXD,MAWO,IAAI2D,OAAM,CAACC,MAAP,KAAkB,GAAtB,EAA2B;AAChC,kBAAA,MAAI,CAACC,MAAL,CAAYR,GAAZ,CAAgB;AACdS,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,OAFK;AAGdC,oBAAAA,MAAM,EAAEL,OAAM,CAAC3D,IAHD;AAIdiE,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD,iBAPM,MAOA;AACL,kBAAA,MAAI,CAACJ,MAAL,CAAYR,GAAZ,CAAgB;AACdS,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,QAFK;AAGdC,oBAAAA,MAAM,EAAEL,OAAM,CAACO,GAHD;AAIdD,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD;;AA5EG;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAgFR,gBAAA,MAAI,CAACJ,MAAL,CAAYR,GAAZ,CAAgB;AACdS,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,QAFK;AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAhFQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAuFX,KAvHM;AAwHPG,IAAAA,IAxHO,gBAwHFC,GAxHE,EAwHG;AACR,WAAK1D,cAAL,GAAsB2D,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,GAAlB,CAAtB;AACA,WAAKnE,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD;AA5HM;AAxEI,CAAf","sourcesContent":["import TreatmentSetupService from \"../../service/setup/TreatmentSetupService\";\r\nimport ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport DoctorSetupService from \"../../service/setup/DoctorSetupService\";\r\nimport MedicineTypeSetupService from \"../../service/setup/MedicineTypeSetupService\";\r\nimport MedicineSetupService from \"../../service/setup/MedicineSetupService\";\r\nimport TreatmentRulesSetupService from \"../../service/setup/TreatmentRulesSetupService\";\r\nimport TreatmentDurationSetupService from \"../../service/setup/TreatmentDurationSetupService\";\r\nimport TreatmentDoseSetupService from \"../../service/setup/TreatmentDoseSetupService\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      departmentList: [],\r\n      doctorList: [],\r\n      medicineTypeList: [],\r\n      medicineList: [],\r\n      treatmentRulesList: [],\r\n      treatmentDurationList: [],\r\n      treatmentDoseList: [],\r\n      treatmentList: [],\r\n      treatmentSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.treatmentSetupService = new TreatmentSetupService();\r\n    this.consultancyDepartmentSetupService = new ConsultancyDepartmentSetupService();\r\n    this.doctorSetupService = new DoctorSetupService();\r\n    this.medicineTypeSetupService = new MedicineTypeSetupService();\r\n    this.medicineSetupService = new MedicineSetupService();\r\n    this.treatmentRulesSetupService = new TreatmentRulesSetupService();\r\n    this.treatmentDurationSetupService = new TreatmentDurationSetupService();\r\n    this.treatmentDoseSetupService = new TreatmentDoseSetupService();\r\n\r\n    this.consultancyDepartmentSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.departmentList = data.data));\r\n\r\n    this.doctorSetupService\r\n      .getAll()\r\n      .then((data) => this.mergeDoctorInfo(data.data));\r\n\r\n    this.medicineTypeSetupService.getActiveCombo().then((data) => (this.medicineTypeList = data.data));\r\n\r\n    this.medicineSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.medicineList = data.data));\r\n\r\n    this.treatmentRulesSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.treatmentRulesList = data.data));\r\n\r\n    this.treatmentDurationSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.treatmentDurationList = data.data));\r\n\r\n    this.treatmentDoseSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.treatmentDoseList = data.data));\r\n\r\n    this.treatmentSetupService\r\n      .getAll()\r\n      .then((data) => (this.treatmentList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.treatmentSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.treatmentSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    mergeDoctorInfo(data) {\r\n      data.forEach((element) => {\r\n        this.doctorList.push({\r\n          department_id: element.department_id,\r\n          doctor_id: element.doctor_id,\r\n          doctor_name: element.doc_no + \"-\" + element.doctor_name,\r\n        });\r\n      });\r\n    },\r\n    filterDoctorsDepartmentWise() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.department_id === this.treatmentSetup.department_id;\r\n      }, this);\r\n    },\r\n    filterMedicineTypeWise(){\r\n      return this.medicineList.filter(function (el) {\r\n        return el.refid === this.treatmentSetup.medicine_type_id;\r\n      }, this);\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.treatmentSetup.department_id) {\r\n          return;\r\n        } else if (!this.treatmentSetup.doctor_id) {\r\n          return;\r\n        } else if (!this.treatmentSetup.medicine_id) {\r\n          return;\r\n        } else if (!this.treatmentSetup.treatment_rules_id) {\r\n          return;\r\n        } else if (!this.treatmentSetup.treatment_duration_id) {\r\n          return;\r\n        } else if (!this.treatmentSetup.treatment_dose_id) {\r\n          return;\r\n        } else {\r\n          if (!this.treatmentSetup.treatment_id) {\r\n            let result = await this.treatmentSetupService.add(\r\n              this.treatmentSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.treatmentSetupService\r\n                .getAll()\r\n                .then((data) => (this.treatmentList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.treatmentSetupService.put(\r\n              this.treatmentSetup,\r\n              this.treatmentSetup.treatment_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.treatmentSetupService\r\n                .getAll()\r\n                .then((data) => (this.treatmentList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.treatmentSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}