LC,UAAAA,MAAM,EAAE;AACJL,YAAAA,MAAM,EAAE;AACJM,cAAAA,KAAK,EAAE;AADH;AADJ;AADH;AADY,OAAD,CAAxB;AASH;AAtDI;AAzDE,CAAf","sourcesContent":["import CommonService from \"../../service/CommonService\";\r\nimport DashboardService from \"../../service/components/DashboardService\";\r\nimport moment from \"moment\";\r\nimport { ref } from 'vue';\r\nimport DashboardInvestigationStatus from \"../../components/DashboardInvestigationStatus\";\r\nimport DashboardInvestigationResult from \"../../components/DashboardInvestigationResult\";\r\nimport PatientPaymentService from \"../../service/cash-counter/PatientPaymentService\";\r\n\r\nexport default {\r\n    components: {\r\n        DashboardInvestigationStatus,\r\n        DashboardInvestigationResult\r\n    },\r\n    data() {\r\n        return {\r\n            isDoctor: false,\r\n            isAdmin: false,\r\n            totalDoctor: 0,\r\n            totalOnTreatmentPatient: 0,\r\n            totalReleasedPatient: 0,\r\n            totalNurse: 0,\r\n            onTreatmentList: [],\r\n            topTreatmentList: [],\r\n\r\n            // chart\r\n            chartData: {},\r\n            lightOptions: {},\r\n            genderData: {},\r\n            genderOptions: {},\r\n            submitted: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.commonService = new CommonService();\r\n        this.dashboardService = new DashboardService();\r\n        this.patientPaymentService = new PatientPaymentService();\r\n\r\n        //var fromDate = \"2021-01-01\";\r\n        //var toDate = moment(String(new Date(new Date().setDate(new Date().getDate() + 7)))).format(\"YYYY-MM-DD\");\r\n        //var status = \"All\";\r\n        this.userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n\r\n        this.patientPaymentService\r\n            .getemergencyPablePatient()\r\n            .then((data) => this.onTreatmentPatientListFormatted(data.data));\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.dashboardService.emergencyGetAllAdmin()\r\n                .then((data) => this.getFormattedEmergencyDashboardByDoctor(data.data));\r\n\r\n            this.isDoctor = true;\r\n        } else {\r\n            this.dashboardService.emergencyGetAllAdmin()\r\n                .then((data) => this.getFormattedEmergencyDashboard(data.data));\r\n\r\n            this.isAdmin = true;\r\n        }\r\n    },\r\n    mounted() {\r\n        // pass data into component \r\n        this.$refs.dashboardInvestigationStatusComponent.getInvestigationStatus('OPD');\r\n        //this.userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.$refs.dashboardInvestigationResultComponent.getInvestigationResult(this.userInfo.doctor_id);\r\n        }\r\n    },\r\n    methods: {\r\n        onTreatmentPatientListFormatted(data) {\r\n            this.onTreatmentList = data;\r\n        },\r\n        getFormattedEmergencyDashboard(data) {\r\n            var emergencyDashboardInfo = JSON.parse(data.emergencyDashboardInfo)\r\n            this.totalDoctor = emergencyDashboardInfo[0].doctors;\r\n            this.totalOnTreatmentPatient = emergencyDashboardInfo[0].ontreatment;\r\n            this.totalReleasedPatient = emergencyDashboardInfo[0].released;\r\n            this.totalNurse = emergencyDashboardInfo[0].nurses;\r\n            this.maleCount = emergencyDashboardInfo[0].ontreatmentmale;\r\n            this.femaleCount = emergencyDashboardInfo[0].ontreatmentfemale;\r\n            this.thirdCount = emergencyDashboardInfo[0].ontreatmentthird;\r\n\r\n            this.topTreatmentList = JSON.parse(data.doctorWiseReleasedPatientCount);\r\n\r\n            this.genderWiseOnTreatmentPatient();\r\n        },\r\n        getFormattedEmergencyDashboardByDoctor(data) {\r\n            var emergencyDashboardInfo = JSON.parse(data.emergencyDashboardInfo)\r\n            this.totalOnTreatmentPatient = emergencyDashboardInfo[0].ontreatment;\r\n            this.totalReleasedPatient = emergencyDashboardInfo[0].released;\r\n        },\r\n\r\n        //\r\n        formatDate(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY h:mm:ss a\");\r\n        },\r\n        formatDate2(value) {\r\n            return moment(String(value)).format(\"MM/DD/YYYY\");\r\n        },\r\n\r\n        genderWiseOnTreatmentPatient() {\r\n            // chart\r\n            this.genderData = ref({\r\n                labels: ['Male', 'Female', 'Third'],\r\n                datasets: [\r\n                    {\r\n                        data: [this.maleCount, this.femaleCount, this.thirdCount],\r\n                        backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"],\r\n                        hoverBackgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"]\r\n                    }\r\n                ]\r\n            });\r\n\r\n            this.genderOptions = ref({\r\n                plugins: {\r\n                    legend: {\r\n                        labels: {\r\n                            color: '#495057'\r\n                        }\r\n                    }\r\n                }\r\n            });\r\n        }\r\n    },\r\n};"]}]}