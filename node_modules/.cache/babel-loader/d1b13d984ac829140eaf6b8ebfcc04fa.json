AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAxEQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA+EX,KApMM;AAqMPG,IAAAA,IArMO,gBAqMFlC,GArME,EAqMG;AAAA;;AACR,WAAKhC,qBAAL,GAA6BmE,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBpC,GAAlB,CAA7B;AACA,WAAKjB,4BAAL,CACGsD,OADH,CACWrC,GAAG,CAACK,oBADf,EAEGjB,IAFH,CAGI,UAAC/B,IAAD;AAAA,eACG,MAAI,CAACW,qBAAL,CAA2BC,aAA3B,GAA2CZ,IAAI,CAACA,IAAL,CAAUY,aADxD;AAAA,OAHJ;AAOA,WAAKN,QAAL,GAAgB,KAAhB;AACA,WAAKH,OAAL,GAAe,IAAf;AACA,WAAKC,OAAL,GAAe,IAAf;AACA,WAAKC,MAAL,GAAc,IAAd;AACA,WAAKH,eAAL,GAAuB,KAAvB;AACD;AAnNM;AAnDI,CAAf","sourcesContent":["import InvestigationBOMSetupService from \"../../service/setup/InvestigationBOMSetupService\";\r\nimport InvestigationSetupService from \"../../service/setup/InvestigationSetupService\";\r\nimport ProductSetupService from \"../../service/setup/ProductSetupService\";\r\nimport UOMSetupService from \"../../service/setup/UOMSetupService\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      isExist: \"\",\r\n      isInvestigation: true,\r\n      newInfo: false,\r\n      addList: false,\r\n      button: false,\r\n      dataList: true,\r\n      investigationList: [],\r\n      productList: [],\r\n      uomList: [],\r\n      invBOMList: [],\r\n      investigationBOMSetup: { lstbomdetails: [] },\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.investigationBOMSetupService = new InvestigationBOMSetupService();\r\n    this.investigationSetupService = new InvestigationSetupService();\r\n    this.productSetupService = new ProductSetupService();\r\n    this.uomSetupService = new UOMSetupService();\r\n    this.investigationSetupService\r\n      .getAll()\r\n      .then((data) => this.formatInvestigationList(data.data));\r\n    this.productSetupService\r\n      .getAll()\r\n      .then((data) => (this.productList = data.data));\r\n    this.uomSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.uomList = data.data));\r\n\r\n    this.investigationBOMSetupService\r\n      .getAll()\r\n      .then((data) => (this.invBOMList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.investigationBOMSetup = { lstbomdetails: [] };\r\n      this.isInvestigation = true;\r\n      this.isExist = \"\";\r\n      this.newInfo = true;\r\n      this.addList = false;\r\n      this.button = false;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.investigationBOMSetup = { lstbomdetails: [] };\r\n      this.isInvestigation = true;\r\n      this.isExist = \"\";\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.addList = false;\r\n      this.button = false;\r\n      this.submitted = false;\r\n    },\r\n    formatInvestigationList(data) {\r\n      let filterData = data.filter(function (el) {\r\n        return el.active == true;\r\n      }, this);\r\n      filterData.forEach((element) => {\r\n        var obj = element;\r\n        obj.investingation_name = element.manual_code + \"-\" + element.investingation_name;\r\n        this.investigationList.push(obj);\r\n      });\r\n    },\r\n    investigationIsExist() {\r\n      if (!this.investigationBOMSetup.investigation_bom_id) {\r\n        var aInvestigation = this.invBOMList.filter(function (el) {\r\n          return (\r\n            el.investigation_id === this.investigationBOMSetup.investigation_id\r\n          );\r\n        }, this);\r\n\r\n        if (aInvestigation.length) {\r\n          this.isExist = \"This Investigation already created\";\r\n          this.isInvestigation = true;\r\n          return;\r\n        } else {\r\n          this.isExist = \"\";\r\n          this.isInvestigation = false;\r\n        }\r\n      }\r\n    },\r\n    filterProduct() {\r\n      return this.productList.filter(function (el) {\r\n        return el.active == true;\r\n      }, this);\r\n    },\r\n    filterProductInfo() {\r\n      var product = this.productList.filter(function (el) {\r\n        return (\r\n          el.product_id === this.investigationBOMSetup.ddlproduct.product_id\r\n        );\r\n      }, this);\r\n      this.investigationBOMSetup.ddluom = this.uomList.filter(function (el) {\r\n        return el.value === product[0].uom_id;\r\n      }, this)[0];\r\n    },\r\n    addProduct() {\r\n      this.submitted = true;\r\n      var aProduct = this.investigationBOMSetup.lstbomdetails.filter(function (\r\n        el\r\n      ) {\r\n        return (\r\n          el.product_id === this.investigationBOMSetup.ddlproduct.product_id\r\n        );\r\n      },\r\n        this);\r\n\r\n      if (aProduct.length) {\r\n        this.isExist =\r\n          \"This Product already added! If add new Qty please delete and add this Product again\";\r\n      } else {\r\n        if (!this.investigationBOMSetup.ddlproduct) {\r\n          return;\r\n        } else if (!this.investigationBOMSetup.quantity) {\r\n          return;\r\n        } else if (!this.investigationBOMSetup.price) {\r\n          return;\r\n        } else {\r\n          this.isExist = \"\";\r\n\r\n          this.investigationBOMSetup.lstbomdetails.push({\r\n            product_id: this.investigationBOMSetup.ddlproduct.product_id,\r\n            _product_name: this.investigationBOMSetup.ddlproduct.product_name,\r\n            uom_id: this.investigationBOMSetup.ddlproduct.uom_id,\r\n            _uom_name: this.investigationBOMSetup.ddlproduct._uom_name,\r\n            quantity: this.investigationBOMSetup.quantity,\r\n            price: this.investigationBOMSetup.price,\r\n          });\r\n        }\r\n      }\r\n\r\n      this.addList = true;\r\n      this.button = true;\r\n    },\r\n    deleteFromList(data) {\r\n      this.investigationBOMSetup.lstbomdetails =\r\n        this.investigationBOMSetup.lstbomdetails.filter(function (el) {\r\n          return el.product_id != data.product_id;\r\n        }, this);\r\n\r\n      // this.investigationBOMSetup.lstbomdetails.splice(data, data.product_id);\r\n      if (\r\n        !this.investigationBOMSetup.lstbomdetails.length &&\r\n        !this.investigationBOMSetup.investigation_bom_id\r\n      ) {\r\n        this.addList = false;\r\n        this.button = false;\r\n      }\r\n    },\r\n    async add() {\r\n      try {\r\n        if (!this.investigationBOMSetup.investigation_id) {\r\n          return;\r\n        } else if (!this.investigationBOMSetup.lstbomdetails.length) {\r\n          this.isExist = \"Please add a Product\";\r\n          return;\r\n        } else {\r\n          if (!this.investigationBOMSetup.investigation_bom_id) {\r\n            let result = await this.investigationBOMSetupService.add(\r\n              this.investigationBOMSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationBOMSetupService\r\n                .getAll()\r\n                .then((data) => (this.invBOMList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.investigationBOMSetupService.put(\r\n              this.investigationBOMSetup,\r\n              this.investigationBOMSetup.investigation_bom_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.investigationBOMSetupService\r\n                .getAll()\r\n                .then((data) => (this.invBOMList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.investigationBOMSetup = Object.assign({}, obj);\r\n      this.investigationBOMSetupService\r\n        .getById(obj.investigation_bom_id)\r\n        .then(\r\n          (data) =>\r\n            (this.investigationBOMSetup.lstbomdetails = data.data.lstbomdetails)\r\n        );\r\n\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n      this.addList = true;\r\n      this.button = true;\r\n      this.isInvestigation = false;\r\n    },\r\n  },\r\n};"]}]}