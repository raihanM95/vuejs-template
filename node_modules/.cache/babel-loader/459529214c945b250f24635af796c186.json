BuC,MAAnB,GAA4BY,GAAG,CAACZ,MAAhC;AACA,WAAKvC,aAAL,CAAmBwC,eAAnB,GAAqCW,GAAG,CAACX,eAAzC;AACA,WAAK7C,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD;AAvHM;AAjDI,CAAf","sourcesContent":["import MedicineSetupService from \"../../service/setup/MedicineSetupService\";\r\nimport MedicineGenericSetupService from \"../../service/setup/MedicineGenericSetupService\";\r\nimport MedicineTypeSetupService from \"../../service/setup/MedicineTypeSetupService\";\r\nimport UOMSetupService from \"../../service/setup/UOMSetupService\";\r\nimport ManufacturerSetupService from \"../../service/setup/ManufacturerSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      medicineList: [],\r\n      page: 100,\r\n      newInfo: false,\r\n      dataList: true,\r\n      medicineGenericList: [],\r\n      medicineTypeList: [],\r\n      uomList: [],\r\n      manufacturerList: [],\r\n      medicineSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.medicineSetupService = new MedicineSetupService();\r\n    this.medicineGenericSetupService = new MedicineGenericSetupService();\r\n    this.medicineTypeSetupService = new MedicineTypeSetupService();\r\n    this.uomSetupService = new UOMSetupService();\r\n    this.manufacturerSetupService = new ManufacturerSetupService();\r\n    this.validation = new Validation();\r\n\r\n    this.medicineGenericSetupService.getActiveCombo().then((data) => (this.medicineGenericList = data.data));\r\n    this.medicineTypeSetupService.getActiveCombo().then((data) => (this.medicineTypeList = data.data));\r\n    this.uomSetupService.getActiveCombo().then((data) => (this.uomList = data.data));\r\n    this.manufacturerSetupService.getActiveCombo().then((data) => (this.manufacturerList = data.data));\r\n\r\n    // server side pagination\r\n    var filter = this.filters['global'].value ? this.filters['global'].value : \"\";\r\n    this.medicineSetupService\r\n      .getMedicinesWithPagination(\"0\", \"10\", filter, this)\r\n      .then((data) => this.medicineFormatted(data.data));\r\n  },\r\n  methods: {\r\n    medicineFormatted(data) {\r\n      this.medicineList = data;\r\n      this.page = data.length ? data[0]._total_records : 100;\r\n    },\r\n    // server side pagination\r\n    onPage(event) {\r\n      var filter = this.filters['global'].value ? this.filters['global'].value : \"\";\r\n      var page = event == 0 ? 0 : event.page;\r\n      var rows = event == 0 ? 10 : event.rows;\r\n\r\n      this.medicineSetupService\r\n        .getMedicinesWithPagination(page, rows, filter, this)\r\n        .then((data) => this.medicineFormatted(data.data));\r\n    },\r\n    openNew() {\r\n      this.medicineSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.medicineSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.medicineSetup.medicine_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.medicineList, 'medicine_name', this.medicineSetup.medicine_name, 'medicine_id', this.medicineSetup.medicine_id, this)) {\r\n          return;\r\n        } else if (!this.medicineSetup.medicine_generic_id) {\r\n          return;\r\n        } else if (!this.medicineSetup.medicine_type_id) {\r\n          return;\r\n        } else if (!this.medicineSetup.uom_id) {\r\n          return;\r\n        } else if (!this.medicineSetup.manufacturer_id) {\r\n          return;\r\n        } else {\r\n          if (!this.medicineSetup.medicine_id) {\r\n            let result = await this.medicineSetupService.add(\r\n              this.medicineSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.onPage(0);\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.medicineSetupService.put(\r\n              this.medicineSetup,\r\n              this.medicineSetup.medicine_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.onPage(0);\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.medicineSetup = Object.assign({}, obj);\r\n      this.medicineSetup.medicine_generic_id = obj.medicine_generic_id;\r\n      this.medicineSetup.medicine_type_id = obj.medicine_type_id;\r\n      this.medicineSetup.uom_id = obj.uom_id;\r\n      this.medicineSetup.manufacturer_id = obj.manufacturer_id;\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}