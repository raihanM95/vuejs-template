             :yearNavigator=\"true\"\r\n              yearRange=\"1800:2030\"\r\n              required=\"true\"\r\n              :class=\"{\r\n                'p-invalid': submitted && !bloodIssued.cdfromdate,\r\n              }\"\r\n              v-on:keyup.enter=\"this.$refs.todate.$el.focus()\"\r\n              @blur=\"show()\"\r\n            />\r\n            <label for=\"fromdate\"\r\n              >From Date<span style=\"color: red\">*</span></label\r\n            >\r\n          </div>\r\n          <small\r\n            class=\"p-invalid\"\r\n            v-if=\"submitted && !bloodIssued.cdfromdate\"\r\n            >From Date is required.</small\r\n          >\r\n        </div>\r\n        <div class=\"p-field p-col-12 p-md-5\">\r\n          <div class=\"p-float-label\">\r\n            <Calendar\r\n              id=\"todate\"\r\n              v-model=\"bloodIssued.cdtodate\"\r\n              :monthNavigator=\"true\"\r\n              :yearNavigator=\"true\"\r\n              yearRange=\"1800:2030\"\r\n              required=\"true\"\r\n              :class=\"{\r\n                'p-invalid': submitted && !bloodIssued.cdtodate,\r\n              }\"\r\n              ref=\"todate\"\r\n              v-on:keyup.enter=\"this.$refs.blood_status.$el.focus()\"\r\n              @blur=\"show()\"\r\n            />\r\n            <label for=\"todate\">To Date<span style=\"color: red\">*</span></label>\r\n          </div>\r\n          <small class=\"p-invalid\" v-if=\"submitted && !bloodIssued.cdtodate\"\r\n            >To Date is required.</small\r\n          >\r\n        </div>\r\n        \r\n        <!-- <div class=\"p-field p-col-12 p-md-2\"></div> -->\r\n        <div class=\"p-field p-col-12 p-md-2\">\r\n          <Button\r\n            label=\"Show\"\r\n            @click=\"show()\"\r\n            class=\"p-button-lg\"\r\n            style=\"background-color: #3f51b5; border: none\"\r\n          />\r\n        </div>\r\n        <!-- <div class=\"p-field p-col-12 p-md-2\">\r\n          <Button\r\n            label=\"Reset\"\r\n            @click=\"reset\"\r\n            class=\"p-button-lg\"\r\n            style=\"background-color: #fb465e; border: none\"\r\n          />\r\n        </div> -->\r\n      </div>\r\n    </div>\r\n    <div class=\"p-grid crud-demo\" v-show=\"dataList\">\r\n      <div class=\"p-col-12\">\r\n        <div class=\"card\">\r\n          <DataTable\r\n            ref=\"dt\"\r\n            :value=\"bloodIssuedList\"\r\n            dataKey=\"blood_receive_id\"\r\n            :paginator=\"true\"\r\n            :rows=\"10\"\r\n            v-model:filters=\"filters\"\r\n            paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n            :rowsPerPageOptions=\"[10, 15, 20]\"\r\n            currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n            responsiveLayout=\"scroll\"\r\n          >\r\n            <template #header>\r\n              <div\r\n                class=\"\r\n                  table-header\r\n                  p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n                \"\r\n              >\r\n                <h5 class=\"p-m-0\">Blood Issued List</h5>\r\n                <span class=\"p-input-icon-left\">\r\n                  <i class=\"pi pi-search\" />\r\n                  <InputText\r\n                    v-model=\"filters['global'].value\"\r\n                    placeholder=\"Search...\"\r\n                  />\r\n                </span>\r\n              </div>\r\n            </template>\r\n  \r\n            <Column field=\"blood_group\" header=\"Blood Group\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Blood Group</span>\r\n                {{ slotProps.data.blood_group }}\r\n              </template>\r\n            </Column>\r\n            <Column\r\n              field=\"bag_number\"\r\n              header=\"Bag Number\"\r\n              :sortable=\"true\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Bag Number</span>\r\n                {{ slotProps.data.bag_number }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"component\" header=\"Component\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Component</span>\r\n                {{ slotProps.data.component }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"donor_no\" header=\"Donor No\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Donor No</span>\r\n                {{ slotProps.data.donor_no }}\r\n              </template>\r\n            </Column>\r\n            <Column\r\n              field=\"first_name\"\r\n              header=\"Donor Name\"\r\n              :sortable=\"true\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Donor Name</span>\r\n                {{ slotProps.data.first_name }}\r\n                {{ slotProps.data.last_name }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"contact_no\" header=\"Donor Contact\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Donor Contact</span>\r\n                {{ slotProps.data.contact_no }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"gender\" header=\"Donor Gender\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Donor Gender</span>\r\n                {{ slotProps.data.gender }}\r\n              </template>\r\n            </Column>\r\n            <Column\r\n              field=\"issue_date\"\r\n              header=\"Issue Date\"\r\n              :sortable=\"true\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Issue Date</span>\r\n                {{ formatDateTime(slotProps.data.issue_date) }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"admission_emergency_no\" header=\"IPD/Emergency No\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">IPD/Emergency No</span>\r\n                {{ slotProps.data.admission_emergency_no }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"registration_no\" header=\"Patient Reg No\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Reg No</span>\r\n                {{ slotProps.data.registration_no }}\r\n              </template>\r\n            </Column>\r\n            <Column\r\n              field=\"p_first_name\"\r\n              header=\"Patient Name\"\r\n              :sortable=\"true\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Name</span>\r\n                {{ slotProps.data.p_first_name }}\r\n                {{ slotProps.data.p_last_name }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"p_contact_no\" header=\"Patient Contact\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Contact</span>\r\n                {{ slotProps.data.p_contact_no }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"p_gender\" header=\"Patient Gender\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Patient Gender</span>\r\n                {{ slotProps.data.p_gender }}\r\n              </template>\r\n            </Column>\r\n            <Column field=\"blood_request_no\" header=\"Req No\" :sortable=\"true\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Req No</span>\r\n                {{ slotProps.data.blood_request_no }}\r\n              </template>\r\n            </Column>\r\n\r\n\r\n            <!-- <Column header=\"Appt Token\">\r\n              <template #body=\"slotProps\">\r\n                <span class=\"p-column-title\">Appt Token</span>\r\n                <Button\r\n                  icon=\"pi pi-print\"\r\n                  class=\"p-button-rounded p-button-success p-mr-2\"\r\n                  @click=\"apptTokenPrint(slotProps.data.appoinment_id)\"\r\n                />\r\n              </template>\r\n            </Column> -->\r\n            \r\n            <!-- <Column\r\n              headerStyle=\"width: 123px; text-align: right\"\r\n              bodyStyle=\"width: 123px; text-align: right\"\r\n              footerStyle=\"width: 123px; text-align: right\"\r\n            >\r\n              <template #body=\"slotProps\">\r\n                <Button\r\n                  icon=\"pi pi-pencil\"\r\n                  v-show=\"slotProps.data.blood_status != 'Visited'\"\r\n                  class=\"p-button-rounded p-button-success p-mr-2\"\r\n                  @click=\"edit(slotProps.data)\"\r\n                />\r\n              </template>\r\n            </Column> -->\r\n          </DataTable>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import app from  \"../../scripts/blood-bank/BloodIssueList\";\r\n  export default app;\r\n  </script>\r\n  \r\n  <style scoped lang=\"scss\">\r\n  .product-badge {\r\n    border-radius: 2px;\r\n    padding: 0.25em 0.5rem;\r\n    text-transform: uppercase;\r\n    font-weight: 700;\r\n    font-size: 12px;\r\n    letter-spacing: 0.3px;\r\n  \r\n    &.status-success {\r\n      background: #c8e6c9;\r\n      color: #256029;\r\n    }\r\n  \r\n    &.status-unsuccessful {\r\n      background: #ffcdd2;\r\n      color: #c63737;\r\n    }\r\n  \r\n    &.status-warning {\r\n      background: #feedaf;\r\n      color: #8a5340;\r\n    }\r\n  }\r\n  </style>"],"sourceRoot":""}]}