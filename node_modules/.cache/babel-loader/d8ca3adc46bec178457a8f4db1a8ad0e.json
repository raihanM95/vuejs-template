emplate v-slot:left>\r\n            <Button\r\n              label=\"New\"\r\n              icon=\"pi pi-plus\"\r\n              class=\"p-button-success p-mr-2\"\r\n              @click=\"openNew\"\r\n            />\r\n          </template>\r\n        </Toolbar> -->\r\n\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"usersList\"\r\n          dataKey=\"user_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Manage Users</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n          <Column field=\"full_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data.full_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"email\" header=\"Email\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Email</span>\r\n              {{ slotProps.data.email }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"user_name\" header=\"Username\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Username</span>\r\n              {{ slotProps.data.user_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"user_type\" header=\"User Type\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">User Type</span>\r\n              {{ slotProps.data.user_type }}\r\n            </template>\r\n          </Column>\r\n          <!-- <Column field=\"_role_name\" header=\"Role\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Role</span>\r\n              {{ slotProps.data._role_name }}\r\n            </template>\r\n          </Column> -->\r\n\r\n          <Column field=\"is_active\" header=\"Status\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Status</span>\r\n              <span\r\n                v-show=\"slotProps.data.is_active\"\r\n                class=\"product-badge status-active\"\r\n                >Active</span\r\n              >\r\n              <span\r\n                v-show=\"!slotProps.data.is_active\"\r\n                class=\"product-badge status-inactive\"\r\n                >Inactive</span\r\n              >\r\n            </template>\r\n          </Column>\r\n\r\n          <Column>\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"editUser(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n\r\n        <Dialog\r\n          v-model:visible=\"userDialog\"\r\n          :style=\"{ width: '450px' }\"\r\n          header=\"User Info\"\r\n          :modal=\"true\"\r\n          class=\"p-fluid\"\r\n        >\r\n          <div class=\"p-field\">\r\n            <label for=\"full_name\">Full Name</label><label style=\"color: red\">*</label>\r\n            <InputText\r\n              id=\"full_name\"\r\n              v-model.trim=\"userSetup.full_name\"\r\n              required=\"true\"\r\n              autofocus\r\n              :class=\"{ 'p-invalid': submitted && !userSetup.full_name }\"\r\n              v-on:keyup.enter=\"this.$refs.user_type.$el.focus()\"\r\n            />\r\n            <small class=\"p-invalid\" v-if=\"submitted && !userSetup.full_name\"\r\n              >Name is required.</small\r\n            >\r\n          </div>\r\n          <div class=\"p-field\">\r\n            <label for=\"user_type\" class=\"p-mb-3\">User Type</label><label style=\"color: red\">*</label>\r\n            <Dropdown\r\n              id=\"user_type\"\r\n              v-model=\"userSetup.user_type\"\r\n              :options=\"userTypeList\"\r\n              optionLabel=\"text\"\r\n              optionValue=\"value\"\r\n              placeholder=\"Select User Type\"\r\n              required=\"true\"\r\n              autofocus\r\n              :class=\"{ 'p-invalid': submitted && !userSetup.user_type }\"\r\n              ref=\"user_type\"\r\n              v-on:keyup.enter=\"this.$refs.role.$el.focus()\"\r\n              :disabled=\"userSetup.user_id\"\r\n            />\r\n            <small class=\"p-invalid\" v-if=\"submitted && !userSetup.user_type\"\r\n              >User Type is required.</small\r\n            >\r\n          </div>\r\n\r\n          <div class=\"p-field\">\r\n            <label for=\"role\" class=\"p-mb-3\">Role</label><label style=\"color: red\">*</label>\r\n            <Dropdown\r\n              id=\"role\"\r\n              v-model=\"userSetup.role_id\"\r\n              :options=\"roleList\"\r\n              optionLabel=\"role_name\"\r\n              optionValue=\"role_id\"\r\n              placeholder=\"Select a Role\"\r\n              required=\"true\"\r\n              autofocus\r\n              :class=\"{ 'p-invalid': submitted && !userSetup.role_id }\"\r\n              ref=\"role\"\r\n              v-on:keyup.enter=\"this.$refs.email.$el.focus()\"\r\n            />\r\n            <small class=\"p-invalid\" v-if=\"submitted && !userSetup.role_id\"\r\n              >Role is required.</small\r\n            >\r\n          </div>\r\n          <div class=\"p-field\">\r\n            <label for=\"email\">Email</label><label style=\"color: red\">*</label>\r\n            <InputText\r\n              id=\"email\"\r\n              v-model.trim=\"userSetup.email\"\r\n              required=\"true\"\r\n              :class=\"{ 'p-invalid': submitted && !userSetup.email }\"\r\n              ref=\"email\"\r\n              v-on:keyup.enter=\"this.$refs.password.$el.focus()\"\r\n              :disabled=\"userSetup.user_id\"\r\n            />\r\n            <small class=\"p-invalid\" v-if=\"submitted && !userSetup.email\"\r\n              >Email is required.</small\r\n            >\r\n          </div>\r\n          <div class=\"p-field\">\r\n            <label for=\"password\">Password</label><label style=\"color: red\">*</label>\r\n            <Password\r\n              id=\"password\"\r\n              v-model.trim=\"userSetup.password\"\r\n              required=\"true\"\r\n              :class=\"{ 'p-invalid': submitted && !userSetup.password }\"\r\n              toggleMask\r\n              ref=\"password\"\r\n            >\r\n              <template #header>\r\n                <h6>Pick a password</h6>\r\n              </template>\r\n              <template #footer=\"sp\">\r\n                {{ sp.level }}\r\n                <Divider />\r\n                <p class=\"p-mt-2\">Suggestions</p>\r\n                <ul class=\"p-pl-2 p-ml-2 p-mt-0\" style=\"line-height: 1.5\">\r\n                  <li>At least one lowercase</li>\r\n                  <li>At least one uppercase</li>\r\n                  <li>At least one numeric</li>\r\n                  <li>Minimum 8 characters</li>\r\n                </ul>\r\n              </template>\r\n            </Password>\r\n            <!-- <InputText id=\"password\" v-model.trim=\"userSetup.password\" required=\"true\" :class=\"{'p-invalid': submitted && !userSetup.password}\" ref=\"password\" /> -->\r\n            <small class=\"p-invalid\" v-if=\"submitted && !userSetup.password\"\r\n              >Password is required.</small\r\n            >\r\n          </div>\r\n          <div class=\"p-field\">\r\n            <div class=\"p-field-checkbox\">\r\n              <!-- <Checkbox id=\"is_active\" name=\"is_active\" value=\"true\" v-model=\"userSetup.is_active\" /> -->\r\n              <input\r\n                type=\"checkbox\"\r\n                id=\"is_active\"\r\n                name=\"is_active\"\r\n                v-model=\"userSetup.is_active\"\r\n              />\r\n              <label for=\"is_active\">Active</label>\r\n            </div>\r\n          </div>\r\n\r\n          <template #footer>\r\n            <Button\r\n              label=\"Cancel\"\r\n              icon=\"pi pi-times\"\r\n              class=\"p-button-text\"\r\n              @click=\"hideDialog\"\r\n            />\r\n            <Button\r\n              label=\"Save\"\r\n              icon=\"pi pi-check\"\r\n              class=\"p-button-text\"\r\n              @click=\"addUser\"\r\n            />\r\n          </template>\r\n        </Dialog>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from  \"../../scripts/security/UserSetup\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-active {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-inactive {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n}\r\n</style>"],"sourceRoot":""}]}