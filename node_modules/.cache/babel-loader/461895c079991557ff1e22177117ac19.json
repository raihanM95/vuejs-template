romdate, todate, doctor_id) {\r\n        return new AxiosHelper().getAllHOS('/api/Admission/AdmittedPatientListForTreatmentByDoctorId?fromdate=' + fromdate + '&todate=' + todate + '&doctor_id=' + doctor_id).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    admittedPatientListForHistoryByNurseId(fromdate, todate, nurse_id) {\r\n        return new AxiosHelper().getAllHOS('/api/Admission/AdmittedPatientListForHistoryByNurseId?fromdate=' + fromdate + '&todate=' + todate + '&nurse_id=' + nurse_id).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    dischargedPatientListForHistoryByNurseId(fromdate, todate, nurse_id) {\r\n        return new AxiosHelper().getAllHOS('/api/Admission/DischargedPatientListForHistoryByNurseId?fromdate=' + fromdate + '&todate=' + todate + '&nurse_id=' + nurse_id).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getDynamic(fromdate, todate, admission_status, payment_status) {\r\n        return new AxiosHelper().getAllHOS('/api/Admission/GetDynamic?fromdate=' + fromdate + '&todate=' + todate + '&admission_status=' + admission_status + '&payment_status=' + payment_status).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    add(admission, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().postHOS('/api/Admission/Save', admission).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}