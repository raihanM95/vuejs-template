AOD,GAAP;AACH,OAHM,CAAP;AAIH;;;;;;SA5EgBf,0B","sourcesContent":["import AxiosHelper from '../axiosHelper';\r\n\r\nexport default class PatientRegistrationService {\r\n    add(patientRegistration, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().post('/api/Patient/Save', patientRegistration).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    put(patientRegistration, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().put('/api/Patient/Update', patientRegistration).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    getAll() {\r\n        return new AxiosHelper().getAll('/api/Patient/GetAll').then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getActiveCombo() {\r\n        return new AxiosHelper().getAll('/api/Patient/GetActiveCombo').then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getById(id) {\r\n        return new AxiosHelper().getById('/api/Patient/getById?id=', id).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n    getByContactOrRegNo(id, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getById('/api/Patient/GetByContactOrRegNo?id=', id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    // server side pagination\r\n    getPatientWithPagination(pageNo, recordPerPage, searchItem, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getAll('/api/Patient/GetPatientWithPagination?pageNo=' + pageNo + '&recordPerPage=' + recordPerPage + '&searchItem=' + searchItem).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}