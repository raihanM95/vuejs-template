r\nimport CommonService from \"../../service/CommonService\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      userDialog: false,\r\n      userSetup: {},\r\n      submitted: false,\r\n      userTypeList: [],\r\n      roleList: [],\r\n      usersList: [],\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters1() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.userSetupService = new UserSetupService();\r\n    this.roleSetupService = new RoleSetupService();\r\n    this.commonService = new CommonService();\r\n\r\n    this.commonService\r\n      .GetAllUserType()\r\n      .then((data) => (this.userTypeList = data.data));\r\n\r\n    this.roleSetupService.getAll().then((data) => (this.roleList = data.data));\r\n\r\n    this.userSetupService.getAll().then((data) => (this.usersList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.userSetup = {};\r\n      this.submitted = false;\r\n      this.userDialog = true;\r\n    },\r\n    hideDialog() {\r\n      this.userDialog = false;\r\n      this.submitted = false;\r\n    },\r\n    async addUser() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.userSetup.full_name) {\r\n          return;\r\n        } else if (!this.userSetup.user_type) {\r\n          return;\r\n        } else if (!this.userSetup.role_id) {\r\n          return;\r\n        } else if (!this.userSetup.email) {\r\n          return;\r\n        } else if (!this.userSetup.password) {\r\n          return;\r\n        } else {\r\n          if (!this.userSetup.user_id) {\r\n            let result = await this.userSetupService.add(this.userSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Successfully Created\",\r\n                life: 10000,\r\n              });\r\n              this.userDialog = false;\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: \"Email already Used\",\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.userSetupService.put(this.userSetup);\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.userDialog = false;\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Faild\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    editUser(user) {\r\n      this.userSetup = user;\r\n      this.userSetup.role_id = user.role_id;\r\n\r\n      this.userDialog = true;\r\n    },\r\n  },\r\n};"]}]}