\n        });\r\n    }\r\n\r\n    getIPDPableByadmission_no(admission_no) {\r\n        return new AxiosHelper().getById('/api/PatientPayment/getIPDPableByadmission_no?admission_no=', admission_no).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n\r\n    getemergencyPablePatient() {\r\n        return new AxiosHelper().getAll('/api/PatientPayment/getemergencyPablePatient').then(res => {\r\n            return res;\r\n        });\r\n    }\r\n\r\n    getemergencyPableBypatientOrInvoice(patientOrInvoiceId) {\r\n        return new AxiosHelper().getById('/api/PatientPayment/getemergencyPableBypatientOrInvoice?patientOrInvoiceId=', patientOrInvoiceId).then(res => {\r\n            return res;\r\n        });\r\n    }\r\n\r\n    addEmergency(patientPayment, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().post('/api/PatientPayment/SaveEmergency', patientPayment).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}