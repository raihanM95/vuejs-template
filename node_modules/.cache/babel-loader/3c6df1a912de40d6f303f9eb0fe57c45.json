ce from \"../../service/CommonService\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      isEmailValid: \"\",\r\n      isPhoneValid: \"\",\r\n      isAssistantExist: \"\",\r\n      newInfo: false,\r\n      dataList: true,\r\n      employeeList: [],\r\n      assistantList: [],\r\n      doctorAssistantSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.doctorAssistantSetupService = new DoctorAssistantSetupService();\r\n    this.employeeSetupService = new EmployeeSetupService();\r\n    this.designationSetupService = new DesignationSetupService();\r\n    this.validation = new Validation();\r\n    this.setupReportService = new SetupReportService();\r\nthis.commonService = new CommonService();\r\n\r\n\r\n\r\n    this.employeeSetupService\r\n      .getAllDoctorAssistant()\r\n      .then((data) => this.mergeEmployeeInfo(data.data));\r\n\r\n    this.designationSetupService\r\n      .getAll()\r\n      .then((data) => (this.designationList = data.data));\r\n\r\n    this.doctorAssistantSetupService\r\n      .getAll()\r\n      .then((data) => (this.assistantList = data.data));\r\n  },\r\n  methods: {\r\n    printAll() {\r\n      this.setupReportService.GetHospitalDoctorAssistantList(this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n    },\r\n    openNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.doctorAssistantSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.doctorAssistantSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    reset() {\r\n      this.doctorAssistantSetup = {};\r\n    },\r\n    mergeEmployeeInfo(data) {\r\n      let filterData = data.filter(function (el) {\r\n        return el.active == true;\r\n      }, this);\r\n      filterData.forEach((element) => {\r\n        var obj = element;\r\n        obj.first_name = element.emp_no + \"-\" + element.first_name;\r\n        this.employeeList.push(obj);\r\n      });\r\n    },\r\n    // filterEmployeesDesignationWise() {\r\n    //   return this.employeeList.filter(function (el) {\r\n    //     return (\r\n    //       el.designation_id === this.doctorAssistantSetup.employeeDesignation_id\r\n    //     );\r\n    //   }, this);\r\n    // },\r\n    filterEmployee() {\r\n      var aEmployee = this.assistantList.filter(function (el) {\r\n        return el.employee_id === this.doctorAssistantSetup.employee_id;\r\n      }, this);\r\n\r\n      if (aEmployee.length) {\r\n        this.isAssistantExist = \"This Employee already as a Assistant\";\r\n      } else {\r\n        this.isAssistantExist = \"\";\r\n        \r\n        var employee = this.employeeList.filter(function (el) {\r\n          return el.employee_id === this.doctorAssistantSetup.employee_id;\r\n        }, this);\r\n        this.doctorAssistantSetup.employee_id = employee[0].employee_id;\r\n        this.doctorAssistantSetup.doctor_assistant_name =\r\n          employee[0].first_name + \" \" + employee[0].last_name;\r\n        this.doctorAssistantSetup.designation_id = employee[0].designation_id;\r\n        this.doctorAssistantSetup.contact_no = employee[0].contact_no;\r\n        this.doctorAssistantSetup.email = employee[0].email;\r\n      }\r\n    },\r\n    isPhoneValidAndExists(){\r\n      this.validation.isPhoneValidAndExists(this.assistantList, 'contact_no', this.doctorAssistantSetup.contact_no, 'employee_id', this.doctorAssistantSetup.employee_id, this)\r\n    },\r\n    isEmailValidAndExists(){\r\n      this.validation.isEmailValidAndExists(this.assistantList, 'email', this.doctorAssistantSetup.email, 'employee_id', this.doctorAssistantSetup.employee_id, this)\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (\r\n          !this.doctorAssistantSetup.designation_id &&\r\n          this.doctorAssistantSetup.as_assistant\r\n        ) {\r\n          return;\r\n        } else if (\r\n          !this.doctorAssistantSetup.employee_id &&\r\n          this.doctorAssistantSetup.as_assistant\r\n        ) {\r\n          return;\r\n        } else if (!this.doctorAssistantSetup.doctor_assistant_name) {\r\n          return;\r\n        } else if (!this.doctorAssistantSetup.designation_id) {\r\n          return;\r\n        } else if (!this.doctorAssistantSetup.contact_no) {\r\n          return;\r\n        } else if (this.isPhoneValid || this.isEmailValid) {\r\n          return;\r\n        } else {\r\n          if (!this.doctorAssistantSetup.doctor_assistant_id) {\r\n            let result = await this.doctorAssistantSetupService.add(\r\n              this.doctorAssistantSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.doctorAssistantSetupService\r\n                .getAll()\r\n                .then((data) => (this.assistantList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Err