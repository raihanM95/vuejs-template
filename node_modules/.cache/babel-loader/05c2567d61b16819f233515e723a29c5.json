ort default {\r\n  data() {\r\n    return {\r\n      doctorList: [],\r\n      doctorCommission: {\r\n        cdfromdate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() - 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        reportType: \"Summary\"\r\n      }\r\n    };\r\n  },\r\n  created() {\r\n    this.diagnosisReportService = new DiagnosisReportService();\r\n    this.doctorSetupService =\r\n      new DoctorSetupService();\r\n    this.commonService = new CommonService();\r\n\r\n    this.doctorSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.doctorList = data.data));\r\n  },\r\n  methods: {\r\n    show() {\r\n      this.submitted = true;\r\n      if (!this.doctorCommission.cdfromdate) {\r\n        return;\r\n      } else if (!this.doctorCommission.cdtodate) {\r\n        return;\r\n      } else {\r\n        this.doctorCommission.fromdate = moment(\r\n          String(this.doctorCommission.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.doctorCommission.todate = moment(\r\n          String(this.doctorCommission.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n\r\n        if (!this.doctorCommission.doctor_id) {\r\n          this.doctorCommission.doctor_id = \"All\"\r\n        }\r\n        if(this.doctorCommission.reportType == \"Summary\"){\r\n          this.diagnosisReportService.GetDoctorCommissionSummary(this.doctorCommission.fromdate, this.doctorCommission.todate, this.doctorCommission.doctor_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        } else if(this.doctorCommission.reportType == \"Details\"){\r\n          this.diagnosisReportService.GetDoctorCommissionDetails(this.doctorCommission.fromdate, this.doctorCommission.todate, this.doctorCommission.doctor_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n        }\r\n        \r\n        this.submitted = false;\r\n      }\r\n    }\r\n  },\r\n};"]}]}