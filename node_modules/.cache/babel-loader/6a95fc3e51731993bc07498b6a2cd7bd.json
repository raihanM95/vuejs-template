E;AAJQ,mBAAhB;AAMD;;AAlMG;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAsMR,gBAAA,MAAI,CAACJ,MAAL,CAAYF,GAAZ,CAAgB;AACdG,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,OAFK;AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAtMQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA6MX,KA5OM;AA6OPoB,IAAAA,IA7OO,gBA6OFC,GA7OE,EA6OG;AACR,WAAK1D,aAAL,GAAqB2D,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,GAAlB,CAArB;AACA,WAAK1D,aAAL,CAAmByC,UAAnB,GAAgCiB,GAAG,CAACjB,UAApC;AACA,WAAKzC,aAAL,CAAmB0C,SAAnB,GAA+BgB,GAAG,CAAChB,SAAnC;AACA,WAAK1C,aAAL,CAAmB2C,aAAnB,GAAmCe,GAAG,CAACf,aAAvC;AACA,WAAK3C,aAAL,CAAmB4C,cAAnB,GAAoCc,GAAG,CAACd,cAAxC;AACA,WAAK5C,aAAL,CAAmB6C,SAAnB,GAA+Ba,GAAG,CAACb,SAAnC;AACA,WAAK7C,aAAL,CAAmB6D,OAAnB,GAA6BH,GAAG,CAACG,OAAjC;AACA,WAAK7D,aAAL,CAAmBuC,YAAnB,GAAkC5D,MAAM,CACtCuE,MAAM,CAAC,KAAKlD,aAAL,CAAmBiD,UAApB,CADgC,CAAN,CAEhCE,MAFgC,CAEzB,YAFyB,CAAlC;AAGA,WAAKnD,aAAL,CAAmB+C,WAAnB,GAAiCpE,MAAM,CACrCuE,MAAM,CAAC,KAAKlD,aAAL,CAAmBoD,SAApB,CAD+B,CAAN,CAE/BD,MAF+B,CAExB,YAFwB,CAAjC;AAIA,WAAKjE,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD,KA9PM;AA+PP6E,IAAAA,UA/PO,sBA+PIxE,KA/PJ,EA+PW;AAChB,aAAOX,MAAM,CAACuE,MAAM,CAAC5D,KAAD,CAAP,CAAN,CAAsB6D,MAAtB,CAA6B,YAA7B,CAAP;AACD;AAjQM;AAjFI,CAAf","sourcesContent":["import EmployeeSetupService from \"../../service/setup/EmployeeSetupService\";\r\nimport CommonService from \"../../service/CommonService\";\r\nimport CompanySetupService from \"../../service/setup/CompanySetupService\";\r\nimport BranchSetupService from \"../../service/setup/BranchSetupService\";\r\nimport DepartmentSetupService from \"../../service/setup/DepartmentSetupService\";\r\nimport DesignationSetupService from \"../../service/setup/DesignationSetupService\";\r\nimport JobTypeSetupService from \"../../service/setup/JobTypeSetupService\";\r\nimport RoleSetupService from \"../../service/security/RoleSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport moment from \"moment\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      isEmailValid: \"\",\r\n      isPhoneValid: \"\",\r\n      newInfo: false,\r\n      dataList: true,\r\n      active: 0,\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      employeeList: [],\r\n      employeeSetup: {\r\n        active: false,\r\n        _createUser: false,\r\n      },\r\n      submitted: false,\r\n      genderList: [],\r\n      companyList: [],\r\n      branchList: [],\r\n      departmentList: [],\r\n      designationList: [],\r\n      jobtypeList: [],\r\n      roleList: [],\r\n    };\r\n  },\r\n\r\n  created() {\r\n    this.employeeSetupService = new EmployeeSetupService();\r\n    this.commonService = new CommonService();\r\n    this.companySetupService = new CompanySetupService();\r\n    this.branchSetupService = new BranchSetupService();\r\n    this.departmentSetupService = new DepartmentSetupService();\r\n    this.designationSetupService = new DesignationSetupService();\r\n    this.jobTypeSetupService = new JobTypeSetupService();\r\n    this.roleSetupService = new RoleSetupService();\r\n    this.validation = new Validation();\r\n    this.employeeSetupService\r\n      .getAll()\r\n      .then((data) => (this.employeeList = data.data));\r\n\r\n    this.commonService.GetAllGender().then(\r\n      (data) =>\r\n        //  console.log(data.data)\r\n        (this.genderList = data.data)\r\n    );\r\n\r\n    this.companySetupService\r\n      .getAll()\r\n      .then((data) => (this.companyList = data.data));\r\n\r\n    this.branchSetupService\r\n      .getAll()\r\n      .then((data) => (this.branchList = data.data));\r\n\r\n    this.departmentSetupService\r\n      .getAll()\r\n      .then((data) => (this.departmentList = data.data));\r\n\r\n    this.designationSetupService\r\n      .getAll()\r\n      .then((data) => (this.designationList = data.data));\r\n\r\n    this.jobTypeSetupService\r\n      .getAll()\r\n      .then((data) => (this.jobtypeList = data.data));\r\n\r\n    this.roleSetupService.getAll().then((data) => (this.roleList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.employeeSetup = {\r\n        active: false,\r\n        _createUser: false,\r\n      };\r\n      this.active = 0;\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.employeeSetup = {\r\n        active: false,\r\n        _createUser: false,\r\n      };\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    isPhoneValidAndExists() {\r\n      this.validation.isPhoneValidAndExists(this.employeeList, 'contact_no', this.employeeSetup.contact_no, 'employee_id', this.employeeSetup.employee_id, this)\r\n    },\r\n    isEmailValidAndExists() {\r\n      this.validation.isEmailValidAndExists(this.employeeList, 'email', this.employeeSetup.email, 'employee_id', this.employeeSetup.employee_id, this\r\n      )\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.employeeSetup.first_name) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.last_name) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.cdbirth_date) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.gender) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.contact_no) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.email) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.company_id) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.branch_id) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.department_id) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.designation_id) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.jobtypeid) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (\r\n          !this.employeeSetup._role_id &&\r\n          this.employeeSetup._createUser\r\n        ) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (!this.employeeSetup.cdjoin_date) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (\r\n          !this.employeeSetup._password &&\r\n          this.employeeSetup._createUser\r\n        ) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Error\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else if (this.isPhoneValid || this.isEmailValid) {\r\n          return;\r\n        } else {\r\n          this.employeeSetup.birth_date = moment(\r\n            String(this.employeeSetup.cdbirth_date)\r\n          ).format(\"YYYY-MM-DD\");\r\n          this.employeeSetup.join_date = moment(\r\n            String(this.employeeSetup.cdjoin_date)\r\n          ).format(\"YYYY-MM-DD\");\r\n\r\n          if (!this.employeeSetup.employee_id) {\r\n            let result = await this.employeeSetupService.add(\r\n              this.employeeSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Successfully Created\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.employeeSetupService\r\n                .getAll()\r\n                .then((data) => (this.employeeList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: \"Email already Used\",\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            if (!this.employeeSetup._createUser) {\r\n              this.employeeSetup._createUser = false;\r\n            }\r\n            let result = await this.employeeSetupService.put(\r\n              this.employeeSetup,\r\n              this.employeeSetup.employee_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.employeeSetupService\r\n                .getAll()\r\n                .then((data) => (this.employeeList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Faild\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.employeeSetup = Object.assign({}, obj);\r\n      this.employeeSetup.company_id = obj.company_id;\r\n      this.employeeSetup.branch_id = obj.branch_id;\r\n      this.employeeSetup.department_id = obj.department_id;\r\n      this.employeeSetup.designation_id = obj.designation_id;\r\n      this.employeeSetup.jobtypeid = obj.jobtypeid;\r\n      this.employeeSetup.role_id = obj.role_id;\r\n      this.employeeSetup.cdbirth_date = moment(\r\n        String(this.employeeSetup.birth_date)\r\n      ).format(\"MM/DD/YYYY\");\r\n      this.employeeSetup.cdjoin_date = moment(\r\n        String(this.employeeSetup.join_date)\r\n      ).format(\"MM/DD/YYYY\");\r\n\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n    formatDate(value) {\r\n      return moment(String(value)).format(\"MM/DD/YYYY\");\r\n    },\r\n  },\r\n};"]}]}