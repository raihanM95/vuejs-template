           // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().postERPHospitalLogin('/api/Menu/SaveModulePermissions', menuPermission).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    adMenuPermission(menuPermission, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().postERPHospitalLogin('/api/Menu/SaveMenuPermissions', menuPermission).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    getAllMenu(that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getAllERPHospitalLogin('/api/Menu/GetAllMenu').then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    getAllModuleByPermissionWise(that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getAllERPHospitalLogin('/api/Menu/GetAllModuleByPermissionWise').then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    getMenuByRoleId(roleId, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getByIdERPHospitalLogin('/api/Menu/FindMenuByRoleId?roleId=', roleId).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n}"]}]}