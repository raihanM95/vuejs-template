 <Column field=\"setting_value\" header=\"Value\" style=\"width: 40%\">\r\n        <template #editor=\"{ data }\">\r\n          <Dropdown\r\n            v-model=\"data.setting_value\"\r\n            :options=\"getSettingsValue(data.setting_name)\"\r\n            optionLabel=\"label\"\r\n            optionValue=\"value\"\r\n            placeholder=\"Select\"\r\n          >\r\n            <template #option=\"slotProps\">\r\n              <span>{{ slotProps.option.label }}</span>\r\n            </template>\r\n          </Dropdown>\r\n        </template>\r\n        <template #body=\"slotProps\">\r\n          {{ slotProps.data.setting_value }}\r\n        </template>\r\n      </Column>\r\n      <Column\r\n        :rowEditor=\"true\"\r\n        style=\"width: 10%; min-width: 8rem\"\r\n        bodyStyle=\"text-align:center\"\r\n      ></Column>\r\n    </DataTable>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/security/GlobalSettings\";\r\nexport default app;\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n::v-deep(.editable-cells-table td.p-cell-editing) {\r\n  padding-top: 0;\r\n  padding-bottom: 0;\r\n}\r\n</style>"],"sourceRoot":""}]}