is.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.wordCabinCategorySetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.wordCabinCategorySetup.word_cabin_category_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.wordCabinCategoryList, 'word_cabin_category_name', this.wordCabinCategorySetup.word_cabin_category_name, 'word_cabin_category_id', this.wordCabinCategorySetup.word_cabin_category_id, this)) {\r\n          return;\r\n        } else {\r\n          if (!this.wordCabinCategorySetup.word_cabin_category_id) {\r\n            let result = await this.wordCabinCategorySetupService.add(\r\n              this.wordCabinCategorySetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.getAll();\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.wordCabinCategorySetupService.put(\r\n              this.wordCabinCategorySetup,\r\n              this.wordCabinCategorySetup.word_cabin_category_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.wordCabinCategorySetupService\r\n                .getAll()\r\n                .then((data) => (this.wordCabinCategoryList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.wordCabinCategorySetup = Object.assign({}, obj);\r\n      this.wordCabinCategorySetup.word_cabin_category_id = obj.word_cabin_category_id;\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}