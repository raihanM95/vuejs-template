  <a :href=\"'/ot-list'\" target=\"_blank\">\r\n        <div class=\"highlight-box\">\r\n          <div class=\"initials dashboard-top-background\">\r\n            <img src=\"../../assets/dashboard/ot-running.svg\" alt=\"\" />\r\n          </div>\r\n          <div class=\"highlight-details\">\r\n            <!-- <i class=\"fa fa-user-plus\"></i> -->\r\n            <span>Running</span>\r\n            <span class=\"count\">{{ totalRunning }}</span>\r\n          </div>\r\n        </div>\r\n      </a>\r\n    </div>\r\n    <div class=\"p-col-12 p-md-6 p-xl-4\">\r\n      <a :href=\"'/ot-list'\" target=\"_blank\">\r\n        <div class=\"highlight-box\">\r\n          <div class=\"initials dashboard-top-background\">\r\n            <img src=\"../../assets/dashboard/ot-done.svg\" alt=\"\" />\r\n          </div>\r\n          <div class=\"highlight-details\">\r\n            <!-- <i class=\"fa fa-address-book-o\"></i> -->\r\n            <span>Done</span>\r\n            <span class=\"count\">{{ totalDone }}</span>\r\n          </div>\r\n        </div>\r\n      </a>\r\n    </div>\r\n    <div class=\"p-col-12 p-lg-6\">\r\n      <Panel header=\"Today's OT\" :toggleable=\"true\">\r\n        <DataTable\r\n          :value=\"todaysOTList\"\r\n          class=\"p-datatable-sm\"\r\n          responsiveLayout=\"scroll\"\r\n          :rows=\"5\"\r\n          style=\"margin-bottom: 20px\"\r\n          :paginator=\"true\"\r\n        >\r\n          <Column field=\"ot_schedule_no\" header=\"OT No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">No</span>\r\n              {{ slotProps.data.ot_schedule_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"first_name\" header=\"Patient Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.first_name }}\r\n              {{ slotProps.data.last_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"contact_no\" header=\"Contact\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Contact</span>\r\n              {{ slotProps.data.contact_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"ot_table_name\" header=\"OT Table\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">OT Table</span>\r\n              {{ slotProps.data.ot_table_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"ot_start_time\" header=\"Start\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Start</span>\r\n              {{ formatDateTime(slotProps.data.ot_start_time) }}\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n        <div>\r\n          <a class=\"drill-down\" :href=\"'/ot-list'\" target=\"_blank\"\r\n            >See more...</a\r\n          >\r\n        </div>\r\n      </Panel>\r\n    </div>\r\n\r\n    <div class=\"p-col-12 p-lg-6\">\r\n      <Panel header=\"Upcoming OT\" :toggleable=\"true\">\r\n        <DataTable\r\n          :value=\"upcomingOTList\"\r\n          class=\"p-datatable-sm\"\r\n          responsiveLayout=\"scroll\"\r\n          :rows=\"5\"\r\n          style=\"margin-bottom: 20px\"\r\n          :paginator=\"true\"\r\n        >\r\n          <Column field=\"ot_schedule_no\" header=\"OT No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">No</span>\r\n              {{ slotProps.data.ot_schedule_no }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"first_name\" header=\"Patient Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-tit