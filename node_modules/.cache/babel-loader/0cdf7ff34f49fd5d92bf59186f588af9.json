n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetInvestigationToken/' + id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetAppointmentPayment(id, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetAppointmentPayment/' + id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetInvestigationPayment(id, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetInvestigationPayment/' + id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetRegPatientDateWise(fromDate, toDate, gender, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetRegPatientDateWise?fromDate=' + fromDate + '&toDate=' + toDate + '&gender=' + gender).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetAppointmentList(fromDate, toDate, departmentId, doctorId, gender, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetAppointmentList?fromDate=' + fromDate + '&toDate=' + toDate + '&departmentId=' + departmentId + '&doctorId=' + doctorId + '&gender=' + gender).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetAppointmentPaidAmount(fromDate, toDate, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetAppointmentPaidAmount?fromDate=' + fromDate + '&toDate=' + toDate).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n    GetBirthCertificate(id, that) {\r\n        let loader = that.$loading.show({\r\n            // Optional parameters\r\n            container: that.$refs.formContainer,\r\n            color: '#800080',\r\n            loader: 'bars',\r\n            width: 64,\r\n            height: 64,\r\n            backgroundColor: '#ffffff',\r\n        });\r\n        return new AxiosHelper().getReport('/report/Patient/GetBirthCertificate/' + id).then(res => {\r\n            loader.hide();\r\n            return res;\r\n        });\r\n    }\r\n\r\n    \r\n}"]}]}