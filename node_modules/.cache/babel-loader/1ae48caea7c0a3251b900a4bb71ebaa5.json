AAEdC,oBAAAA,OAAO,EAAE,OAFK;AAGdC,oBAAAA,MAAM,EAAEL,OAAM,CAACzC,IAHD;AAId+C,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD,iBAPM,MAOA;AACL,kBAAA,MAAI,CAACJ,MAAL,CAAYZ,GAAZ,CAAgB;AACda,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,QAFK;AAGdC,oBAAAA,MAAM,EAAEL,OAAM,CAACO,GAHD;AAIdD,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD;;AAhFG;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAoFR,gBAAA,MAAI,CAACJ,MAAL,CAAYZ,GAAZ,CAAgB;AACda,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,QAFK;AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AApFQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA2FX,KAxGM;AAyGPG,IAAAA,IAzGO,gBAyGFC,GAzGE,EAyGG;AACR,WAAK7C,eAAL,GAAuB8C,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBF,GAAlB,CAAvB;AACA,WAAKjD,QAAL,GAAgB,KAAhB;AACA,WAAKD,OAAL,GAAe,IAAf;AACD;AA7GM;AA/CI,CAAf","sourcesContent":["import ReferredBySetupService from \"../../service/setup/ReferredBySetupService\";\r\nimport CommonService from \"../../service/CommonService\";\r\nimport EmployeeSetupService from \"../../service/setup/EmployeeSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      employeeList: [],\r\n      genderList: [],\r\n      referredByList: [],\r\n      referredBySetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.referredBySetupService = new ReferredBySetupService();\r\n    this.commonService = new CommonService();\r\n    this.employeeSetupService = new EmployeeSetupService();\r\n    this.validation = new Validation();\r\n\r\n    this.commonService.GetAllGender().then(\r\n      (data) =>\r\n        //  console.log(data.data)\r\n        (this.genderList = data.data)\r\n    );\r\n\r\n    this.employeeSetupService\r\n      .getAllMarketingTeam()\r\n      .then((data) => (this.employeeList = data.data));\r\n\r\n    this.referredBySetupService\r\n      .getAll()\r\n      .then((data) => (this.referredByList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.referredBySetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.referredBySetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.referredBySetup.doctor_or_agent) {\r\n          return;\r\n        } else if (!this.referredBySetup.reffered_by_name) {\r\n          return;\r\n        } else if (!this.referredBySetup.manual_code) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.referredByList, 'manual_code', this.referredBySetup.manual_code, 'reffered_by_id', this.referredBySetup.reffered_by_id, this)) {\r\n          return;\r\n        } else if (!this.referredBySetup.employee_id) {\r\n          return;\r\n        } else if (!this.referredBySetup.gender) {\r\n          return;\r\n        } else if (!this.referredBySetup.commission_rate) {\r\n          return;\r\n        } else if (!this.referredBySetup.phone_personal) {\r\n          return;\r\n        } else {\r\n          if (!this.referredBySetup.reffered_by_id) {\r\n            let result = await this.referredBySetupService.add(\r\n              this.referredBySetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.referredBySetupService\r\n                .getAll()\r\n                .then((data) => (this.referredByList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.referredBySetupService.put(\r\n              this.referredBySetup,\r\n              this.referredBySetup.reffered_by_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.referredBySetupService\r\n                .getAll()\r\n                .then((data) => (this.referredByList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.referredBySetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}