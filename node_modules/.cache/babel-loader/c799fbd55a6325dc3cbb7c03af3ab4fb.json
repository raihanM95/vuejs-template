AA,MAAM,EAAEU,MAAM,CAAC1E,IAHD;AAIdiE,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD,iBAPM,MAOA;AACL,kBAAA,MAAI,CAACJ,MAAL,CAAYH,GAAZ,CAAgB;AACdI,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,QAFK;AAGdC,oBAAAA,MAAM,EAAEU,MAAM,CAACE,GAHD;AAIdX,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD;;AA9Ba;AAAA;;AAAA;AAiCd,gBAAA,MAAI,CAACJ,MAAL,CAAYH,GAAZ,CAAgB;AACdI,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,UAFK;AAGdC,kBAAAA,MAAM,EAAE,+BAHM;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAjCc;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAyChB,gBAAA,MAAI,CAACJ,MAAL,CAAYH,GAAZ,CAAgB;AACdI,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,QAFK;AAGdC,kBAAAA,MAAM,cAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAzCgB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgDnB;AAjNM;AA9DI,CAAf","sourcesContent":["import NurseScheduleService from \"../../service/nurse-panel/NurseScheduleService\";\r\nimport WordCabinSetupService from \"../../service/setup/WordCabinSetupService\";\r\nimport ShiftSetupService from \"../../service/setup/ShiftSetupService\";\r\nimport moment from \"moment\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\nimport $ from \"jquery\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      fileDialog: false,\r\n      file: \"\",\r\n      dataList: true,\r\n      button: true,\r\n      shiftList: [],\r\n      wordCabinList: [],\r\n      nurseScheduleList: [],\r\n      nurseScheduleListFinal: [],\r\n      nurseSchedule: {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        //cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n        isCheck: true,\r\n      },\r\n      submitted: false,\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    // past date-year disabled\r\n    // let today = new Date();\r\n    // let month = today.getMonth();\r\n    // let year = today.getFullYear();\r\n    // let prevMonth = (month === 0) ? 11 : month - 0;\r\n    // let prevYear = (prevMonth === 11) ? year - 0 : year;\r\n    // this.minDate = new Date();\r\n    // this.minDate.setMonth(prevMonth);\r\n    // this.minDate.setFullYear(prevYear);\r\n    this.minDate = new Date(new Date().setDate(new Date().getDate()));\r\n\r\n    this.shiftSetupService = new ShiftSetupService();\r\n    this.wordCabinSetupService = new WordCabinSetupService();\r\n    this.nurseScheduleService = new NurseScheduleService();\r\n\r\n    this.shiftSetupService\r\n      .getAll().then((data) => this.shiftFilter(data.data));\r\n\r\n    this.wordCabinSetupService\r\n      .getAll()\r\n      .then((data) => this.wordCabinFormatted(data.data));\r\n\r\n    this.nurseDutyForSetRoster();\r\n  },\r\n  methods: {\r\n    wordCabinFormatted(data) {\r\n      var selectAll = {\r\n        word_cabin_id: \"All\",\r\n        word_cabin_name: \"All\",\r\n      }\r\n\r\n      data.splice(0, 0, selectAll)\r\n      this.wordCabinList = data;\r\n      this.nurseSchedule.word_cabin_id = \"All\";\r\n    },\r\n    openFileDialog() {\r\n      this.submitted = false;\r\n      this.fileDialog = true;\r\n    },\r\n    selectFile() {\r\n      this.file = this.$refs.file.files[0];\r\n    },\r\n    checkAll() {\r\n      if (!this.nurseSchedule.isCheck) {\r\n        this.nurseScheduleList.forEach((element) => {\r\n          element.isCheck = true;\r\n        })\r\n      } else {\r\n        this.nurseScheduleList.forEach((element) => {\r\n          element.isCheck = false;\r\n        })\r\n      }\r\n    },\r\n    shiftFilter(data) {\r\n      this.shiftList = data.filter(function (el) {\r\n        return el.shift_category_id == \"1\" && el.active == true;\r\n      }, this);\r\n    },\r\n    hideNew() {\r\n      this.nurseSchedule = {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        //cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(\r\n          String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        ).format(\"MM/DD/YYYY\"),\r\n      };\r\n      //this.dataList = false;\r\n      //this.button = false;\r\n      this.submitted = false;\r\n      this.fileDialog = false;\r\n    },\r\n    nurseDutyForSetRoster() {\r\n      this.nurseScheduleService\r\n        .getNurseDutyForSetRoster()\r\n        .then((data) => (this.nurseScheduleList = data.data));\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.nurseSchedule.word_cabin_id) {\r\n          return;\r\n        } else if (!this.nurseSchedule.cdfromdate) {\r\n          return;\r\n        } else if (!this.nurseSchedule.cdtodate) {\r\n          return;\r\n        }\r\n        this.requred = \"\";\r\n        this.nurseScheduleList.forEach((element) => {\r\n          if (element.isCheck && !element.shift_id) {\r\n            this.requred = \"required\";\r\n          }\r\n        });\r\n\r\n        if (this.requred) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Required\",\r\n            detail: \"Please select Shift\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else {\r\n          this.nurseScheduleList = this.nurseScheduleList.filter(function (elm) {\r\n            return elm.isCheck == true;\r\n          }, this);\r\n          if (this.nurseSchedule.word_cabin_id == \"All\") {\r\n            this.nurseScheduleListFinal = [];\r\n            this.wordCabinList.forEach((el) => {\r\n              if (el.word_cabin_id != \"All\") {\r\n                this.nurseScheduleList.forEach((element) => {\r\n                  var obj = Object.assign({}, element);\r\n                  obj.word_cabin_id = el.word_cabin_id;\r\n                  obj.from_date = moment(\r\n                    String(this.nurseSchedule.cdfromdate)\r\n                  ).format(\"YYYY-MM-DD\");\r\n                  obj.to_date = moment(\r\n                    String(this.nurseSchedule.cdtodate)\r\n                  ).format(\"YYYY-MM-DD\");\r\n\r\n                  //\r\n                  this.nurseScheduleListFinal.push(obj);\r\n                });\r\n              }\r\n            });\r\n          } else {\r\n            this.nurseScheduleListFinal = [];\r\n            this.nurseScheduleList.forEach((element) => {\r\n              element.word_cabin_id = this.nurseSchedule.word_cabin_id;\r\n              element.from_date = moment(\r\n                String(this.nurseSchedule.cdfromdate)\r\n              ).format(\"YYYY-MM-DD\");\r\n              element.to_date = moment(\r\n                String(this.nurseSchedule.cdtodate)\r\n              ).format(\"YYYY-MM-DD\");\r\n            });\r\n            this.nurseScheduleListFinal = this.nurseScheduleList;\r\n          }\r\n          \r\n          if (this.nurseScheduleListFinal.length > 0) {\r\n            let result = await this.nurseScheduleService.add(\r\n              this.nurseScheduleListFinal, this\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n          else {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"Required\",\r\n              detail: \"Nurse select and save schedule\",\r\n              life: 10000,\r\n            });\r\n            this.nurseDutyForSetRoster();\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    async addManually() {\r\n      try {\r\n        if ($(\"#file\")[0].files.length != 0) {\r\n          const formData = new FormData();\r\n          formData.append(\"file\", this.file);\r\n          let result = await this.nurseScheduleService.addManually(\r\n            formData, this\r\n          );\r\n          if (result.status === 201) {\r\n            this.$toast.add({\r\n              severity: \"success\",\r\n              summary: \"Success\",\r\n              detail: \"Saved Successfully\",\r\n              life: 10000,\r\n            });\r\n            this.hideNew();\r\n          } else if (result.status === 401) {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"Error\",\r\n              detail: result.data,\r\n              life: 10000,\r\n            });\r\n          } else {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"Failed\",\r\n              detail: result.res,\r\n              life: 10000,\r\n            });\r\n          }\r\n        }\r\n        else {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Required\",\r\n            detail: \"Select file and save schedule\",\r\n            life: 10000,\r\n          });\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n  },\r\n};"]}]}