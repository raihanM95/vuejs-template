primevue/api\";\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            isExist: \"\",\r\n            addList: false,\r\n            button: false,\r\n            packageList: [],\r\n            investigationList: [],\r\n            //servicePackageDetailsList: [],\r\n            healthPackageDetailsSetup: {\r\n                lstHealthPackageDetails: []\r\n            },\r\n            healthPackageSetup: {},\r\n            filters: {\r\n                global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n                name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            },\r\n            initFilters() {\r\n                this.filters = {\r\n                    global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n                    name: {\r\n                        operator: FilterOperator.AND,\r\n                        constraints: [\r\n                            { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n                        ],\r\n                    },\r\n                };\r\n            },\r\n            submitted: false,\r\n            submitted2: false,\r\n            submitted3: false,\r\n            packageDialog: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.healthPackageDetailsSetupService = new HealthPackageDetailsSetupService();\r\n        this.investigationSetupService = new InvestigationSetupService();\r\n        this.healthPackageSetupService = new HealthPackageSetupService();\r\n        this.validation = new Validation();\r\n\r\n        // this.healthPackageSetupService\r\n        //         .getActiveCombo()\r\n        //         .then((data) => (this.packageList = data.data));\r\n        this.getHealthPackage();\r\n\r\n        this.investigationSetupService\r\n            .getAll()\r\n            .then((data) => (this.investigationList = data.data));\r\n    },\r\n    methods: {\r\n        getHealthPackage() {\r\n            this.healthPackageSetupService\r\n                .getActiveCombo()\r\n                .then((data) => this.getPackageFormatted(data.data));\r\n        },\r\n        getPackageFormatted(data){\r\n            data.forEach((element) => {\r\n                this.packageList.push({\r\n                  value: element.value,\r\n                  text: element.refid + \"-\" + element.text ,\r\n                });\r\n              });\r\n            this.packageDialog = false;\r\n            this.submitted2 = false;\r\n        },\r\n        hideNew() {\r\n            //this.servicePackageDetailsList = [];\r\n            this.healthPackageDetailsSetup = {\r\n                lstHealthPackageDetails: []\r\n            };\r\n            this.healthPackageSetup = {};\r\n\r\n            this.submitted = false;\r\n            this.submitted2 = false;\r\n            this.submitted3 = false;\r\n            this.isExist = \"\";\r\n            this.addList = false;\r\n            this.button = false;\r\n        },\r\n\r\n        packageWiseInvestigationList() {\r\n            if(this.healthPackageDetailsSetup.health_package_id){\r\n                this.healthPackageDetailsSetupService\r\n                .getHealthPackageByPackageId(this.healthPackageDetailsSetup.health_package_id)\r\n                .then((data) => this.formatPackageDetailsList(data.data));\r\n            } else {\r\n                this.hideNew();\r\n            }\r\n        },\r\n        formatPackageDetailsList(data) {\r\n            this.healthPackageDetailsSetup = data;\r\n            // this.testParameterList = data.filter(function (el) {\r\n            //     return el.machine_id == this.testParameterSetup.machine_id;\r\n            // }, this);\r\n            this.addList = true;\r\n            this.button = true;\r\n        },\r\n        addInvestigation() {\r\n            this.submitted = true;\r\n            var aInvestigation = this.healthPackageDetailsSetup.lstHealthPackageDetails.filter(function (el) {\r\n                return (\r\n                    el.investigation_id ===\r\n                    this.healthPackageDetailsSetup.ddlInvestigation.value\r\n                );\r\n            }, this);\r\n\r\n            if (aInvestigation.length) {\r\n                this.isExist = \"This Investigation already added!\";\r\n            } else {\r\n                if (!this.healthPackageDetailsSetup.health_package_id) {\r\n                    return;\r\n                } else if (!this.healthPackageDetailsSetup.ddlInvestigation) {\r\n                    return;\r\n                } else {\r\n                    this.isExist = \"\";\r\n                    this.healthPackageDetailsSetup.lstHealthPackageDetails.push({\r\n                        health_package_id: this.healthPackageDetailsSetup.health_package_id,\r\n                        investigation_id: this.healthPackageDetailsSetup.ddlInvestigation.investigation_id,\r\n                        _investingation_name: this.healthPackageDetailsSetup.ddlInvestigation.investingation_name,\r\n                        _charge: this.healthPackageDetailsSetup.ddlInvestigation.charge,\r\n                    });\r\n                    if(!this.healthPackageDetailsSetup.original_charge){\r\n                        this.healthPackageDetailsSetup.original_charge = 0;\r\n                    }\r\n                    this.healthPackageDetailsSetup.original_charge += this.healthPackageDetailsSetup.ddlInvestigation.charge\r\n                }\r\n            }\r\n\r\n            this.addList = true;\r\n            this.button = true;\r\n        },\r\n        deleteFromList(data) {\r\n            this.healthPackageDetailsSetup.lstHealthPackageDetails = this.healthPackageDetailsSetup.lstHealthPackageDetails.filter(function (el) {\r\n                return el.investigation_id != data.investigation_id;\r\n            }, this);\r\n\r\n            if (!this.healthPackageDetailsSetup.lstHealthPackageDetails.length) {\r\n                this.addList = false;\r\n                this.button = false;\r\n            } else {\r\n                this.button = true;\r\n            }\r\n        },\r\n        newPackage() {\r\n            this.packageDialog = true;\r\n        },\r\n        hideDialog() {\r\n            this.healthPackageDetailsSetup = {\r\n                lstHealthPackageDetails: []\r\n            };\r\n            this.healthPackageSetup = {};\r\n            \r\n            this.packageDialog = false;\r\n            this.submitted2 = false;\r\n        },\r\n        async addPackage() {\r\n            try {\r\n                this.submitted2 = true;\r\n                if (!this.healthPackageSetup.manual_code) {\r\n                    return;\r\n                } else if (!this.healthPackageSetup.health_package_name) {\r\n                    return;\r\n                } else if (this.validation.isNameExists(this.packageList, 'text', this.healthPackageSetup.health_package_name, 'value', this.healthPackageSetup.health_package_id, this)) {\r\n                    return;\r\n                } else {\r\n                    let result = await this.healthPackageSetupService.add(\r\n                        this.healthPackageSetup\r\n                    );\r\n                    if (result.status === 201) {\r\n                        this.$toast.add({\r\n                            severity: \"success\",\r\n                            summary: \"Success\",\r\n                            detail: \"Saved Successfully\",\r\n                            life: 10000,\r\n                        });\r\n                        this.getHealthPackage();\r\n                        this.hideNew();\r\n                        //this.hideDialog();\r\n                    } else {\r\n                        this.$toast.add({\r\n                            severity: \"error\",\r\n                            summary: \"Failed\",\r\n                            detail: result.res,\r\n                            life: 10000,\r\n               