{"remainingRequest":"I:\\sigma-vue\\node_modules\\babel-loader\\lib\\index.js!I:\\sigma-vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader-v16\\dist\\templateLoader.js??ref--6!I:\\sigma-vue\\node_modules\\cache-loader\\dist\\cjs.js??ref--0-0!I:\\sigma-vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader-v16\\dist\\index.js??ref--0-1!I:\\sigma-vue\\src\\components\\Documentation.vue?vue&type=template&id=38e80c8c&scoped=true","dependencies":[{"path":"I:\\sigma-vue\\src\\components\\Documentation.vue","mtime":1696333198466},{"path":"I:\\sigma-vue\\babel.config.js","mtime":1696333198378},{"path":"I:\\sigma-vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"I:\\sigma-vue\\node_modules\\babel-loader\\lib\\index.js","mtime":456789000000},{"path":"I:\\sigma-vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader-v16\\dist\\templateLoader.js","mtime":499162500000},{"path":"I:\\sigma-vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"I:\\sigma-vue\\node_modules\\@vue\\cli-service\\node_modules\\vue-loader-v16\\dist\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["I:\\sigma-vue\\src\\components\\Documentation.vue"],"names":[],"mappings":";;;;;;;AACS,EAAA,KAAK,EAAC;;;AACF,EAAA,KAAK,EAAC;;;AACF,EAAA,KAAK,EAAC;;;;;gDA0CX,E;;+BACZ,YAAA,CA4BO,MA5BP,EA4BO,IA5BP,EA4BO,C,aA5BD,YAAA,CA2BK,UA3BL,EA2BK,IA3BL,EA2BK,C,8BA3BW,y+B,CA2BX,CA3BL,CA4BC,E,8BADI,M,CACJ,CA5BP,E;;AAAA,C;;+BA8BgB,YAAA,CAAa,IAAb,EAAa,IAAb,EAAI,MAAJ,EAAQ,CAAA;AAAA;AAAR,C;;+BACA,YAAA,CAEyJ,GAFzJ,EAEyJ,IAFzJ,EAEyJ,C,8BAFtJ,0C,CAEsJ,E,aAF9G,YAAA,CAAkB,GAAlB,EAAkB,IAAlB,EAAG,aAAH,CAE8G,E,8BAF5F,sG,CAE4F,E,aADzH,YAAA,CAAc,GAAd,EAAc,IAAd,EAAG,SAAH,CACyH,E,8BAD3G,kO,CAC2G,CAFzJ,E;;AAAA,C;;gDAIG,E;;+BACnB,YAAA,CAkGO,MAlGP,EAkGO,IAlGP,EAAM,sjLAAN,EAkGA,CAAA;AAAA;AAlGA,C;;+BAoGgB,YAAA,CAAqB,IAArB,EAAqB,IAArB,EAAI,cAAJ,EAAgB,CAAA;AAAA;AAAhB,C;;+BAEA,YAAA,CAA0K,GAA1K,EAA