B,MAAM,CAACC,MAAP,KAAkB,GAAtB,EAA2B;AAChC,kBAAA,MAAI,CAACR,MAAL,CAAYJ,GAAZ,CAAgB;AACdK,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,OAFK;AAGdC,oBAAAA,MAAM,EAAEI,MAAM,CAAClD,IAHD;AAId+C,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD,iBAPM,MAOA;AACL,kBAAA,MAAI,CAACJ,MAAL,CAAYJ,GAAZ,CAAgB;AACdK,oBAAAA,QAAQ,EAAE,OADI;AAEdC,oBAAAA,OAAO,EAAE,QAFK;AAGdC,oBAAAA,MAAM,EAAEI,MAAM,CAACE,GAHD;AAIdL,oBAAAA,IAAI,EAAE;AAJQ,mBAAhB;AAMD;;AArEK;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAwER,gBAAA,MAAI,CAACJ,MAAL,CAAYJ,GAAZ,CAAgB;AACdK,kBAAAA,QAAQ,EAAE,OADI;AAEdC,kBAAAA,OAAO,EAAE,QAFK;AAGdC,kBAAAA,MAAM,aAHQ;AAIdC,kBAAAA,IAAI,EAAE;AAJQ,iBAAhB;;AAxEQ;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA+EX;AAzHM;AA3BI,CAAf","sourcesContent":["import ConsultancyDepartmentSetupService from \"../../service/setup/ConsultancyDepartmentSetupService\";\r\nimport DoctorSetupService from \"../../service/setup/DoctorSetupService\";\r\nimport SetupReportService from \"../../service/report/SetupReportService\";\r\nimport CommonService from \"../../service/CommonService\"\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dataList: false,\r\n      button: false,\r\n      departmentList: [],\r\n      doctorList: [],\r\n      scheduleList: [],\r\n      doctorScheduleSetup: {},\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.doctorSetupService = new DoctorSetupService();\r\n    this.consultancyDepartmentSetupService = new ConsultancyDepartmentSetupService();\r\n    this.setupReportService = new SetupReportService();\r\n    this.commonService = new CommonService();\r\n\r\n\r\n    this.consultancyDepartmentSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.departmentList = data.data));\r\n\r\n    this.doctorSetupService\r\n      .getAll()\r\n      .then((data) => this.mergeDoctorInfo(data.data));\r\n  },\r\n  methods: {\r\n    printAll() {\r\n      this.setupReportService.GetHospitalDoctorSchedule(this.doctorScheduleSetup.doctor_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n    },\r\n    hideNew() {\r\n      this.doctorScheduleSetup = {};\r\n      this.dataList = false;\r\n      this.button = false;\r\n      this.submitted = false;\r\n    },\r\n    mergeDoctorInfo(data) {\r\n      data.forEach((element) => {\r\n        this.doctorList.push({\r\n          department_id: element.department_id,\r\n          doctor_id: element.doctor_id,\r\n          doctor_name: element.doc_no + \"-\" + element.doctor_name,\r\n        });\r\n      });\r\n    },\r\n    filterDoctorsDepartmentWise() {\r\n      return this.doctorList.filter(function (el) {\r\n        return el.department_id === this.doctorScheduleSetup.department_id;\r\n      }, this);\r\n    },\r\n    getFormatted(data) {\r\n      data.forEach((element) => {\r\n        element.start_time1 = new Date(\"2000-01-01 \" + element.start_time1);\r\n        element.end_time1 = new Date(\"2000-01-01 \" + element.end_time1);\r\n        element.start_time2 = new Date(\"2000-01-01 \" + element.start_time2);\r\n        element.end_time2 = new Date(\"2000-01-01 \" + element.end_time2);\r\n      });\r\n\r\n      this.scheduleList = data;\r\n    },\r\n    getDoctorScheduleById() {\r\n      this.dataList = true;\r\n      this.button = true;\r\n\r\n      this.doctorSetupService\r\n        .getDoctorScheduleById(this.doctorScheduleSetup.doctor_id)\r\n        .then((data) => this.getFormatted(data.data));\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.doctorScheduleSetup.doctor_id) {\r\n          return;\r\n        }\r\n        this.requred = \"\";\r\n        this.scheduleList.forEach((element) => {\r\n          if (element.time1_patient_no < 0) {\r\n            this.requred = \"required\";\r\n          }\r\n          if (!element.time2_patient_no < 0) {\r\n            this.requred = \"required\";\r\n          }\r\n        });\r\n\r\n        if (this.requred) {\r\n          this.$toast.add({\r\n            severity: \"error\",\r\n            summary: \"Required\",\r\n            detail: \"Please fill in the required fields\",\r\n            life: 10000,\r\n          });\r\n          return;\r\n        } else {\r\n          this.scheduleList.forEach((element) => {\r\n            element.start_time1 =\r\n              element.start_time1 == \"\"\r\n                ? (element.start_time1 = \"00:00\")\r\n                : element.start_time1.toLocaleTimeString(\"it-IT\");\r\n            element.end_time1 =\r\n              element.end_time1 == \"\"\r\n                ? (element.end_time1 = \"00:00\")\r\n                : element.end_time1.toLocaleTimeString(\"it-IT\");\r\n            element.start_time2 =\r\n              element.start_time2 == \"\"\r\n                ? (element.start_time2 = \"00:00\")\r\n                : element.start_time2.toLocaleTimeString(\"it-IT\");\r\n            element.end_time2 =\r\n              element.end_time2 == \"\"\r\n                ? (element.end_time2 = \"00:00\")\r\n                : element.end_time2.toLocaleTimeString(\"it-IT\");\r\n          });\r\n\r\n          let result = await this.doctorSetupService.addDoctorSchedule(\r\n            this.scheduleList\r\n          );\r\n          if (result.status === 201) {\r\n            this.$toast.add({\r\n              severity: \"success\",\r\n              summary: \"Success\",\r\n              detail: \"Saved Successfully\",\r\n              life: 10000,\r\n            });\r\n            this.hideNew();\r\n          } else if (result.status === 401) {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"Error\",\r\n              detail: result.data,\r\n              life: 10000,\r\n            });\r\n          } else {\r\n            this.$toast.add({\r\n              severity: \"error\",\r\n              summary: \"Failed\",\r\n              detail: result.res,\r\n              life: 10000,\r\n            });\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n  },\r\n};"]}]}