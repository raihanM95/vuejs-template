AA/FM;AA9BI,CAAf","sourcesContent":["import MedicineTypeSetupService from \"../../service/setup/MedicineTypeSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      newInfo: false,\r\n      dataList: true,\r\n      medicineTypeList: [],\r\n      medicineTypeSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.medicineTypeSetupService = new MedicineTypeSetupService();\r\n    this.validation = new Validation();\r\n    this.medicineTypeSetupService.getAll().then((data) => (this.medicineTypeList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.medicineTypeSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.medicineTypeSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.medicineTypeSetup.medicine_type_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.medicineTypeList, 'medicine_type_name', this.medicineTypeSetup.medicine_type_name, 'medicine_type_id', this.medicineTypeSetup.medicine_type_id, this)) {\r\n          return;\r\n        } else {\r\n          if (!this.medicineTypeSetup.medicine_type_id) {\r\n            let result = await this.medicineTypeSetupService.add(this.medicineTypeSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.medicineTypeSetupService\r\n                .getAll()\r\n                .then((data) => (this.medicineTypeList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.medicineTypeSetupService.put(\r\n              this.medicineTypeSetup,\r\n              this.medicineTypeSetup.medicine_type_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.medicineTypeSetupService\r\n                .getAll()\r\n                .then((data) => (this.medicineTypeList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.medicineTypeSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}