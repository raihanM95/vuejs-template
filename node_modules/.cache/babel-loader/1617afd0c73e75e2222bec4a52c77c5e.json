J,OATT,E;AAKa,QAAA,IAAI,EAAA,OAAA,CACb,UADwB,SACxB;AAAA,iBADwB,CACxB,WADwB,E,iBACsB,MAC9C,gBAAA,CAAG,SAAS,CAAC,IAAV,CAAe,kBAAlB,C,EAAoC,C,CAFZ,CACxB;AAAA,SADa,C;;OALjB,CADS,EAqBT,YAAA,CAKS,iBALT,EAKS;AALD,QAAA,KAAK,EAAC,aAKL;AALmB,QAAA,MAAM,EAAC,aAK1B;AALyC,QAAA,QAAQ,EAAE;AAKnD,OALT,E;AACa,QAAA,IAAI,EAAA,OAAA,CACb,UADwB,SACxB;AAAA,iBADwB,CACxB,WADwB,E,iBACuB,MAC/C,gBAAA,CAAG,SAAS,CAAC,IAAV,CAAe,WAAlB,C,EAA6B,C,CAFL,CACxB;AAAA,SADa,C;;OADjB,CArBS,EA2BT,YAAA,CAcS,iBAdT,EAcS;AAdD,QAAA,KAAK,EAAC,cAcL;AAdoB,QAAA,MAAM,EAAC,WAc3B;AAdwC,QAAA,QAAQ,EAAE;AAclD,OAdT,E;AACa,QAAA,IAAI,EAAA,OAAA,CACb,UADwB,SACxB;AAAA,iBADwB,CACxB,WADwB,E,gBAExB,YAAA,CAIC,MAJD,EAAA,WAAA,EAGG,KAHH,EAGM,GAHN,C,EAGM,C,SAFI,SAAS,CAAC,IAAV,CAAe,Y,CAEnB,C,CALkB,E,gBAOxB,YAAA,CAIC,MAJD,EAAA,WAAA,EAGG,IAHH,EAGK,GAHL,C,EAGK,C,UAFM,SAAS,CAAC,IAAV,CAAe,Y,CAErB,C,CAVmB,CACxB;AAAA,SADa,C;;OADjB,CA3BS,EA0CT,YAAA,CAYS,iBAZT,EAYS;AAXP,QAAA,WAAW,EAAC,iCAWL;AAVP,QAAA,SAAS,EAAC,iCAUH;AATP,QAAA,WAAW,EAAC;AASL,OAZT,E;AAKa,QAAA,IAAI,EAAA,OAAA,CACb,UADwB,SACxB;AAAA,iBADwB,CACxB,YAAA,CAIE,iBAJF,EAIE;AAHA,YAAA,IAAI,EAAC,cAGL;AAFA,YAAA,KAAK,EAAC,0CAEN;AADC,YAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,qBAAE,IAAA,CAAA,IAAA,CAAK,SAAS,CAAC,IAAf,CAAF;AAAA;AACN,WAJF,E,IAAA,E,CAAA,E,WAAA,CADwB,CACxB;AAAA,SADa,C;;OALjB,CA1CS,CALT;AAAA,K;;GA9BF,E,CAAA,E,oBAAA,CA2FI,CAvGN,CAwGI,CAzGN,CA0GI,CA3GN,E,GAAA,C,YAAsC,IAAA,CAAA,Q","sourcesContent":["<template>\r\n  <div class=\"card\" v-show=\"newInfo\">\r\n    <h3 class=\"p-text-center\" style=\"margin-bottom: 28px\">Figure Info</h3>\r\n    <div class=\"p-fluid p-grid\">\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <InputText\r\n            id=\"figure_name\"\r\n            v-model.trim=\"figureSetup.figure_name\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !figureSetup.figure_name,\r\n            }\"\r\n            v-on:keyup.enter=\"this.$refs.figure_group_id.$el.focus()\"\r\n          />\r\n          <label for=\"figure_name\">Name<span style=\"color: red\">*</span></label>\r\n        </div>\r\n        <small class=\"p-invalid\" v-if=\"submitted && !figureSetup.figure_name\"\r\n          >Name is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-3\">\r\n        <input\r\n          type=\"file\"\r\n          @change=\"onFileChange\"\r\n          class=\"file\"\r\n          accept=\"image/*\"\r\n          ref=\"inputImage\"\r\n        />\r\n        <div v-if=\"imageError\" class=\"imageError\">\r\n          {{ imageError }}\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-3\">\r\n        <output>\r\n          <img\r\n            :src=\"figureSetup.cffigure_image\"\r\n            v-if=\"figureSetup.cffigure_image\"\r\n            style=\"width: 300px; height: 200px\"\r\n          />\r\n          <p v-else>No image...</p>\r\n        </output>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <Dropdown\r\n            id=\"figure_group_id\"\r\n            v-model=\"figureSetup.figure_group_id\"\r\n            :options=\"figureGroupList\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !figureSetup.figure_group_id,\r\n            }\"\r\n            ref=\"figure_group_id\"\r\n            v-on:keyup.enter=\"this.$refs.department_id.$el.focus()\"\r\n          />\r\n          <label for=\"figure_group_id\"\r\n            >Figure Groups<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !figureSetup.figure_group_id\"\r\n          >Group is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-6\">\r\n        <div class=\"p-float-label\">\r\n          <Dropdown\r\n            id=\"department_id\"\r\n            v-model=\"figureSetup.department_id\"\r\n            :options=\"departmentList\"\r\n            optionLabel=\"text\"\r\n            optionValue=\"value\"\r\n            required=\"true\"\r\n            :filter=\"true\"\r\n            :showClear=\"true\"\r\n            autofocus\r\n            :class=\"{\r\n              'p-invalid': submitted && !figureSetup.department_id,\r\n            }\"\r\n            ref=\"department_id\"\r\n            v-on:keyup.enter=\"this.$refs.description.$el.focus()\"\r\n          />\r\n          <label for=\"department_id\"\r\n            >Department<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small class=\"p-invalid\" v-if=\"submitted && !figureSetup.department_id\"\r\n          >Department is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-12\">\r\n        <div class=\"p-float-label\">\r\n          <Textarea\r\n            id=\"description\"\r\n            v-model=\"figureSetup.description\"\r\n            required=\"true\"\r\n            rows=\"3\"\r\n            cols=\"20\"\r\n            ref=\"description\"\r\n          />\r\n          <label for=\"description\">Description</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-10\">\r\n        <div class=\"p-field-checkbox\">\r\n          <Checkbox\r\n            id=\"is_favourite\"\r\n            v-model=\"figureSetup.is_favourite\"\r\n            :binary=\"true\"\r\n          />\r\n          <!-- <input\r\n            type=\"checkbox\"\r\n            id=\"is_favourite\"\r\n            name=\"is_favourite\"\r\n            v-model=\"figureSetup.is_favourite\"\r\n          /> -->\r\n          <label for=\"is_favourite\">Favourite</label>\r\n        </div>\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Save\"\r\n          @click=\"add\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5; border: none\"\r\n        />\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Cancel\"\r\n          @click=\"hideNew\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #fb465e; border: none\"\r\n        />\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <div class=\"p-grid crud-demo\" v-show=\"dataList\">\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <Toolbar class=\"p-mb-4\">\r\n          <template v-slot:left>\r\n            <Button\r\n              label=\"New\"\r\n              icon=\"pi pi-plus\"\r\n              class=\"p-button-success p-mr-2\"\r\n              @click=\"openNew\"\r\n            />\r\n          </template>\r\n        </Toolbar>\r\n\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"figureList\"\r\n          dataKey=\"figure_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Manage Figures</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"figure_name\" header=\"Name\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Name</span>\r\n              {{ slotProps.data.figure_name }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"_figure_group_name\"\r\n            header=\"Group Name\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Group Name</span>\r\n              {{ slotProps.data._figure_group_name }}\r\n            </template>\r\n          </Column>\r\n          <!-- <Column\r\n            field=\"_department_name\"\r\n            header=\"Department Name\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Department Name</span>\r\n              {{ slotProps.data._department_name }}\r\n            </template>\r\n          </Column> -->\r\n          <Column field=\"description\" header=\"Description\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Description</span>\r\n              {{ slotProps.data.description }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"is_favourite\" header=\"Favourite\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Status</span>\r\n              <span\r\n                v-show=\"slotProps.data.is_favourite\"\r\n                class=\"product-badge status-active\"\r\n                >YES</span\r\n              >\r\n              <span\r\n                v-show=\"!slotProps.data.is_favourite\"\r\n                class=\"product-badge status-inactive\"\r\n                >NO</span\r\n              >\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <Button\r\n                icon=\"pi pi-pencil\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"edit(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from  \"../../scripts/setup/FigureSetup\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-active {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-inactive {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n}\r\n\r\n.imageError {\r\n  color: red;\r\n}\r\n\r\n// .file {\r\n//   border-radius: 4px;\r\n//   padding: .5rem;\r\n//   margin-bottom: .5rem;\r\n//   width: 100%;\r\n// }\r\n</style>"],"sourceRoot":""}]}