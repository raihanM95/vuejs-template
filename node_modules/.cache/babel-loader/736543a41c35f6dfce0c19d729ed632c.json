 initFilters() {\r\n                this.filters = {\r\n                    global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n                    name: {\r\n                        operator: FilterOperator.AND,\r\n                        constraints: [\r\n                            { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n                        ],\r\n                    },\r\n                };\r\n            },\r\n            submitted: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.shiftSetupService = new ShiftSetupService();\r\n        this.shiftCategorySetupService = new ShiftCategorySetupService();\r\n        this.validation = new Validation();\r\n        this.shiftCategorySetupService\r\n            .getActiveCombo()\r\n            .then((data) => (this.shiftCategoryList = data.data));\r\n        this.getAll();\r\n    },\r\n    methods: {\r\n        getAll() {\r\n            this.shiftSetupService\r\n                .getAll()\r\n                .then((data) => (this.shiftList = data.data));\r\n        },\r\n        openNew() {\r\n            this.shiftSetup = {};\r\n            this.newInfo = true;\r\n            this.dataList = false;\r\n            this.submitted = false;\r\n        },\r\n        hideNew() {\r\n            this.shiftSetup = {};\r\n            this.dataList = true;\r\n            this.newInfo = false;\r\n            this.submitted = false;\r\n        },\r\n        async add() {\r\n            this.submitted = true;\r\n            try {\r\n                if (!this.shiftSetup.shift_name) {\r\n                    return;\r\n                } else if (this.validation.isNameExistswithReference(this.shiftList, 'shift_name', this.shiftSetup.shift_name, 'shift_id', this.shiftSetup.shift_id, 'shift_category_id', this.shiftSetup.shift_category_id, this)) {\r\n                    return;\r\n                } else if (!this.shiftSetup.shift_category_id) {\r\n                    return;\r\n                } else if (!this.shiftSetup.start_timev) {\r\n                    return;\r\n                } else if (!this.shiftSetup.end_timev) {\r\n                    return;\r\n                } else {\r\n                    this.shiftSetup.start_time = this.shiftSetup.start_timev.toLocaleTimeString(\"it-IT\");\r\n                    this.shiftSetup.end_time = this.shiftSetup.end_timev.toLocaleTimeString(\"it-IT\");\r\n                    if (!this.shiftSetup.shift_id) {\r\n                        let result = await this.shiftSetupService.add(\r\n                            this.shiftSetup\r\n                        );\r\n                        if (result.status === 201) {\r\n                            this.$toast.add({\r\n                                severity: \"success\",\r\n                                summary: \"Success\",\r\n                                detail: \"Saved Successfully\",\r\n                                life: 10000,\r\n                            });\r\n                            this.hideNew();\r\n                            this.getAll();\r\n                        } else if (result.status === 401) {\r\n                            this.$toast.add({\r\n                                severity: \"error\",\r\n                                summary: \"Error\",\r\n                                detail: result.data,\r\n                                life: 10000,\r\n                            });\r\n                        } else {\r\n                            this.$toast.add({\r\n                                severity: \"error\",\r\n                                summary: \"Failed\",\r\n                                detail: result.res,\r\n                                life: 10000,\r\n                            });\r\n                        }\r\n                    } else {\r\n                        let result = await this.shiftSetupService.put(\r\n                            this.shiftSetup,\r\n                            this.shiftSetup.shift_id\r\n                        );\r\n                        if (result.status === 202) {\r\n                            this.$toast.add({\r\n                                severity: \"success\",\r\n                                summary: \"Success\",\r\n                                detail: \"Updated Successfully\",\r\n                                life: 10000,\r\n                            });\r\n                            this.hideNew();\r\n                            this.shiftSetupService\r\n                                .getAll()\r\n                                .then((data) => (this.shiftList = data.data));\r\n                        } else if (result.status === 401) {\r\n                            this.$toast.add({\r\n                                severity: \"error\",\r\n                                summary: \"Error\",\r\n                                detail: result.data,\r\n                                life: 10000,\r\n                            });\r\n                        } else {\r\n                            this.$toast.add({\r\n                                severity: \"error\",\r\n                                summary: \"Failed\",\r\n                                detail: result.res,\r\n                                life: 10000,\r\n                            });\r\n                        }\r\n                    }\r\n                }\r\n            } catch (error) {\r\n                this.$toast.add({\r\n                    severity: \"error\",\r\n                    summary: \"Failed\",\r\n                    detail: error,\r\n                    life: 10000,\r\n                });\r\n            }\r\n        },\r\n        edit(obj) {\r\n            this.shiftSetup = Object.assign({}, obj);\r\n\r\n            this.shiftSetup.start_timev = obj.start_time\r\n                ? new Date(\"2000-01-01 \" + obj.start_time)\r\n                : obj.start_time;\r\n            this.shiftSetup.end_timev = obj.end_time\r\n                ? new Date(\"2000-01-01 \" + obj.end_time)\r\n                : obj.end_time;\r\n\r\n            this.dataList = false;\r\n            this.newInfo = true;\r\n        },\r\n        formatTime(value) {\r\n            return moment(String(value)).format(\"h:mm:ss a\");\r\n          },\r\n    },\r\n};"]}]}