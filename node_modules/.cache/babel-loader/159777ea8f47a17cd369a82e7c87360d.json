created() {\r\n    this.productSetupService = new ProductSetupService();\r\n    this.productCategorySetupService = new ProductCategorySetupService();\r\n    this.uomSetupService = new UOMSetupService();\r\n    this.validation = new Validation();\r\n\r\n    this.uomSetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.uomList = data.data));\r\n\r\n    this.productCategorySetupService\r\n      .getActiveCombo()\r\n      .then((data) => (this.productCategoryList = data.data));\r\n\r\n    this.productSetupService\r\n      .getAll()\r\n      .then((data) => (this.productList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.productSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.productSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.productSetup.product_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.productList, 'product_name', this.productSetup.product_name, 'product_id', this.productSetup.product_id, this)) {\r\n          return;\r\n        } else if (!this.productSetup.product_category_id) {\r\n          return;\r\n        } else if (!this.productSetup.uom_id) {\r\n          return;\r\n        } else if (!this.productSetup.purchase_uom_id) {\r\n          return;\r\n        } else if (!this.productSetup.packing_size) {\r\n          return;\r\n        } else if (!this.productSetup.reordering_quantity) {\r\n          return;\r\n        } else if (!this.productSetup.cost_price) {\r\n          return;\r\n        } else {\r\n          if (!this.productSetup.product_id) {\r\n            let result = await this.productSetupService.add(this.productSetup);\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Saved Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.productSetupService\r\n                .getAll()\r\n                .then((data) => (this.productList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.productSetupService.put(\r\n              this.productSetup,\r\n              this.productSetup.product_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Sucess\",\r\n                detail: \"Updated Sucessfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.productSetupService\r\n                .getAll()\r\n                .then((data) => (this.productList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Faild\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Faild\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.productSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}