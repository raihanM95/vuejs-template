Content":["import LabCenterSetupService from \"../../service/setup/LabCenterSetupService\";\r\nimport Validation from \"../../scripts/validation\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      isEmailValid: \"\",\r\n      isPhoneValid: \"\",\r\n      newInfo : false,\r\n      dataList : true,\r\n      labCenterList: [],\r\n      labCenterSetup: {},\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.labCenterSetupService = new LabCenterSetupService();\r\n    this.validation = new Validation();\r\n    this.labCenterSetupService\r\n      .getAll()\r\n      .then((data) => (this.labCenterList = data.data));\r\n  },\r\n  methods: {\r\n    openNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.labCenterSetup = {};\r\n      this.newInfo = true;\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n    },\r\n    hideNew() {\r\n      this.isEmailValid = \"\";\r\n      this.isPhoneValid = \"\";\r\n      this.labCenterSetup = {};\r\n      this.dataList = true;\r\n      this.newInfo = false;\r\n      this.submitted = false;\r\n    },\r\n    async add() {\r\n      this.submitted = true;\r\n      try {\r\n        if (!this.labCenterSetup.labratory_center_name) {\r\n          return;\r\n        } else if (this.validation.isNameExists(this.labCenterList, 'labratory_center_name', this.labCenterSetup.labratory_center_name, 'labraotry_center_id', this.labCenterSetup.labraotry_center_id, this)) {\r\n          return;\r\n        } else if (!this.labCenterSetup.contact_no) {\r\n          return;\r\n        } else if (this.isPhoneValid || this.isEmailValid) {\r\n          return;\r\n        } else {\r\n          if (!this.labCenterSetup.labraotry_center_id) {\r\n            let result = await this.labCenterSetupService.add(\r\n              this.labCenterSetup\r\n            );\r\n            if (result.status === 201) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Saved Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.labCenterSetupService\r\n                .getAll()\r\n                .then((data) => (this.labCenterList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          } else {\r\n            let result = await this.labCenterSetupService.put(\r\n              this.labCenterSetup,\r\n              this.labCenterSetup.labraotry_center_id\r\n            );\r\n            if (result.status === 202) {\r\n              this.$toast.add({\r\n                severity: \"success\",\r\n                summary: \"Success\",\r\n                detail: \"Updated Successfully\",\r\n                life: 10000,\r\n              });\r\n              this.hideNew();\r\n              this.labCenterSetupService\r\n                .getAll()\r\n                .then((data) => (this.labCenterList = data.data));\r\n            } else if (result.status === 401) {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Error\",\r\n                detail: result.data,\r\n                life: 10000,\r\n              });\r\n            } else {\r\n              this.$toast.add({\r\n                severity: \"error\",\r\n                summary: \"Failed\",\r\n                detail: result.res,\r\n                life: 10000,\r\n              });\r\n            }\r\n          }\r\n        }\r\n      } catch (error) {\r\n        this.$toast.add({\r\n          severity: \"error\",\r\n          summary: \"Failed\",\r\n          detail: error,\r\n          life: 10000,\r\n        });\r\n      }\r\n    },\r\n    edit(obj) {\r\n      this.labCenterSetup = Object.assign({}, obj);\r\n      this.dataList = false;\r\n      this.newInfo = true;\r\n    },\r\n  },\r\n};"]}]}