A,OAAO,EAAE;AACLC,UAAAA,MAAM,EAAE;AACJL,YAAAA,MAAM,EAAE;AACJM,cAAAA,KAAK,EAAE;AADH;AADJ;AADH;AADY,OAAD,CAAxB;AASH;AA/CI;AA1DE,CAAf","sourcesContent":["import CommonService from \"../../service/CommonService\";\r\nimport DashboardService from \"../../service/components/DashboardService\";\r\nimport moment from \"moment\";\r\nimport { ref } from 'vue';\r\nimport DashboardInvestigationStatus from \"../../components/DashboardInvestigationStatus\";\r\nimport DashboardInvestigationResult from \"../../components/DashboardInvestigationResult\";\r\nimport AdmissionService from \"../../service/front-desk/AdmissionService\";\r\n\r\nexport default {\r\n    components: {\r\n        DashboardInvestigationStatus,\r\n        DashboardInvestigationResult\r\n    },\r\n    data() {\r\n        return {\r\n            isDoctor: false,\r\n            isAdmin: false,\r\n            totalDoctor: 0,\r\n            totalAdmittedPatient: 0,\r\n            totalDischargedPatient: 0,\r\n            totalNurse: 0,\r\n            todaysAdmittedList: [],\r\n            topAdmittedList: [],\r\n\r\n            todaysAdmittedCount: 0,\r\n\r\n            // chart\r\n            chartData: {},\r\n            lightOptions: {},\r\n            genderData: {},\r\n            genderOptions: {},\r\n            submitted: false,\r\n        };\r\n    },\r\n    created() {\r\n        this.commonService = new CommonService();\r\n        this.dashboardService = new DashboardService();\r\n        this.admissionService = new AdmissionService();\r\n\r\n        //var fromDate = \"2021-01-01\";\r\n        //var toDate = moment(String(new Date(new Date().setDate(new Date().getDate() + 7)))).format(\"YYYY-MM-DD\");\r\n        //var status = \"All\";\r\n        this.userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n        this.admissionService\r\n            .admittedPatientListForTreatmentByDoctorId(moment(String(new Date())).format(\"YYYY-MM-DD\"), moment(String(new Date())).format(\"YYYY-MM-DD\"), this.userInfo.doctor_id)\r\n            .then((data) => this.admittedPatientListFormatted(data.data));\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.dashboardService.getIPDDashboardInfoByDoctorId(this.userInfo.doctor_id)\r\n                .then((data) => this.getFormattedIPDDashboardByDoctor(data.data));\r\n\r\n            this.isDoctor = true;\r\n        } else {\r\n            this.dashboardService.ipdGetAllAdmin()\r\n                .then((data) => this.getFormattedIPDDashboard(data.data));\r\n\r\n            this.isAdmin = true;\r\n        }\r\n    },\r\n    mounted() {\r\n        // pass data into component \r\n        this.$refs.dashboardInvestigationStatusComponent.getInvestigationStatus('IPD');\r\n        //this.userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n        if (this.userInfo.role_id == \"2\" && this.userInfo.doctor_id) {\r\n            this.$refs.dashboardInvestigationResultComponent.getInvestigationResult(this.userInfo.doctor_id);\r\n        }\r\n    },\r\n    methods: {\r\n        admittedPatientListFormatted(data) {\r\n            this.todaysAdmittedList = data;\r\n            this.todaysAdmittedCount = this.todaysAdmittedList.length;\r\n        },\r\n        getFormattedIPDDashboard(data) {\r\n            var ipdDashboardInfo = JSON.parse(data.ipdDashboardInfo)\r\n            this.totalDoctor = ipdDashboardInfo[0].doctors;\r\n            this.totalAdmittedPatient = ipdDashboardInfo[0].admitted;\r\n            this.totalDischargedPatient = ipdDashboardInfo[0].discharged;\r\n            this.totalNurse = ipdDashboardInfo[0].nurses;\r\n            this.maleCount = ipdDashboardInfo[0].admittedmale;\r\n            this.femaleCount = ipdDashboardInfo[0].admittedfemale;\r\n            this.thirdCount = ipdDashboardInfo[0].admittedthird;\r\n\r\n            this.topAdmittedList = JSON.parse(data.doctorWiseAdmittedPatientCount);\r\n\r\n            this.genderWiseAdmittedPatient();\r\n        },\r\n        getFormattedIPDDashboardByDoctor(data) {\r\n            var ipdDashboardInfo = JSON.parse(data.ipdDashboardInfo)\r\n\r\n            this.totalAdmittedPatient = ipdDashboardInfo[0].admitted;\r\n            this.totalDischargedPatient = ipdDashboardInfo[0].discharged;\r\n        },\r\n        genderWiseAdmittedPatient() {\r\n            // chart\r\n            this.genderData = ref({\r\n                labels: ['Male', 'Female', 'Third'],\r\n                datasets: [\r\n                    {\r\n                        data: [this.maleCount, this.femaleCount, this.thirdCount],\r\n                        backgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"],\r\n                        hoverBackgroundColor: [\"#36A2EB\", \"#FF6384\", \"#FFCE56\"]\r\n                    }\r\n                ]\r\n            });\r\n\r\n            this.genderOptions = ref({\r\n                plugins: {\r\n                    legend: {\r\n                        labels: {\r\n                            color: '#495057'\r\n                        }\r\n                    }\r\n                }\r\n            });\r\n        }\r\n    },\r\n};"]}]}