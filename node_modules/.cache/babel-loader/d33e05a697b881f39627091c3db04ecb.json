tB;AACA,WAAKC,YAAL,GAAoB1D,IAAI,CAAC2D,UAAL,GAAkB,GAAlB,GAAwB3D,IAAI,CAAC4D,SAAjD;AACA,WAAKC,2BAAL,GAAmC7D,IAAI,CAAC6D,2BAAxC;AACA,WAAKC,WAAL,GAAmB9D,IAAI,CAAC8D,WAAxB;AACA,WAAKrC,kBAAL,GAA0B,IAA1B;AACD;AAxEM;AAnDI,CAAf","sourcesContent":["import CommonService from \"../../service/CommonService\";\r\nimport AdmissionService from \"../../service/front-desk/AdmissionService\";\r\nimport IPDReportService from \"../../service/report/IPDReportService\";\r\n\r\nimport moment from \"moment\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dataList: false,\r\n      admissionStatusList: [],\r\n      paymentStatusList: [],\r\n      admissionList: [],\r\n      admission: {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        // cdtodate: moment(\r\n        //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        // ).format(\"MM/DD/YYYY\"),\r\n        admission_status: \"All\",\r\n        payment_status: \"All\",\r\n      },\r\n      submitted: false,\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n      admittedInfoDialog: false,\r\n    };\r\n  },\r\n  created() {\r\n    this.admissionService = new AdmissionService();\r\n    this.iPDReportService = new IPDReportService();\r\n    this.commonService = new CommonService();\r\n\r\n\r\n    this.commonService\r\n      .GetAllAdmissionStatus()\r\n      .then((data) => (this.admissionStatusList = data.data));\r\n\r\n    this.commonService\r\n      .GetAllPaymentStatus()\r\n      .then((data) => (this.paymentStatusList = data.data));\r\n\r\n    this.show();\r\n  },\r\n  methods: {\r\n    reset() {\r\n      this.admission = {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        // cdtodate: moment(\r\n        //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        // ).format(\"MM/DD/YYYY\"),\r\n        admission_status: \"All\",\r\n        payment_status: \"All\",\r\n      };\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n      this.show();\r\n    },\r\n    show() {\r\n      this.submitted = true;\r\n      if (!this.admission.cdfromdate) {\r\n        return;\r\n      } else if (!this.admission.cdtodate) {\r\n        return;\r\n      } else if (!this.admission.admission_status) {\r\n        return;\r\n      } else if (!this.admission.payment_status) {\r\n        return;\r\n      } else {\r\n        this.admission.fromdate = moment(\r\n          String(this.admission.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.admission.todate = moment(\r\n          String(this.admission.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.admissionService\r\n          .getDynamic(\r\n            this.admission.fromdate,\r\n            this.admission.todate,\r\n            this.admission.admission_status,\r\n            this.admission.payment_status\r\n          )\r\n          .then((data) => (this.admissionList = data.data));\r\n      }\r\n      this.dataList = true;\r\n      this.submitted = true;\r\n    },\r\n    formatDate(value) {\r\n      return moment(String(value)).format(\"MM/DD/YYYY h:mm:ss a\");\r\n    },\r\n    moneyReceiptPrint(id) {\r\n      this.iPDReportService.GetAdmissionPayment(id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n    },\r\n    printDischargeBillSummary(id) {\r\n      this.iPDReportService.GetDischargeBillSummary(id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n    },\r\n    payNow(data) {\r\n      localStorage.setItem('admission_no_for_payment_from_admissionlist', data);\r\n      this.$router.push(\"/admission-payment-collection\");\r\n    },\r\n    // edit(data){\r\n    //   localStorage.setItem('appointmentInfo', JSON.stringify(data));\r\n    //   this.$router.push(\"/patient-doctor-appointment\");\r\n    // }\r\n    showAdmittedInfo(data) {\r\n      this.building_name = data.building_name;\r\n      this.word_cabin_category_name = data.word_cabin_category_name;\r\n      this.word_cabin_name = data.word_cabin_name;\r\n      this.bed_name = data.bed_name;\r\n      this.admission_no = data.admission_no;\r\n      this.admission_date = this.formatDate(data.admission_date);\r\n      this.patient_name = data.first_name + \" \" + data.last_name;\r\n      this.consultancy_department_name = data.consultancy_department_name;\r\n      this.doctor_name = data.doctor_name;\r\n      this.admittedInfoDialog = true;\r\n    }\r\n  }\r\n};"]}]}