mit\"\r\n        icon=\"pi pi-check\"\r\n        class=\"p-button-text\"\r\n        @click=\"addFigure\"\r\n      />\r\n    </template>\r\n  </Dialog> -->\r\n   <!-- Figure Dialog End -->\r\n  <!-- Prescription Start -->\r\n  <div class=\"card\" v-show=\"!dataList\">\r\n    <!-- <h3 class=\"p-text-center\" style=\"margin-bottom: 28px\">Prescription</h3> -->\r\n    <p class=\"p-border\">\r\n        <table style=\"width: 100%;\">\r\n          <tr>\r\n            <th style=\"text-align: left\">Registration: {{ appointmentInfo.patientReg }}</th>\r\n            <th style=\"text-align: right\">Age: {{ appointmentInfo.patientAgeYear }} Y {{ appointmentInfo.patientAgeMonth }} M {{ appointmentInfo.patientAgeDay }} D | Gender: {{ appointmentInfo.patientGender }}</th>\r\n          </tr>\r\n          <tr>\r\n            <th style=\"text-align: left\">Name: {{ appointmentInfo.patientName }}</th>\r\n            <th style=\"text-align: right\">Date: {{ appointmentPrescription.cdprescription_date }}</th>\r\n          </tr>\r\n        </table>\r\n      </p>\r\n    <div class=\"p-grid\">\r\n      <!-- Previous Design Start -->\r\n      <!-- <div class=\"p-col-12 p-md-3\" style=\"height: 600px;overflow: auto;\">\r\n        <Panel header=\"Figure (Ctrl+Alt+F)\" :toggleable=\"true\" class=\"margin-top\" @click=\"getFigure\">\r\n            <DataTable :value=\"appointmentPrescription.lstAppoinmentPrescriptionFigure\" dataKey=\"figure_id\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" :scrollable=\"true\" scrollHeight=\"100px\" v-show=\"appointmentPrescription.lstAppoinmentPrescriptionFigure.length > 0\">\r\n                <Column field=\"name\" style=\"margin: 0;padding: 1px\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data.name}}\r\n                </template>\r\n                </Column>\r\n                \r\n                <Column>\r\n                  <template #body=\"slotProps\">\r\n                    <label class=\"pi pi-trash trash-style\" @click=\"deleteItem(appointmentPrescription.lstAppoinmentPrescriptionFigure, slotProps.data)\"></label>\r\n                  </template>\r\n                </Column>\r\n            </DataTable>\r\n            <input\r\n                type=\"checkbox\"\r\n                id=\"show_in_prescription\"\r\n                name=\"show_in_prescription\"\r\n                v-on:click=\"showInPrescription(appointmentPrescription.lstAppoinmentPrescriptionFigure, appointmentPrescription.figure_show_in_prescription)\"\r\n                v-model=\"appointmentPrescription.figure_show_in_prescription\"\r\n              />\r\n              <label for=\"show_in_prescription\">Print</label>\r\n        </Panel>\r\n        <Panel header=\"Diagnosis (Ctrl+Alt+D)\" :toggleable=\"true\" class=\"margin-top\" @click=\"getDiagnosis\">\r\n            <DataTable :value=\"appointmentPrescription.lstAppoinmentPrescriptionDiagnosis\" dataKey=\"diagnosis_id\" class=\"p-datatable-sm\" responsiveLayout=\"scroll\" :scrollable=\"true\" scrollHeight=\"100px\" v-show=\"appointmentPrescription.lstAppoinmentPrescriptionDiagnosis.length > 0\">\r\n                <Column field=\"name\" style=\"margin: 0;padding: 1px\">\r\n                <template #body=\"slotProps\">\r\n                  {{slotProps.data.name}}\r\n                </template>\r\n                </Column>\r\n                \r\n                <Column>\r\n                  <template #body=\"slotProps\">\r\n                    <label class=\"pi pi-trash trash-style\" @click=\"deleteItem(appointmentPrescription.lstAppoinmentPrescriptionDiagnosis, slotProps.data)\"></label>\r\n                  </template>\r\n                </Column>\r\n            </DataTable>\r\n        </Panel>\r\n      </div> -->\r\n      <!-- Previous Design End -->\r\n      <Prescription ref=\"prescriptionComponent\"/>\r\n    </div>\r\n    <!-- <Panel :header=headerName>\r\n            <p style=\"height: 425px;\">\r\n              \r\n              <DataTable