:keyup.enter=\"this.$refs.payment_status.$el.focus()\"\r\n            @change=\"show()\"\r\n          />\r\n          <label for=\"appoinment_status\"\r\n            >Appoinment Status<span style=\"color: red\">*</span></label\r\n          >\r\n        </div>\r\n        <small\r\n          class=\"p-invalid\"\r\n          v-if=\"submitted && !doctorAppointment.appoinment_status\"\r\n          >Appoinment Status is required.</small\r\n        >\r\n      </div>\r\n      <div class=\"p-field p-col-12 p-md-1\">\r\n        <Button\r\n          label=\"Show\"\r\n          @click=\"show()\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #3f51b5; border: none\"\r\n        />\r\n      </div>\r\n      <!-- <div class=\"p-field p-col-12 p-md-2\">\r\n        <Button\r\n          label=\"Reset\"\r\n          @click=\"reset\"\r\n          class=\"p-button-lg\"\r\n          style=\"background-color: #fb465e; border: none\"\r\n        />\r\n      </div> -->\r\n    </div>\r\n  </div>\r\n  <div\r\n    class=\"p-grid crud-demo\"\r\n    v-show=\"doctorAppointmentList.length > 0 && dataList && !isHistory\"\r\n  >\r\n    <div class=\"p-col-12\">\r\n      <div class=\"card\">\r\n        <DataTable\r\n          ref=\"dt\"\r\n          :value=\"doctorAppointmentList\"\r\n          dataKey=\"appoinment_id\"\r\n          :paginator=\"true\"\r\n          :rows=\"10\"\r\n          v-model:filters=\"filters\"\r\n          paginatorTemplate=\"FirstPageLink PrevPageLink PageLinks NextPageLink LastPageLink  RowsPerPageDropdown\"\r\n          :rowsPerPageOptions=\"[10, 15, 20]\"\r\n          currentPageReportTemplate=\"Showing {first} to {last} of {totalRecords}\"\r\n          responsiveLayout=\"scroll\"\r\n        >\r\n          <template #header>\r\n            <div\r\n              class=\"\r\n                table-header\r\n                p-d-flex p-flex-column p-flex-md-row p-jc-md-between\r\n              \"\r\n            >\r\n              <h5 class=\"p-m-0\">Appointment List</h5>\r\n              <span class=\"p-input-icon-left\">\r\n                <i class=\"pi pi-search\" />\r\n                <InputText\r\n                  v-model=\"filters['global'].value\"\r\n                  placeholder=\"Search...\"\r\n                />\r\n              </span>\r\n            </div>\r\n          </template>\r\n\r\n          <Column field=\"appoinment_no\" header=\"No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">No</span>\r\n              {{ slotProps.data.appoinment_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.registration_no\"\r\n            header=\"Patient Reg No\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Reg No</span>\r\n              {{ slotProps.data.aPatient.registration_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"aPatient.first_name\"\r\n            header=\"Patient Name\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Name</span>\r\n              {{ slotProps.data.aPatient.first_name }}\r\n              {{ slotProps.data.aPatient.last_name }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"aPatient.contact_no\" header=\"Contact\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Contact</span>\r\n              {{ slotProps.data.aPatient.contact_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"appoint_date\"\r\n            header=\"Appointment Date\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Appointment Date</span>\r\n              {{ formatDate(slotProps.data.appoint_date) }}\r\n            </template>\r\n          </Column>\r\n          <Column field=\"serial_no\" header=\"Serial No\" :sortable=\"true\">\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Serial No</span>\r\n              {{ slotProps.data.serial_no }}\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            field=\"appoinment_status\"\r\n            header=\"Appoinment Status\"\r\n            :sortable=\"true\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Appoinment Status</span>\r\n              <span\r\n                v-if=\"slotProps.data.appoinment_status == 'Active'\"\r\n                class=\"product-badge status-success\"\r\n                >Active</span\r\n              >\r\n              <span\r\n                v-if=\"slotProps.data.appoinment_status == 'Visited'\"\r\n                class=\"product-badge status-warning\"\r\n                >Visited</span\r\n              >\r\n              <span\r\n                v-if=\"slotProps.data.appoinment_status == 'Expired'\"\r\n                class=\"product-badge status-unsuccessful\"\r\n                >Expired</span\r\n              >\r\n            </template>\r\n          </Column>\r\n          <!-- <Column\r\n            header=\"History\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">History</span>\r\n              <Button\r\n                v-if=\"slotProps.data.appoinment_status == 'Active' && checkDate(slotProps.data.appoint_date)\"\r\n                icon=\"pi fa fa-info-circle\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"historyCollection(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column> -->\r\n          <Column\r\n            header=\"Rx\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Rx</span>\r\n              <Button\r\n                v-if=\"slotProps.data.appoinment_status == 'Active' && checkDate(slotProps.data.appoint_date) && isDoctor\"\r\n                icon=\"pi pi-file-excel\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"prescription(slotProps.data)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n            header=\"Prev Rx\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Prev Rx</span>\r\n              <Button\r\n                v-if=\"!isAssistant\"\r\n                icon=\"pi pi-file-excel\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"previousPrescription(slotProps.data.aPatient.patient_id)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n          <Column\r\n            headerStyle=\"width: 123px; text-align: right\"\r\n            bodyStyle=\"width: 123px; text-align: right\"\r\n            footerStyle=\"width: 123px; text-align: right\"\r\n            header=\"Patient Profile\"\r\n          >\r\n            <template #body=\"slotProps\">\r\n              <span class=\"p-column-title\">Patient Profile</span>\r\n              <Button\r\n                v-if=\"!isAssistant\"\r\n                icon=\"fa fa-eye\"\r\n                class=\"p-button-rounded p-button-success p-mr-2\"\r\n                @click=\"viewPatientProfile(slotProps.data.aPatient.patient_id)\"\r\n              />\r\n            </template>\r\n          </Column>\r\n        </DataTable>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport app from \"../../scripts/opd/AppointmentPrescription\";\r\nexport default app;\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.drill-down {\r\n  float: right;\r\n  cursor: pointer;\r\n  font-weight: 900;\r\n}\r\n\r\n.product-badge {\r\n  border-radius: 2px;\r\n  padding: 0.25em 0.5rem;\r\n  text-transform: uppercase;\r\n  font-weight: 700;\r\n  font-size: 12px;\r\n  letter-spacing: 0.3px;\r\n\r\n  &.status-success {\r\n    background: #c8e6c9;\r\n    color: #256029;\r\n  }\r\n\r\n  &.status-unsuccessful {\r\n    background: #ffcdd2;\r\n    color: #c63737;\r\n  }\r\n\r\n  &.status-warning {\r\n    background: #feedaf;\r\n    color: #8a5340;\r\n  }\r\n}\r\n\r\n.margin-top {\r\n  margin-top: 10px;\r\n}\r\n\r\n.p-border {\r\n  border: 1px solid #6cc3d5 !important;\r\n  border-radius: 5px;\r\n  padding: 5px;\r\n  padding-left: 50px;\r\n  padding-right: 50px;\r\n}\r\n\r\n.div-border {\r\n  border: 1px solid #6cc3d5 !important;\r\n  border-radius: 5px;\r\n  padding: 5px;\r\n}\r\n\r\n.trash-style {\r\n  cursor: pointer;\r\n  color: #ff7851 !important;\r\n  right: 0;\r\n  position: absolute;\r\n}\r\n\r\n.pencil-style {\r\n  cursor: pointer;\r\n  color: #56cc9d !important;\r\n  right: 24px;\r\n  position: absolute;\r\n}\r\n\r\n.center {\r\n  margin: auto;\r\n  width: 50%;\r\n  border: 1px solid #6cc3d5 !important;\r\n  border-radius: 5px;\r\n  padding: 1px;\r\n}\r\n.canvas {\r\n  margin: auto;\r\n  cursor: crosshair;\r\n}\r\n.img-employee {\r\n  display: block;\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  border: 5px solid #f8f9fa;\r\n  box-shadow: 0 0 10px #a6a3db !important;\r\n  width: 50%;\r\n  border-radius: 50%;\r\n}\r\n.orders-subtable {\r\n  padding: 1rem;\r\n}\r\n</style>"],"sourceRoot":""}]}