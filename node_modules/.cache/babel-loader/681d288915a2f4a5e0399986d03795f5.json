N,CAAsBL,MAAtB,CAA6B,YAA7B,CAAP;AACH,KAtCM;AAuCPyB,IAAAA,cAvCO,0BAuCQpB,KAvCR,EAuCe;AACpB,aAAOf,MAAM,CAACQ,MAAM,CAACO,KAAD,CAAP,CAAN,CAAsBL,MAAtB,CAA6B,sBAA7B,CAAP;AACD,KAzCM;AA0CP;AACA;AACA;AACA;AACA;AACA;AACA;AACA0B,IAAAA,aAjDO,yBAiDOC,YAjDP,EAiDoB;AACzBC,MAAAA,YAAY,CAACC,OAAb,CAAqB,8BAArB,EAAqDF,YAArD;AACE,WAAKG,OAAL,CAAaC,IAAb,CAAkB,cAAlB;AACH;AApDM;AArCI,CAAf","sourcesContent":["import CommonService from \"../../service/CommonService\";\r\nimport BloodRequestService from \"../../service/blood-bank/BloodRequestService\";\r\n//import PatientReportService from \"../../service/report/PatientReportService\";\r\nimport moment from \"moment\";\r\nimport { FilterMatchMode, FilterOperator } from \"primevue/api\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      dataList: false,\r\n      bloodRequestList: [],\r\n      bloodRequest: {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        // cdtodate: moment(\r\n        //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        // ).format(\"MM/DD/YYYY\"),\r\n      },\r\n      submitted: false,\r\n      filters: {\r\n        global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n        name: { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n      },\r\n      initFilters() {\r\n        this.filters = {\r\n          global: { value: null, matchMode: FilterMatchMode.CONTAINS },\r\n          name: {\r\n            operator: FilterOperator.AND,\r\n            constraints: [\r\n              { value: null, matchMode: FilterMatchMode.STARTS_WITH },\r\n            ],\r\n          },\r\n        };\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.bloodRequestService = new BloodRequestService();\r\n    this.commonService = new CommonService();\r\n    //this.patientReportService = new PatientReportService();\r\n\r\n    this.show();\r\n  },\r\n  methods: {\r\n    reset() {\r\n      this.bloodRequest = {\r\n        cdfromdate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        cdtodate: moment(String(new Date())).format(\"MM/DD/YYYY\"),\r\n        // cdtodate: moment(\r\n        //   String(new Date(new Date().setDate(new Date().getDate() + 7)))\r\n        // ).format(\"MM/DD/YYYY\"),\r\n      };\r\n      this.dataList = false;\r\n      this.submitted = false;\r\n      this.show();\r\n    },\r\n    show() {\r\n      this.submitted = true;\r\n      if (!this.bloodRequest.cdfromdate) {\r\n        return;\r\n      } else if (!this.bloodRequest.cdtodate) {\r\n        return;\r\n      } else {\r\n        this.bloodRequest.fromdate = moment(\r\n          String(this.bloodRequest.cdfromdate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.bloodRequest.todate = moment(\r\n          String(this.bloodRequest.cdtodate)\r\n        ).format(\"YYYY-MM-DD\");\r\n        this.bloodRequestService\r\n          .getDynamic(\r\n            this.bloodRequest.fromdate,\r\n            this.bloodRequest.todate,\r\n          )\r\n          .then((data) => (this.bloodRequestList = data.data));\r\n      }\r\n      this.dataList = true;\r\n      this.submitted = true;\r\n    },\r\n    formatDate(value) {\r\n        return moment(String(value)).format(\"MM/DD/YYYY\");\r\n    },\r\n    formatDateTime(value) {\r\n      return moment(String(value)).format(\"MM/DD/YYYY h:mm:ss a\");\r\n    },\r\n    // apptTokenPrint(appoinment_id){\r\n    //   this.patientReportService.GetAppointmentTokenByAppointmentId(appoinment_id, this).then((data) => (this.commonService.Downloadfile(data.data)));\r\n    // },\r\n    // edit(data){\r\n    //   localStorage.setItem('appointmentInfo', JSON.stringify(data));\r\n    //   this.$router.push(\"/patient-doctor-appointment\");\r\n    // }\r\n    bloodIssueNow(admission_no){\r\n      localStorage.setItem('admission_no_for_blood_issue', admission_no);\r\n        this.$router.push(\"/blood-issue\");\r\n    }\r\n  }\r\n};"]